INSERT INTO ASSET VALUES(49,'test-var1','1826_433A_v1.xml','<?xml version="1.0" encoding="UTF-8"?><div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="variantMain">\u000a   <p class="witness">Teksten er etableret af:</p>\u000a   <i>\u000a      <div class="editor">Krista Stinne Greve Rasmussen</div>\u000a      <div class="editor">Else Riisager</div>\u000a   </i>\u000a   <p class="witness">Tekstkilde:</p>\u000a   <i>\u000a      <p class="witness">L.C. Hagen (1832) Historiske Psalmer og\u000a         Riim til B\u00f8rne-L\u00e6rdom, nr. 45\u000a      </p>\u000a   </i>\u000a   <hr></hr>\u000a   \u000a   \u000a   \u000a   \u000a   \u000a   \u000a   <div class="head2">No. 45.</div>\u000a   \u000a   \u000a   <div class="anchor">anchor: struct35a</div>\u000a   \u000a   <div class="center">Egen Melodie.</div>\u000a   \u000a   <div class="anchor">anchor: struct35b</div>\u000a   \u000a   \u000a   <div class="lg">\u000a      \u000a      <div class="">Den signede \u000a         <span class="ide">Dag med Fryd vi seer,</span>\u000a      </div>\u000a      \u000a      <div class="">\u000a         <span class="ide">Af Havet</span> til os \u000a         <span class="ide">opkomme;</span>\u000a      </div>\u000a      \u000a      <div class="">Den lyse paa \u000a         <span class="ide">Himlen,</span> meer og \u000a         <span class="ide">meer,</span>\u000a      </div>\u000a      \u000a      <div class="">Os \u000a         <span class="ide">alle</span> til Lyst og \u000a         <span class="ide">Fromme!</span>\u000a      </div>\u000a      \u000a      <div class="">Det \u000a         <span class="ort">kjendes</span> paa os, som \u000a         <span class="ide">Lysets B\u00f8rn</span>,\u000a      </div>\u000a      \u000a      <div class="">At \u000a         <span class="ide">Natten</span>\u000a         <span class="ide">hun</span> er nu omme!\u000a      </div>\u000a      \u000a   </div>\u000a   \u000a   \u000a   <div class="lg">\u000a      \u000a      <div class="">Den \u000a         <span class="ide">signede</span> Stund, den \u000a         <span class="ide">Midnats-Tid</span>,\u000a      </div>\u000a      \u000a      <div class="">\u000a         <span class="ide">Vor Herre</span>\u000a         <span class="ort">Han</span> lod \u000a         <span class="ort">Sig</span> f\u00f8de,\u000a      </div>\u000a      \u000a      <div class="">Da \u000a         <span class="ort">klared''</span> det op i \u000a         <span class="ide">\u00d8ster-Lid,</span>\u000a      </div>\u000a      \u000a      <div class="">Til \u000a         <span class="ide">deiligste</span>\u000a         <span class="ide">Morgen-R\u00f8de:</span>\u000a      </div>\u000a      \u000a      <div class="anchor">anchor: struct36a</div>\u000a      \u000a      <div class="">Da \u000a         <span class="ide">Lyset</span>\u000a         <span class="ide">oprandt,</span>\u000a         <span class="ide">som Jordens Bold</span>\u000a      </div>\u000a      \u000a      <div class="anchor">anchor: struct36b</div>\u000a      \u000a      <div class="anchor">anchor: struct31a</div>\u000a      \u000a      <div class="">Skal lysne udi og \u000a         <span class="ide">gl\u00f8de!</span>\u000a      </div>\u000a      \u000a      <div class="anchor">anchor: struct31b</div>\u000a      \u000a   </div>\u000a   \u000a   \u000a   \u000a   <div class="lg">\u000a      \u000a      <div class="">Om levende blev hvert Tr\u00e6 i Skov,</div>\u000a      \u000a      <div class="">Og var saa hvert Blad en Tunge,</div>\u000a      \u000a      <div class="">De kunde dog \u000a         <span class="ide">ei</span> Guds Naades \u000a         <span class="ide">              Lov</span>\u000a      </div>\u000a      \u000a      <div class="">Med v\u00e6rdelig R\u00f8st \u000a         <span class="ide">udsjunge;          </span>\u000a      </div>\u000a      \u000a      <div class="">Thi evig \u000a         <span class="ide">nu skinner</span> Livets \u000a         <span class="int">            Lys</span>\u000a      </div>\u000a      \u000a      <div class="">For \u000a         <span class="ide">Gamle</span>\u000a         <span class="ide">og saa              </span> for \u000a         <span class="ide">Unge!</span>\u000a      </div>\u000a      \u000a   </div>\u000a   \u000a   \u000a   <div class="lg">\u000a      \u000a      <div class="anchor">anchor: struct1a</div>\u000a      \u000a      <div class="">\u000a         <span class="int">(Ja,</span> havde end Maal hvert Straa i Vang,\u000a      </div>\u000a      \u000a      <div class="">Hver Urt udi Mark og Lunde,</div>\u000a      \u000a      <div class="">Slet ikke for os den \u000a         <span class="ide">Takke-Sang</span>\u000a      </div>\u000a      \u000a      <div class="">Opstemme tilgavns de kunde,</div>\u000a      \u000a      <div class="">Som Dagen h\u00f8r \u000a         <span class="ide">til,</span> for Lys og Liv,\u000a      </div>\u000a      \u000a      <div class="">Mens tusinde Aar \u000a         <span class="int">henrunde!)</span>\u000a      </div>\u000a      \u000a      <div class="anchor">anchor: struct1b</div>\u000a      \u000a   </div>\u000a   \u000a   \u000a   <div class="lg">\u000a      \u000a      <div class="anchor">anchor: struct33a</div>\u000a      \u000a      <div class="">\u000a         <span class="ort">\u000a            Forgj\u00e6ves\u000a            \u000a         </span>\u000a         \u000a         <span class="ide">\u000a            det er,\u000a            \u000a         </span> med liden \u000a         <span class="ide">\u000a            Magt,\u000a            \u000a         </span>\u000a      </div>\u000a      \u000a      <div class="">At ville mod \u000a         <span class="ide">\u000a            Bjerg\u000a            \u000a         </span>\u000a         \u000a         <span class="ide">\u000a            opspringe,\u000a            \u000a         </span>\u000a      </div>\u000a      \u000a      <div class="">Men \u00d8rnen er snild, han naaer sin Agt,</div>\u000a      \u000a      <div class=""