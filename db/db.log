INSERT INTO TEXTREFERENCE VALUES(10357,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com1">\u000a      <b>com1:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>H\u00f8itids-Psalmer \u000a      til\u000a      \u000a      Tusindaars-Festen\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">salmer til gudstjenesterne ved den kirkelige h\u00f8jtid, 1. pinsedag, den 14. maj 1826,\u000a         i anledning af den danske kirkes tusind\u00e5rsjubil\u00e6um. <a  class="comment_ref">Se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com1','comment')
INSERT INTO TEXTREFERENCE VALUES(10358,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com2">\u000a      <b>com2:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>residerende Capellan \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. pr\u00e6st, fast ansat ved kirken.</div>\u000a      \u000a   </div>','scrollTarget_com2','comment')
INSERT INTO TEXTREFERENCE VALUES(10359,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com4">\u000a      <b>com4:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>den Wahlske Boghandlings Forlag \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">var fra 1822-40 ejet af <a  class="persName" href="ajax/getReference/pe211" rel="ajax/getReference/pe211">Jacob Frederik Milo</a>, s\u00f8n af den kendte boghandler <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe213" rel="ajax/getReference/pe213">Friedrich Wilhelm Milo</a> fra Odense. Den Wahlske Boghandling l\u00e5 p\u00e5 udgivelsestidspunktet i K\u00f8benhavn p\u00e5 Ulfeldtsplads\u000a         118, vore dages Gr\u00e5br\u00f8dretorv.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com4','comment')
INSERT INTO TEXTREFERENCE VALUES(10360,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com5">\u000a      <b>com5:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>C. Gr\u00e6be \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">k\u00f8benhavnsk bogtrykkeri, grundlagt 1809 af <a  class="persName" href="ajax/getReference/pe38" rel="ajax/getReference/pe38">Christopher Gr\u00e6be</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com5','comment')
INSERT INTO TEXTREFERENCE VALUES(10361,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com7">\u000a      <b>com7:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>den \u00e6ldgamle Danske\u000a      Dag-Vise \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Dagvisen har r\u00f8dder i middelalderen og er f\u00e6llesnordisk. Den \u00e6ldst kendte optegnelse,\u000a         \u201c<span  class="rs_title">Den signade dag</span>\u201d fra klosteret i Vadstena, er fra 1450. Den f\u00f8rste danske version, \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Den signede dag som wi nu see</span>\u201d, findes i <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Hans Thomiss\u00f8n</a>s <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a            <span xmlns="" class="italic">Psalmebog</span></span> fra 1569 (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe291" rel="ajax/getReference/pe291">Balslev-Clausen</a> 2010a, s. 98 f.).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com7','comment')
INSERT INTO TEXTREFERENCE VALUES(10362,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com8">\u000a      <b>com8:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>\u000a      <span class="italic"><a  class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Hans Thomasen</a>s\u000a      </span>\u000a      <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Psalmebog</span> S. 316 \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Omtalen af dagvisen og dens sidetal er en n\u00f8jagtig gengivelse af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a>s tekst. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> har dog sandsynligvis ikke haft originaludgaven til sin r\u00e5dighed, men en udgave fra\u000a         1617, som han if\u00f8lge en bogfortegnelse fra 1839 har ejet et eksemplar af (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569]</span>) (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Bogfortegnelse 1839</span>, nr. 2297; Fasc. 520).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com8','comment')
INSERT INTO TEXTREFERENCE VALUES(10363,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com9">\u000a      <b>com9:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>K\u00e6mpevise-Tonen er i\u000a      Psalmer blevet os fremmed \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Med K\u00e6mpevise-Tonen hentydes til folkevisernes litter\u00e6re stil, der sammen med den\u000a         f\u00f8rreformatoriske og reformatoriske salmestil l\u00e5 langt fra den samtidige \u000a         <span class="italic"><span  class="rs_title">Evangelisk-kristelig Psalmebog</span>s\u000a         </span> tilstr\u00e6bt enkle og tydelige udtryksm\u00e5de. <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">Se indledningen</a>.   \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com9','comment')
INSERT INTO TEXTREFERENCE VALUES(10364,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com10">\u000a      <b>com10:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>passende paa\u000a      Takke-Festen for \u000a      <span class="italic">Tusind-Aaret,</span> \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Salmen var af <a  class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> skrevet til at blive sunget ved gudstjenesten pinsedag, d. 14. maj 1826, hvor den\u000a         officielle, kirkelige fest skulle afholdes i anledning af 1000-\u00e5ret for <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgars</a> komme til Danmark. <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">Se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com10','comment')
INSERT INTO TEXTREFERENCE VALUES(10365,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com11">\u000a      <b>com11:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>meer end\u000a      Totredie-Dele henrandt, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">hentydning til reformationen 1536.</div>\u000a      \u000a   </div>','scrollTarget_com11','comment')
INSERT INTO TEXTREFERENCE VALUES(10366,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com12">\u000a      <b>com12:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Danske Psalmer \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Salmedigtning p\u00e5 modersm\u00e5let begyndte f\u00f8rst for alvor i reformationstiden, da <a  class="persName" href="ajax/getReference/pe292" rel="ajax/getReference/pe292">Hans Tausen</a> fordanskede bl.a. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe42" rel="ajax/getReference/pe42">Luther</a>s salmer, og <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Hans Thomiss\u00f8n</a> oversatte den romerske messes latinske hymner og lovsange til dansk.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com12','comment')
INSERT INTO TEXTREFERENCE VALUES(10367,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com13">\u000a      <b>com13:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>den Danske Kirkes Morgen-R\u00f8de. \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Den danske kirke blev grundlagt i vikingetiden, og kristendommen anses for at have\u000a         v\u00e6ret udbredt i hele landet i f\u00f8rste tredjedel af 1000-tallet. Den kirkelige organisation\u000a         var fuldt udbygget med det danske \u00e6rkebispes\u00e6de i Lund \u00e5r 1104.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com13','comment')
INSERT INTO TEXTREFERENCE VALUES(10368,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com14">\u000a      <b>com14:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Den signede Dag med Fryd vi seer,\u000a      Af Havet til os opkomme;\u000a      Den lyse paa Himlen, meer og meer,\u000a      Os alle til Lyst og Fromme!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">De f\u00f8rste fire verslinjer har forl\u00e6g i 1. strofe, vers 1-4 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201c (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569]</span>, blad 316 verso - 317 recto): \u201cDen signede Dag som wi nu see, / ned til oss aff Himmelen\u000a         komme, / Gud liuser for oss jo lenger oc meer, / oss alle til gl\u00e6de oc fromme\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com14','comment')
INSERT INTO TEXTREFERENCE VALUES(10369,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com6">\u000a      <b>com6:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>vi seer \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Pga. rimtvang er verbet anf\u00f8rt i singularis, selvom det efter datidens b\u00f8jningsformer\u000a         skulle have st\u00e5et i pluralis, dvs. \u201cvi see\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com6','comment')
INSERT INTO TEXTREFERENCE VALUES(10370,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com15">\u000a      <b>com15:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>lyse \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) optativ.</div>\u000a      \u000a   </div>','scrollTarget_com15','comment')
INSERT INTO TEXTREFERENCE VALUES(10371,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com16">\u000a      <b>com16:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Fromme \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">gavn, nytte.</div>\u000a      \u000a   </div>','scrollTarget_com16','comment')
INSERT INTO TEXTREFERENCE VALUES(10372,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com17">\u000a      <b>com17:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Lysets B\u00f8rn \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Ef 5,8: \u201cThi I vare fordum m\u00f8rkhed, men nu ere I et lys i HErren: omgaaes som lysets b\u00f8rn\u201d\u000a         (\u000a         <span class="italic">Biblia</span> 1787). Skriftstedet var den tekst, der if\u00f8lge kongeligt p\u00e5bud skulle pr\u00e6dikes over\u000a         ved morgengudstjenesten p\u00e5 festdagen 1. pinsedag (<a  class="persName" href="ajax/getReference/pe333" rel="ajax/getReference/pe333">Algreen-Ussing</a> 1833, s. 68 f). Jf. ogs\u00e5 1 Thess 5,5: \u201cI ere allesammen lysets b\u00f8rn, og dagens b\u00f8rn; vi ere ikke nattens b\u00f8rn, og ei m\u00f8rkets\u201d\u000a         (\u000a         <span class="italic">Biblia</span> 1787). Om udtrykket \u201cLysets B\u00f8rn\u201d, <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com17','comment')
INSERT INTO TEXTREFERENCE VALUES(10373,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com18">\u000a      <b>com18:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Den signede Stund, den Midnats-Tid,\u000a      Vor Herre han lod sig f\u00f8de,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i 2. strofe, vers 1-2 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 recto): \u201cDen signede Dag den signede tid / Vor HErre hans F\u00f8dsels\u000a         time\u201d. Resten af 2. strofe har indholdsm\u00e6ssige ligheder med de resterende vers af\u000a         strofen hos <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com18','comment')
INSERT INTO TEXTREFERENCE VALUES(10374,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com19">\u000a      <b>com19:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>\u00d8ster-Lid \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">himlen i \u00f8st, hvor solen st\u00e5r op.</div>\u000a      \u000a   </div>','scrollTarget_com19','comment')
INSERT INTO TEXTREFERENCE VALUES(10375,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com20">\u000a      <b>com20:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Da Lyset oprandt, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Joh 1,9.</div> \u000a      \u000a   </div>','scrollTarget_com20','comment')
INSERT INTO TEXTREFERENCE VALUES(10376,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com21">\u000a      <b>com21:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Jordens Bold \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. jordkloden.</div>\u000a      \u000a   </div>','scrollTarget_com21','comment')
INSERT INTO TEXTREFERENCE VALUES(10377,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com22">\u000a      <b>com22:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Om levende blev hvert Tr\u00e6 i Skov,\u000a      Og var saa hvert Blad en Tunge,\u000a      De kunde dog ei Guds Naades Lov\u000a      Med v\u00e6rdelig R\u00f8st udsjunge;\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i 5. strofe, vers 1-4 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 verso): \u201cVaar alle de Tr\u00e6 i Verden er, / L\u00f8ff, Gr\u00e6ss, oc Sand\u000a         paa Haffsens bonde, / Haffde de Maal oc kunde de tale / De siunge met Engle stemme\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com22','comment')
INSERT INTO TEXTREFERENCE VALUES(10378,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com23">\u000a      <b>com23:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Lov \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">pris; (her) lovsang.</div>\u000a      \u000a   </div>','scrollTarget_com23','comment')
INSERT INTO TEXTREFERENCE VALUES(10379,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com24">\u000a      <b>com24:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>v\u00e6rdelig \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">v\u00e6rdig.</div>\u000a      \u000a   </div>','scrollTarget_com24','comment')
INSERT INTO TEXTREFERENCE VALUES(10380,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com25">\u000a      <b>com25:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Thi evig nu skinner Livets Lys,\u000a      For Gamle og saa for Unge!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i 2. strofe, vers 5-6 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 recto): \u201cDet liuser for oss euindelig, / Nu oc foruden all\u000a         Ende\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com25','comment')
INSERT INTO TEXTREFERENCE VALUES(10381,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com26">\u000a      <b>com26:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Livets Lys \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Joh 8,12: \u201cAtter talte <a  class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesus</a> til dem og sagde: \u0312Jeg er verdens lys. Den, der f\u00f8lger mig, skal aldrig vandre i\u000a         m\u00f8rket, men have livets lys\u0315 \u201d. Om udtrykkene \u201cLiv\u201d og \u201cLys\u201d, <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">se indledningen</a>. \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com26','comment')
INSERT INTO TEXTREFERENCE VALUES(10382,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com133">\u000a      <b>com133:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>saa \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">s\u00e5ledes ogs\u00e5.</div>\u000a      \u000a   </div>','scrollTarget_com133','comment')
INSERT INTO TEXTREFERENCE VALUES(10383,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com27">\u000a      <b>com27:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Ja, havde end Maal hvert Straa i Vang,\u000a      Hver Urt udi Mark og Lunde,                \u000a      Slet ikke for os den Takke-Sang\u000a      Opstemme tilgavns de kunde,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i vers 2-3 og 5 af 5. strofe af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 verso): \u201cVaar alle de Tr\u00e6 i Verden er, / L\u00f8ff, Gr\u00e6ss, oc Sand\u000a         paa Haffsens bonde, / Haffde de Maal oc kunde de tale / De siunge met Engle stemme,\u000a         / Da kunde de aldrig til fulde loffue / <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">JEsum</a> <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe307" rel="ajax/getReference/pe307">Mari\u00e6</a> S\u00f8n den rene\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com27','comment')
INSERT INTO TEXTREFERENCE VALUES(10384,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com28">\u000a      <b>com28:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Maal \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">m\u00e6le, evne til at tale.</div>\u000a      \u000a   </div>','scrollTarget_com28','comment')
INSERT INTO TEXTREFERENCE VALUES(10385,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com29">\u000a      <b>com29:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Opstemme \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">istemme.</div>\u000a      \u000a   </div>','scrollTarget_com29','comment')
INSERT INTO TEXTREFERENCE VALUES(10386,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com30">\u000a      <b>com30:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Lys og Liv \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Joh 1,4: \u201cI det [Ordet] var livet, og livet var menneskenes lys\u201d (\u000a         <span class="italic">Biblia</span> 1787). Om udtrykkene \u201cLys\u201d og \u201cLiv\u201d, <a  class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com30','comment')
INSERT INTO TEXTREFERENCE VALUES(10387,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com31">\u000a      <b>com31:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>tusinde Aar henrunde \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Kristendommen havde v\u00e6ret i Danmark i 1000 \u00e5r siden <a  class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgars</a> komme til Danmark i 826. Om fejringen af tusind\u00e5rsjubil\u00e6et, <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com31','comment')
INSERT INTO TEXTREFERENCE VALUES(10388,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com32">\u000a      <b>com32:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Forgi\u00e6ves det er, med liden Magt,\u000a      At ville mod Bjerg opspringe,\u000a      Men \u00d8rnen er snild, han naaer sin Agt,\u000a      Naar Veiret ham b\u00e6r paa Vinge,\u000a      Og L\u00e6rken hun er en lille Fugl,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i 6. strofe, vers 1-4 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 verso): \u201cDen lille Fuel hand fluer saa h\u00f8yt / Oc V\u00e6rit b\u00e6r\u000a         under hans vinger, / Det er fuld ont met liden mact / Mod h\u00f8ye Bierg at springe\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com32','comment')
INSERT INTO TEXTREFERENCE VALUES(10389,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com33">\u000a      <b>com33:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Veiret \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">vinden, luftstr\u00f8mmen.</div>\u000a      \u000a   </div>','scrollTarget_com33','comment')
INSERT INTO TEXTREFERENCE VALUES(10390,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com34">\u000a      <b>com34:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Nedfuser \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">v\u00e6lder ned.</div>\u000a      \u000a   </div>','scrollTarget_com34','comment')
INSERT INTO TEXTREFERENCE VALUES(10391,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com35">\u000a      <b>com35:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Ei m\u00e6le saa lydt de B\u00e6kke smaa,\u000a      Dog risle de fort og rinde,\u000a      Saa frydelig snoe de sig fra Eng,\u000a      Op under de gr\u00f8nne Linde!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i 6. strofe, vers 5-6 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 verso): \u201cDe Str\u00f8mme de rinde saa frydelig / Op under de gr\u00f8ne\u000a         Linde\u201d. \u201cunder Linde\u201d er sandsynligvis en forvanskning af \u201cunder lide\u201d (dvs. bakkeskr\u00e5ning),\u000a         der er en ofte anvendt formel i folkeviser. En <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">svensk version</span> af dagvisen fra 1450 har: \u201ctha str\u00f8mbrin swa gladhelika / under the gr\u00f8no lidha\u201d\u000a         (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe291" rel="ajax/getReference/pe291">Balslev-Clausen</a> 2010a, s. 103 f).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com35','comment')
INSERT INTO TEXTREFERENCE VALUES(10392,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com36">\u000a      <b>com36:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>fort \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">af sted; hurtigt.</div>\u000a      \u000a   </div>','scrollTarget_com36','comment')
INSERT INTO TEXTREFERENCE VALUES(10393,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com134">\u000a      <b>com134:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Livet, Han gav af D\u00f8de \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Fordi <a  class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesus</a> d\u00f8de p\u00e5 korset og opstod, kan ogs\u00e5 de troende opst\u00e5 fra de d\u00f8de og f\u00e5 evigt liv,\u000a         jf. bl.a. Joh 11,25-26.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com134','comment')
INSERT INTO TEXTREFERENCE VALUES(10394,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com37">\u000a      <b>com37:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Mark \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">her ogs\u00e5 i betydningen Danmark.</div>\u000a      \u000a   </div>','scrollTarget_com37','comment')
INSERT INTO TEXTREFERENCE VALUES(10395,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com38">\u000a      <b>com38:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>tusind Aar \u25aa \u000a      <span class="initial">T</span>usind Aar \u25aa tusind Aar \u25aa tusind Aar \u25aa tusind Aar \u25aa tusind Aar \u25aa tusind Aar \u25aa tusind Aar \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">tusind\u00e5rsjubil\u00e6et for <a  class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a>s komme til Danmark i 826 og den danske kirkes grundl\u00e6ggelse. Jubil\u00e6et blev fejret\u000a         1. pinsedag, d. 14. maj 1826. Om fejringen af jubil\u00e6et, <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com38','comment')
INSERT INTO TEXTREFERENCE VALUES(10396,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com39">\u000a      <b>com39:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>plante vi May i Kirke-Stol \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Maj var i gammel tid ogs\u00e5 betegnelse for nyudsprungne grene, is\u00e6r af b\u00f8g, som det\u000a         var skik at bruge til at udsmykke kirken med under pinsefesten.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com39','comment')
INSERT INTO TEXTREFERENCE VALUES(10397,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com40">\u000a      <b>com40:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Liv og Lyst \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">om udtrykkene \u201cLiv\u201d og \u201cLyst\u201d, <a  class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com40','comment')
INSERT INTO TEXTREFERENCE VALUES(10398,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com41">\u000a      <b>com41:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Til mindelig \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) til minde om eller v\u00e6rd at mindes (se <a  class="persName" href="ajax/getReference/pe293" rel="ajax/getReference/pe293">Uffe Hansen</a> 1837, bind 1, s. 125). \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com41','comment')
INSERT INTO TEXTREFERENCE VALUES(10399,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com42">\u000a      <b>com42:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Pindse-Gave \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">I billedet indg\u00e5r bl.a. fejringen af den danske kirkes tusind\u00e5rsjubil\u00e6um 1. pinsedag,\u000a         d. 14. maj 1826, og pinseunderet, ApG 2,1-13.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com42','comment')
INSERT INTO TEXTREFERENCE VALUES(10400,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com135">\u000a      <b>com135:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Da rinde vel og, som B\u00e6kke smaa,\u000a      Fra \u00d8ine os Taarer milde\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Et ofte brugt billede i is\u00e6r <a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>s barokke salmedigtning, se f.eks. f\u00f8lgende citat fra Hierte-Suk til den 13. salme:\u000a         \u201cHvorfor skal jeg Arme gaa / Daglig spendt i Korsens Hilde, / og i \u00d8ynis Taare trilde\u000a         / Sindets Smerter i mit Ski\u00f8d? / Hvorfor skal jeg Ene velte / Sorgens Steen, som ey\u000a         vil smelte, / Om min Graad, som Elven, fl\u00f8d?\u201d (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a> 1941 [1681]</span>, bind 3, s. 232).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com135','comment')
INSERT INTO TEXTREFERENCE VALUES(10401,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com43">\u000a      <b>com43:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>B\u00e6kke i Flok de gi\u00f8r en Aa \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. ordsproget: \u201cMange B\u00e6kke og smaa gj\u00f8re en stor Aa\u201d (<span  class="rs_title"><a class="persName" href="ajax/getReference/pe288" rel="ajax/getReference/pe288">Mau</a> 1879</span>, nr. 6323).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com43','comment')
INSERT INTO TEXTREFERENCE VALUES(10402,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com44">\u000a      <b>com44:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Hjerte-Suk \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. <a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>s genre, Hierte-Suk, i hans <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a            <span xmlns="" class="italic">Aandelige Siunge-Koor</span>s Anden Part</span>, 1681. I f.eks. Hierte-Suk til den 13. salme  sammenk\u00e6des menneskets t\u00e5rer og vandl\u00f8bene\u000a         i naturen i en billedbrug, der ofte ses hos <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a>(<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a> 1941 [1681], bind 3, s. 232 f.).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com44','comment')
INSERT INTO TEXTREFERENCE VALUES(10403,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com45">\u000a      <b>com45:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Alt \u25aa alt \u25aa alt \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) allerede.</div>\u000a      \u000a   </div>','scrollTarget_com45','comment')
INSERT INTO TEXTREFERENCE VALUES(10404,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com46">\u000a      <b>com46:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>end \u25aa end \u25aa End \u25aa end \u25aa End \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) endnu.</div>\u000a      \u000a   </div>','scrollTarget_com46','comment')
INSERT INTO TEXTREFERENCE VALUES(10405,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com47">\u000a      <b>com47:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Som aldrig saa lang er nogen Dag,\u000a      At Aften er jo i Vente,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne er n\u00e6sten pr\u00e6cise citater af 7. strofe, vers 1-2 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569]</span>, blad 317 verso): \u201cDagen hand er aldrig saa lang / At afftenen maa wi jo vente\u201d.\u000a         Jf. desuden ordsproget: \u201cDagen er aldrig saa lang, at Aften maa vi jo vente\u201d (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe288" rel="ajax/getReference/pe288">Mau</a> 1879</span>, nr. 1182).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com47','comment')
INSERT INTO TEXTREFERENCE VALUES(10406,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com54">\u000a      <b>com54:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>jo \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) ikke</div>\u000a      \u000a   </div>','scrollTarget_com54','comment')
INSERT INTO TEXTREFERENCE VALUES(10407,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com48">\u000a      <b>com48:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>haver \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">har. Formen var endnu almindelig.</div>\u000a      \u000a   </div>','scrollTarget_com48','comment')
INSERT INTO TEXTREFERENCE VALUES(10408,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com49">\u000a      <b>com49:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>og \u25aa og \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) ogs\u00e5.</div>\u000a      \u000a   </div>','scrollTarget_com49','comment')
INSERT INTO TEXTREFERENCE VALUES(10409,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com50">\u000a      <b>com50:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Solbjergs-Lag \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. solbj\u00e6rgslag; \u201cSolens Nedgangs-Tid\u201d, s\u00e5dan som <a  class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> oplyser i den lille fortale til salmen (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a            <span xmlns="" class="italic">H\u00f8itids-Psalmer</span> 1826</span>, [s. 2]).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com50','comment')
INSERT INTO TEXTREFERENCE VALUES(10410,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com51">\u000a      <b>com51:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>immer \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) stadigt, bestandigt.</div>\u000a      \u000a   </div>','scrollTarget_com51','comment')
INSERT INTO TEXTREFERENCE VALUES(10411,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com52">\u000a      <b>com52:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Pindse-Dag \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) ogs\u00e5 jubil\u00e6umsdagen, 1. pinsedag, den 14. maj 1826.</div>\u000a      \u000a   </div>','scrollTarget_com52','comment')
INSERT INTO TEXTREFERENCE VALUES(10412,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com53">\u000a      <b>com53:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Med Straaler i Krands om Tinde \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. lysmetaforikken fra forklarelsen p\u00e5 bjerget, Matt 17,1-2. Metaforen alluderer ogs\u00e5 til den billedlige, symbolske fremstilling af Gud som solgissel,\u000a         dvs. Jahve indskrevet p\u00e5 hebr\u00e6isk i solskiven, omkranset af t\u00e6tte str\u00e5ler. Fremstillingen\u000a         har baggrund i \u00c5b 12,1-2 og var almindeligt kendt. Bl.a. indg\u00e5r symbolet i et kobberstik p\u00e5 titelbladet af\u000a         f\u00f8rste bind af <span  class="rs_title">\u000a            <span xmlns="" class="italic">Theologisk Maanedsskrift,</span></span> udgivet af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> og <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe275" rel="ajax/getReference/pe275">A.G. Rudelbach</a> i 1825.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com53','comment')
INSERT INTO TEXTREFERENCE VALUES(10413,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com55">\u000a      <b>com55:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Saa frydelig sig den Sidste snoer\u000a      Op under de gr\u00f8nne Linde!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjerne har forl\u00e6g i 6. strofe, vers 5-6 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a> 1617 [1569], blad 317 verso): \u201cDe Str\u00f8mme de rinde saa frydelig / Op under de gr\u00f8ne\u000a         Linde\u201d. \u201cunder Linde\u201d er sandsynligvis en forvanskning af \u201cunder lide\u201d (dvs. bakkeskr\u00e5ning),\u000a         der er en ofte anvendt formel i folkeviser. En <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">svensk version</span> af dagvisen fra 1450 har: \u201ctha str\u00f8mbrin swa gladhelika / under the gr\u00f8no lidha\u201d\u000a         (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe291" rel="ajax/getReference/pe291">Balslev-Clausen</a> 2010a, s. 103 f).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com55','comment')
INSERT INTO TEXTREFERENCE VALUES(10414,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com56">\u000a      <b>com56:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Dagen \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. bl.a. den eskatologiske beskrivelse af den nye himmel og jord og det ny Jerusalem\u000a         i \u00c5b 21. \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com56','comment')
INSERT INTO TEXTREFERENCE VALUES(10415,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com59">\u000a      <b>com59:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Saa reise vi til vort F\u00e6dre-Land,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Verslinjen har forl\u00e6g i 9. strofe, vers 1 af morgensalmen \u201c<span  class="rs_title">Den gamle Christelige Dageuise</span>\u201d (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe37" rel="ajax/getReference/pe37">Thomiss\u00f8n</a></span> 1617[1569], blad 317 verso): \u201cNaar wi skulle til vort F\u00e6derne Land\u201d.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com59','comment')
INSERT INTO TEXTREFERENCE VALUES(10416,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com60">\u000a      <b>com60:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>vort F\u00e6dre-Land, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Hebr 11,14-16.</div>\u000a      \u000a   </div>','scrollTarget_com60','comment')
INSERT INTO TEXTREFERENCE VALUES(10417,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com61">\u000a      <b>com61:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Der ligger ei Dag i Dvale \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. \u00c5b 21,25.</div>\u000a      \u000a   </div>','scrollTarget_com61','comment')
INSERT INTO TEXTREFERENCE VALUES(10418,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com62">\u000a      <b>com62:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Der stander en Borg, saa prud og grand,\u000a      Med Gammen i gyldne Sale,\u000a      Saa frydelig der, til evig Tid,\u000a      Med Venner i Lys vi tale!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">allusion til beskrivelsen fra eddadigtet \u201c<span  class="rs_title">V\u00f8lvens Sp\u00e5dom</span>\u201d af de lykkelige tider efter <a xmlns="http://www.w3.org/1999/xhtml" class="myth rs_myth" href="ajax/getReference/myth104" rel="ajax/getReference/myth104">Ragnarok</a> i den nordiske mytologi. I en vision ser <a xmlns="http://www.w3.org/1999/xhtml" class="myth rs_myth" href="ajax/getReference/myth120" rel="ajax/getReference/myth120">v\u00f8lven</a> en ny jord stige op af havet, og hun ser borgen <a xmlns="http://www.w3.org/1999/xhtml" class="myth rs_myth" href="ajax/getReference/myth121" rel="ajax/getReference/myth121">Gimle</a>, i hvis gyldne sale der leves lyst og lykkeligt. Se \u201cV\u00f8lvens Sp\u00e5dom\u201d, str. 64:\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i> \u000a         <div class="p">\u201cSal ser hun knejse / lys som Solen, / gyldent t\u00e6kket / i Gimle staa; / der skal skyldfri\u000a            / Skarer m\u00f8des, / og i Lys leve / Lykkedage.\u201d (\u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">V\u00f8lvens Sp\u00e5dom</span>\u201d, i <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a               <span xmlns="" class="italic">Den \u00e6ldre Edda og Eddica minora</span></span> (1943-46), overs. af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe155" rel="ajax/getReference/pe155">Martin Larsen</a>, bind 1, s. 73).\u000a         </div>  \u000a      </div> \u000a      \u000a      \u000a   </div>','scrollTarget_com62','comment')
INSERT INTO TEXTREFERENCE VALUES(10419,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com63">\u000a      <b>com63:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>grand \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">statelig, pr\u00e6gtig</div>\u000a      \u000a   </div>','scrollTarget_com63','comment')
INSERT INTO TEXTREFERENCE VALUES(10420,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com64">\u000a      <b>com64:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>gyldne Sale \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. ogs\u00e5 \u201cstaden var reent guld\u201d (\u00c5b 21,18, \u000a         <span class="italic">Biblia</span> 1787).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com64','comment')
INSERT INTO TEXTREFERENCE VALUES(10421,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com65">\u000a      <b>com65:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Med Venner i Lys vi tale \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. forestillingen om det lykkelige gensyn med venner i det evige liv, der var en\u000a         udbredt forestilling i Grundtvigs samtid.\u000a      </div>\u000a      \u000a      \u000a   </div>','scrollTarget_com65','comment')
INSERT INTO TEXTREFERENCE VALUES(10422,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com66">\u000a      <b>com66:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Mel. Som den gyldne Soel frembryder.frembryder, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"><a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>s salme \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Som den gyldne Soel frembryder</span>\u201d blev sunget p\u00e5 en melodi, der var komponeret af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe296" rel="ajax/getReference/pe296">Johann Schop</a> i 1642. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a>s samtid kendte melodien som nr. 88 i <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe295" rel="ajax/getReference/pe295">Zinck</a>s koralbog</span> fra 1801, der var den seneste, officielle koralbog. <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">Se indledningen</a>.\u000a      </div> \u000a      \u000a   </div>','scrollTarget_com66','comment')
INSERT INTO TEXTREFERENCE VALUES(10423,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com68">\u000a      <b>com68:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Christi Kirke,\u000a      Paa sin Klippe\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. at kirkens grundvold er <a  class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Kristus</a> selv, jf. 1 Kor 3,11 og Ef 2,20 f. Jf. ogs\u00e5 lignelsen om at bygge sit hus p\u00e5 klippen og p\u00e5 sand, Matt 7,24-27.\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>  \u000a         \u000a         <div class="p">Jf. 1 Kor 3,10 og Matt 16,18, hvor <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesus</a> siger til apostelen <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe306" rel="ajax/getReference/pe306">Simon Peter</a>, \u201cdu er Petrus [lat. af gr. \u03a0\u03ad\u03c4\u03c1\u03bf\u03c2, klippe] og paa denne Klippe vil jeg bygge min\u000a            Menighed\u201d (\u000a            <span class="italic">Biblia </span>1787). I Det Gamle Testamente bruges klippe som billede p\u00e5 Jahves beskyttelse og styrke\u000a            med betoning af det faste og sikre, se f.eks. Sl 31,3-4.\u000a         </div>\u000a         \u000a      </div> \u000a      \u000a   </div>','scrollTarget_com68','comment')
INSERT INTO TEXTREFERENCE VALUES(10424,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com69">\u000a      <b>com69:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Dannevirke \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">hentydning til voldanl\u00e6gget i forl\u00e6ngelse af Slien, der i \u00e5rene ca. 600-1200 beskyttede\u000a         Jyllands sydgr\u00e6nse. P\u00e5 <a  class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a>s tid blev Dannevirke opfattet som det symbolske v\u00e6rn mod faren fra syd.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com69','comment')
INSERT INTO TEXTREFERENCE VALUES(10425,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com70">\u000a      <b>com70:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Fiendens Frygt \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) trussel, frygt for fjenden.</div>\u000a      \u000a   </div>','scrollTarget_com70','comment')
INSERT INTO TEXTREFERENCE VALUES(10426,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com71">\u000a      <b>com71:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Kunde Tr\u00e6, og Tag af Straa,\u000a      End i Ilden ei bestaae,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. 1 Kor 3,12-13.</div>\u000a      \u000a   </div>','scrollTarget_com71','comment')
INSERT INTO TEXTREFERENCE VALUES(10427,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com72">\u000a      <b>com72:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Altid dog, fra h\u00f8ie Taarne,\u000a      K\u00e6mped for os den Eenbaarne!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Sl 61,3-4.</div>\u000a      \u000a   </div>','scrollTarget_com72','comment')
INSERT INTO TEXTREFERENCE VALUES(10428,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com73">\u000a      <b>com73:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Gud! for Dig, som aldrig \u00e6ldes,\u000a      Tiden som en Time gled,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Sl 102,28 og 2. Pet 3,8.</div>\u000a      \u000a   </div>','scrollTarget_com73','comment')
INSERT INTO TEXTREFERENCE VALUES(10429,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com74">\u000a      <b>com74:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Thi er tusind Aar os lange \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. 2. Pet 3,8.</div>\u000a      \u000a   </div>','scrollTarget_com74','comment')
INSERT INTO TEXTREFERENCE VALUES(10430,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com75">\u000a      <b>com75:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Christne F\u00e6dre \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">hentyder ogs\u00e5 til kirkef\u00e6drene, indflydelsesrige teologer fra oldkirken (indtil ca.\u000a         500).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com75','comment')
INSERT INTO TEXTREFERENCE VALUES(10431,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com76">\u000a      <b>com76:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>I dit Huus, med Tunger nye \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">kan henf\u00f8res til pinseunderet, ApG 2,2, og til den eskatologiske takkesang i de himmelske boliger, jf. strofe 8.</div>\u000a      \u000a   </div>','scrollTarget_com76','comment')
INSERT INTO TEXTREFERENCE VALUES(10432,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com77">\u000a      <b>com77:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>alle gode Gaver \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Jak 1,17: \u201cAlle gode og fuldkomne gaver kommer ned fra oven [...]\u201d. Teksten er brugt af <a  class="persName" href="ajax/getReference/pe297" rel="ajax/getReference/pe297">Matthias Claudius</a> i et digt fra 1782: \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Im Anfang wars auf Erden</span>\u201d, hvor det i omkv\u00e6det lyder: \u201cAlle gute Gabe / Kam oben her von Gott\u201d. Digtet blev\u000a         senere \u00e6ndret til h\u00f8stsalmen \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Wir pfl\u00fcgen und wir streuen</span>\u201d ((1800) \u000a         <span class="italic">Melodien f\u00fcr Volksschulen,</span> Hannover) (Kj\u00e6rgaard 2003, bind 2 nr. 730).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com77','comment')
INSERT INTO TEXTREFERENCE VALUES(10433,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com78">\u000a      <b>com78:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Mark \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">her ogs\u00e5 i betydningen Danmark.</div>\u000a      \u000a   </div>','scrollTarget_com78','comment')
INSERT INTO TEXTREFERENCE VALUES(10434,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com79">\u000a      <b>com79:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>alle Paver \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">For <a  class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> er der ikke kun tale om den katolske kirkes overhoved, men ogs\u00e5 om indflydelsesrige\u000a         teologer, som <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> mente forvanskede kristendommen.\u000a         \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com79','comment')
INSERT INTO TEXTREFERENCE VALUES(10435,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com80">\u000a      <b>com80:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Med-B\u00f8r, Fader, til god Havn,\u000a      Ved din Aand, i Jesu Navn!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">det fra antikken brugte billede om livet som en sejlads, f.eks. i barokkens digtning.</div>\u000a      \u000a   </div>','scrollTarget_com80','comment')
INSERT INTO TEXTREFERENCE VALUES(10436,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com81">\u000a      <b>com81:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Saa \u25aa Saa \u25aa saa \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) s\u00e5ledes.</div>\u000a      \u000a   </div>','scrollTarget_com81','comment')
INSERT INTO TEXTREFERENCE VALUES(10437,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com83">\u000a      <b>com83:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Ansgarius \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">latiniseret form af <a  class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a>.\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>  \u000a         \u000a         <div class="p">Ansgar (801-65), munk, mission\u00e6r og \u00e6rkebiskop, kaldet \u201cNordens apostel\u201d. Ansgar voksede\u000a            op i benediktinerklosteret Corbie i Flandern og var som ung med til at grundl\u00e6gge\u000a            datterklosteret Corvey i Sachsen. P\u00e5 opfordring af den frankiske kejser <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe298" rel="ajax/getReference/pe298">Ludvig 1. den Fromme</a> fulgte han i 826 med den danske kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> til Danmark. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> og hans familie var lige blevet d\u00f8bt ved kejserens hof, og Ansgars opgave var at\u000a            st\u00f8tte dem i deres trosforhold og at missionere i Danmark. Ansgar og hans f\u00f8lgesvend,\u000a            munken <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe300" rel="ajax/getReference/pe300">Autbert</a>, havde dog kun ringe succes, bl.a. fordi <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> blev fordrevet allerede \u00e5ret efter. I 830 tog Ansgar til handelsbyen Birka i Sverige,\u000a            hvor hans kristne mission havde bedre fremgang. 831 udn\u00e6vntes han til \u00e6rkebiskop i\u000a            det nyoprettede bisped\u00f8mme i Hamburg. Pga. et vikingeangreb 845 blev bispes\u00e6det flyttet\u000a            til Bremen, som bisped\u00f8mmet blev sammenlagt med efter en del politiske forviklinger.\u000a            I Danmark l\u00e5 missionen stille, men efter diplomatiske forhandlinger med kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe301" rel="ajax/getReference/pe301">Erik (Horik) 1.</a> fik Ansgar ca. 850 bygget en kirke p\u00e5 handelspladsen Hedeby ved Slesvig. Ca. 855\u000a            fik han af kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe302" rel="ajax/getReference/pe302">Horik 2. Barn</a> en grund i Ribe til et kirkebyggeri. Ansgars liv er skildret af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe303" rel="ajax/getReference/pe303">Rimbert</a>, hans medarbejder og efterf\u00f8lger som \u00e6rkebiskop, i helgenbiografien <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a               <span xmlns="" class="italic">Vita Anskarii</span></span> (Ansgars levned).\u000a         </div>\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com83','comment')
INSERT INTO TEXTREFERENCE VALUES(10438,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com84">\u000a      <b>com84:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Balle \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Biskop <a  class="persName" href="ajax/getReference/pe39" rel="ajax/getReference/pe39">Nicolai Edinger Balle</a>, der var oplysningsteolog, ans\u00e5s af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> for at v\u00e6re blandt de levende forkyndere af kristendommen, fordi han aktivt bek\u00e6mpede\u000a         rationalismen og skabte en v\u00e6kkelse i statskirken. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> havde bl.a. hyldet <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe39" rel="ajax/getReference/pe39">Balle</a> i \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Nikolai Edinger <a class="persName" href="ajax/getReference/pe39" rel="ajax/getReference/pe39">Balle</a>s Amindelse\u201d</span> i \u000a         <span class="italic">Danne-Virke</span> 2 1817, s. 48-64. Fornavnet Nicolai havde <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> fra <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe39" rel="ajax/getReference/pe39">Balle</a>, der var gift med <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a>s faster.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com84','comment')
INSERT INTO TEXTREFERENCE VALUES(10439,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com85">\u000a      <b>com85:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Skjold \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">den danske kongesl\u00e6gt fra sagntiden, skjoldungerne, der var efterkommere af <a  class="myth rs_myth" href="ajax/getReference/myth67" rel="ajax/getReference/myth67">Skjold</a>, den f\u00f8rste konge i Lejre.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com85','comment')
INSERT INTO TEXTREFERENCE VALUES(10440,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com86">\u000a      <b>com86:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Frode \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">sagnkonger af skjoldungersl\u00e6gten. Den mest fremtr\u00e6dende var <a  class="myth rs_myth" href="ajax/getReference/myth137" rel="ajax/getReference/myth137">Frode Fredegod.</a></div>\u000a      \u000a   </div>','scrollTarget_com86','comment')
INSERT INTO TEXTREFERENCE VALUES(10441,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com87">\u000a      <b>com87:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Igienf\u00f8dt Du lod opstaae, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. b\u00f8nnen i d\u00e5bsritualet lige efter over\u00f8sningen: \u201cDen Alm\u00e6gtigste GUd, vor \u000a         <span class="italic">HErres</span> <a  class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">JEsu Christi</a> Fader, som \u000a         <span class="italic">dig</span> nu haver \u000a         <span class="italic">igienf\u00f8d</span> formedelst Vand og den Hellig Aand, og \u000a         <span class="italic">haver</span> forladt dig alle dine \u000a         <span class="italic">Synder,</span> han styrke dig med sin Naade \u000a         <span class="italic">til</span> det \u00e6vige \u000a         <span class="italic">Liv! Amen!</span>\u201d (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">(1762) \u000a            <span xmlns="" class="italic">Dannemarkes og Norges Kirke-Ritual. Med Kongelig Allernaadigst Privilegio</span></span>. K\u00f8benhavn; s. 69 f.).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com87','comment')
INSERT INTO TEXTREFERENCE VALUES(10442,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com88">\u000a      <b>com88:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Siden Daab og Christendom\u000a      Med Kong Harald til os kom!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"><a  class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a>, hans familie og f\u00f8lge blev i 826 d\u00f8bt ved den frankiske kejser <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe298" rel="ajax/getReference/pe298">Ludvig 1. den Fromme</a>s hof. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a> fulgte med kongefamilien tilbage til Danmark for at st\u00f8tte dem i den nye tro og missionere\u000a         i Danmark. F\u00f8rst p\u00e5 <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe299" rel="ajax/getReference/pe299">Harald Bl\u00e5tand</a>s tid op mod \u00e5rtusindskiftet bredte kristendommen sig for alvor i landet.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com88','comment')
INSERT INTO TEXTREFERENCE VALUES(10443,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com89">\u000a      <b>com89:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/><a  class="persName" href="ajax/getReference/pe41" rel="ajax/getReference/pe41">Christian</a>-Stammen \u25aa <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe41" rel="ajax/getReference/pe41">Christjan</a>-Stammen \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. den oldenborgske kongesl\u00e6gt, hvis f\u00f8rste led var <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe41" rel="ajax/getReference/pe41">Christian 1.</a></div>\u000a      \u000a   </div>','scrollTarget_com89','comment')
INSERT INTO TEXTREFERENCE VALUES(10444,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com90">\u000a      <b>com90:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Du, som Ydmygheden \u00e6rer, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. 1. Pet 5,5.</div>\u000a      \u000a   </div>','scrollTarget_com90','comment')
INSERT INTO TEXTREFERENCE VALUES(10445,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com91">\u000a      <b>com91:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Stammens Krone paa vor Fest \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Kong <a  class="persName" href="ajax/getReference/pe96" rel="ajax/getReference/pe96">Frederik 6.</a> Om brug af tr\u00e6metaforikken og allusionerne til barokkens billedsprog, <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com91','comment')
INSERT INTO TEXTREFERENCE VALUES(10446,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com93">\u000a      <b>com93:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Pindse-May \u25aa Pindse-May \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Maj var i gammel tid ogs\u00e5 betegnelse for nyudsprungne grene, is\u00e6r af b\u00f8g, som det\u000a         var skik at bruge til at udsmykke kirken og hjemmet med under pinsefesten.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com93','comment')
INSERT INTO TEXTREFERENCE VALUES(10447,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com96">\u000a      <b>com96:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>i dit Jerusalem,\u000a      Hvor vi glemme St\u00f8vets Hjem,\u000a      Takke-Psalmen Christne sjunge,\u000a      Alle med hverandres Tunge!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. pinseunderet i Jerusalem, ApG 2,1-13, og gentagelsen i det eskatologiske, himmelske Jerusalem, \u00c5b 5,9 og 7,9-10.</div>\u000a      \u000a   </div>','scrollTarget_com96','comment')
INSERT INTO TEXTREFERENCE VALUES(10448,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com97">\u000a      <b>com97:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>i dit <a  class="placeName" href="ajax/getReference/fak10" rel="ajax/getReference/fak10">Jerusalem</a> \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. \u00c5b 21,10.</div>\u000a      \u000a   </div>','scrollTarget_com97','comment')
INSERT INTO TEXTREFERENCE VALUES(10449,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com98">\u000a      <b>com98:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>St\u00f8vets \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">St\u00f8v er i de \u00e6ldre overs\u00e6ttelser af b\u00e5de Det Gamle og Det Nye Testamente en almindeligt\u000a         brugt betegnelse for menneskets tilknytning til det jordiske, f.eks. \u201cthi du er st\u00f8v\u000a         og skal blive til st\u00f8v igien\u201d (\u000a         <span class="italic">Biblia</span> 1787, 1 Mos 3,19).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com98','comment')
INSERT INTO TEXTREFERENCE VALUES(10450,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com92">\u000a      <b>com92:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Mel. Klar op mit Hjerte, Sjel og Sind. \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"><a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>s salme \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Klar op mit Hjerte, Sjel og Sind</span>\u201d blev i <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a>s samtid sunget p\u00e5 en melodi, der var komponeret af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe322" rel="ajax/getReference/pe322">Valentin Schumann</a> i 1539. <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">Se indledningen</a>.\u000a      </div> \u000a      \u000a   </div>','scrollTarget_com92','comment')
INSERT INTO TEXTREFERENCE VALUES(10451,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com99">\u000a      <b>com99:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Klar op, du Sky paa \u00d8ien-Bryn,\u000a      Og skjul ei Solen for vort Syn!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">allusion til billedbrugen i 1. strofe af <a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>: \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Som den gyldne Soel frembryder</span> / Giennem dend kulsorte Sky / og sin Straale-Glands udskyder, / Saa at M\u00f8rk og Molm\u000a         maa fly\u201d (i <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a> 1941 [1689]</span>, s. 513).\u000a      </div>   \u000a      \u000a   </div>','scrollTarget_com99','comment')
INSERT INTO TEXTREFERENCE VALUES(10452,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com100">\u000a      <b>com100:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>\u000a      <span class="initial">K</span>lar op, du Sky \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Optakten har forl\u00e6g i selvanr\u00e5belsen hos <a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>: \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Klaer op mit Hierte, Si\u00e6l og Sind</span>\u201d (\u000a         <span class="italic"><span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a>s salmebog</span> \u000a         </span> 1943 [1699], salme til 1. pinsedag, aftensang til indgang, s. 43-45).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com100','comment')
INSERT INTO TEXTREFERENCE VALUES(10453,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com101">\u000a      <b>com101:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>K\u00e6mpe-V\u00e6rk \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. skaberv\u00e6rket og dets opretholdelse.</div>\u000a      \u000a   </div>','scrollTarget_com101','comment')
INSERT INTO TEXTREFERENCE VALUES(10454,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com102">\u000a      <b>com102:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Guds Haand er st\u00e6rk \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Sl 89,14; Sl 136,12; se ogs\u00e5 adskillige parallelsteder is\u00e6r i Moseb\u00f8gerne, f.eks. 5 Mos 3,24. I det Gamle Testamente st\u00e5r Guds h\u00e5nd ofte som en konkret omskrivning af hans magt\u000a         og styrke.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com102','comment')
INSERT INTO TEXTREFERENCE VALUES(10455,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com103">\u000a      <b>com103:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Hans det gode Ord bestaaer, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Sl 119,89: \u201cHERRE, dit ord bestaaer evindelig i himlene\u201d (\u000a         <span class="italic">Biblia</span> 1787); jf. ogs\u00e5 Es 40,8.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com103','comment')
INSERT INTO TEXTREFERENCE VALUES(10456,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com104">\u000a      <b>com104:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Alt som en Dag, saa tusind Aar\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Sl 90,4: \u201cFordi tusinde aar ere for dine \u00f8yne som den dag i gaar [...]\u201d (\u000a         <span class="italic">Biblia </span>1787); jf. ogs\u00e5 2 Pet 3,8.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com104','comment')
INSERT INTO TEXTREFERENCE VALUES(10457,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com105">\u000a      <b>com105:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Alt som \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">ligesom.</div>\u000a      \u000a   </div>','scrollTarget_com105','comment')
INSERT INTO TEXTREFERENCE VALUES(10458,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com108">\u000a      <b>com108:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Vor Pindse-Fest vi holde nu, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Festlighederne i forbindelse med tusind\u00e5rsjubil\u00e6et var planlagt til at blive afholdt\u000a         1. pinsedag, d. 14. maj 1826. Verslinjen er et n\u00f8jagtigt citat fra <a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Thomas Kingo</a>: \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Klaer op mit Hierte, Si\u00e6l og Sind</span>\u201d, 1. strofe, 3. vers (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a            <span xmlns="" class="italic"><a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a>s salmebog \u000a            </span> 1943 [1699]</span>, s. 42).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com108','comment')
INSERT INTO TEXTREFERENCE VALUES(10459,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com109">\u000a      <b>com109:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Du lod h\u00f8re Livets Ord \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">allusion til 1 Joh 1: \u201cDet, som var fra begyndelsen, det, som vi har h\u00f8rt, det, som vi har set med vore\u000a         \u00f8jne, det, som vi betragtede og vore h\u00e6nder r\u00f8rte ved: livets ord\u201d; jf. ogs\u00e5 Joh 6,68.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com109','comment')
INSERT INTO TEXTREFERENCE VALUES(10460,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com110">\u000a      <b>com110:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>I Tungers gl\u00f8dende Gestalt,\u000a      Din Aand paa Herrens Venner faldt,\u000a      Saa hvad de h\u00f8rde, hvad de saae,\u000a      Dem levende paa Tungen laae,\u000a      Og L\u00e6ben blev Din Naades Tolk,\u000a      Med Lys og Liv, for alle Folk!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"> jf. beretningen om pinseunderet p\u00e5 den f\u00f8rste pinsedag i ApG 2,1-13. 3. strofe har forl\u00e6g i 3. strofe af <a  class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a>s \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Klaer op mit Hierte, Si\u00e6l og Sind</span>\u201d: \u201cI Tungers br\u00e6ndende Gestalt / Oppaa Apostlerne du falt / Strax <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">JEsus</a> dem paa Tungen laa, / De kunde tale og forstaa / Hver Tungemaal, hvor hen de kom,\u000a         / At breede ud hans HEerredom\u201d (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a            <span xmlns="" class="italic"><a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe294" rel="ajax/getReference/pe294">Kingo</a>s salmebog\u000a            </span></span>1943 [1699], s. 43 strofe 3).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com110','comment')
INSERT INTO TEXTREFERENCE VALUES(10461,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com111">\u000a      <b>com111:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Lys og Liv \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Joh 1,4: \u201cI det [Ordet] var livet, og livet var menneskenes lys\u201d (\u000a         <span class="italic">Biblia</span> 1787). Om udtrykkene \u201cLys\u201d og \u201cLiv\u201d, <a  class="comment_ref">se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com111','comment')
INSERT INTO TEXTREFERENCE VALUES(10462,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com112">\u000a      <b>com112:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Gnister af den Gl\u00f8d,\u000a      Du f\u00f8rste Pindse-Dag udgi\u00f8d,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. beretningen om pinseunderet p\u00e5 den f\u00f8rste pinsedag i ApG 2,1-13.</div>\u000a      \u000a   </div>','scrollTarget_com112','comment')
INSERT INTO TEXTREFERENCE VALUES(10463,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com114">\u000a      <b>com114:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Urokket, alt i tusind Aar, \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Selv om det lykkedes <a  class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a> at bygge en kirke i Hedeby og skaffe en grund til et kirkebyggeri i Ribe, var hans\u000a         missionsvirksomhed i Danmark kun en episode. F\u00f8rst 100 \u00e5r efter Ansgars d\u00f8d med bl.a.\u000a         <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe299" rel="ajax/getReference/pe299">Harald Bl\u00e5tand</a>s omvendelse ca. 960 bredte kristendommen sig for alvor i landet.\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>\u000a         \u000a         <div class="p">Ansgar (801-65), munk, mission\u00e6r og \u00e6rkebiskop, kaldet \u201cNordens apostel\u201d. Ansgar voksede\u000a            op i benediktinerklosteret Corbie i Flandern og var som ung med til at grundl\u00e6gge\u000a            datterklosteret Corvey i Sachsen. P\u00e5 opfordring af den frankiske kejser <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe298" rel="ajax/getReference/pe298">Ludvig 1. den Fromme</a> fulgte han i 826 med den danske kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> til Danmark. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> og hans familie var lige blevet d\u00f8bt ved kejserens hof, og Ansgars opgave var at\u000a            st\u00f8tte dem i deres trosforhold og at missionere i Danmark. Ansgar og hans f\u00f8lgesvend,\u000a            munken <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe300" rel="ajax/getReference/pe300">Autbert</a>, havde dog kun ringe succes, bl.a. fordi <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> blev fordrevet allerede \u00e5ret efter. I 830 tog Ansgar til handelsbyen Birka i Sverige,\u000a            hvor hans kristne mission havde bedre fremgang. 831 udn\u00e6vntes han til \u00e6rkebiskop i\u000a            det nyoprettede bisped\u00f8mme i Hamburg. Pga. et vikingeangreb 845 blev bispes\u00e6det flyttet\u000a            til Bremen, som bisped\u00f8mmet blev sammenlagt med efter en del politiske forviklinger.\u000a            I Danmark l\u00e5 missionen stille, men efter diplomatiske forhandlinger med kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe301" rel="ajax/getReference/pe301">Erik (Horik) 1.</a> fik Ansgar ca. 850 bygget en kirke p\u00e5 handelspladsen Hedeby ved Slesvig. Ca. 855\u000a            fik han af kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe302" rel="ajax/getReference/pe302">Horik 2. Barn</a> en grund i Ribe til et kirkebyggeri. Ansgars liv er skildret af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe303" rel="ajax/getReference/pe303">Rimbert</a>, hans medarbejder og efterf\u00f8lger som \u00e6rkebiskop, i helgenbiografien <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a               <span xmlns="" class="italic">Vita  <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Anskarii</a></span></span> (Ansgars levned).\u000a         </div>\u000a         \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com114','comment')
INSERT INTO TEXTREFERENCE VALUES(10464,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com115">\u000a      <b>com115:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Klippe-Grund \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Det grundl\u00e6ggende i kirken er <a  class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesus Kristus</a> selv, jf. 1 Kor 3,11. I Matt 16,18 siger <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesus</a> til apostelen <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe306" rel="ajax/getReference/pe306">Simon Peter</a>: \u201cdu er Petrus [lat. af gr. \u03a0\u03ad\u03c4\u03c1\u03bf\u03c2, klippe] og paa denne klippe vil jeg bygge min\u000a         menighed\u201d (\u000a         <span class="italic">Biblia </span>1787); jf. en kladde til denne strofes 5. og 6. verslinje: \u201cOpbygt en hellig Aften-Stund\u000a         / Paa Ordets faste Klippe-Grund!\u201d (ms. i fasc. 382, strofe 5; se varianter).\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>\u000a         \u000a         <div class="p">Jf. ogs\u00e5 lignelsen om at bygge sit hus p\u00e5 klippen og p\u00e5 sand (Matt 7,24-27). Jf. desuden den gammeltestamentlige brug af klippe som billede p\u00e5 Jahves beskyttelse\u000a            og styrke med betoning af det faste og sikre, f.eks. Sl 18,32; Sl 31,3-4. F\u00e5 m\u00e5neder tidligere brugte <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> udtrykket i \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Zions Sang. T\u00f8r end Nogen ihukomme</span>\u201d: \u201cUnder Sang vi atter bygge / Paa den gamle Klippe-Grund!\u201d (i <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a               <span xmlns="" class="italic">Theologisk Maanedsskrift</span></span> 3 1825, [s. V-VIII] strofe 1). Salmen, der har kirkens genopbygning som et fremtr\u00e6dende\u000a            tema, kan ses som foranlediget af striden efter <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a               <span xmlns="" class="italic">Kirkens Gienm\u00e6le</span></span> 1825, hvor <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> bl.a. h\u00e6vdede, at kristendommens grundvold var trosbekendelsen ved d\u00e5b og nadver.\u000a         </div>\u000a         \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com115','comment')
INSERT INTO TEXTREFERENCE VALUES(10465,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com116">\u000a      <b>com116:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Stjerne \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. stjernen over Betlehem, Matt 2,9 og den bagvedliggende profeti om Jakobsstjernen, 4. Mos 24,17.</div>\u000a      \u000a   </div>','scrollTarget_com116','comment')
INSERT INTO TEXTREFERENCE VALUES(10466,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com117">\u000a      <b>com117:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Ansgar \u25aa Ansgar \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"><a  class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a> blev betragtet som den, der for alvor begyndte missionsgerningen i Danmark.\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>\u000a         \u000a         <div class="p">Ansgar voksede op i benediktinerklosteret Corbie i Flandern og var som ung med til\u000a            at grundl\u00e6gge datterklosteret Corvey i Sachsen. P\u00e5 opfordring af den frankiske kejser\u000a            <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe298" rel="ajax/getReference/pe298">Ludvig 1. den Fromme</a> fulgte han i 826 med den danske kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> til Danmark. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> og hans familie var lige blevet d\u00f8bt ved kejserens hof, og Ansgars opgave var at\u000a            st\u00f8tte dem i deres trosforhold og at missionere i Danmark. Ansgar og hans f\u00f8lgesvend,\u000a            munken <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe300" rel="ajax/getReference/pe300">Autbert</a>, havde dog kun ringe succes, bl.a. fordi <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe40" rel="ajax/getReference/pe40">Harald Klak</a> blev fordrevet allerede \u00e5ret efter. I 830 tog Ansgar til handelsbyen Birka i Sverige,\u000a            hvor hans kristne mission havde bedre fremgang. 831 udn\u00e6vntes han til \u00e6rkebiskop i\u000a            det nyoprettede bisped\u00f8mme i Hamburg. Pga. et vikingeangreb 845 blev bispes\u00e6det flyttet\u000a            til Bremen, som bisped\u00f8mmet blev sammenlagt med efter en del politiske forviklinger.\u000a            I Danmark l\u00e5 missionen stille, men efter diplomatiske forhandlinger med kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe301" rel="ajax/getReference/pe301">Erik (Horik) 1.</a> fik Ansgar ca. 850 bygget en kirke p\u00e5 handelspladsen Hedeby ved Slesvig. Ca. 855\u000a            fik han af kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe302" rel="ajax/getReference/pe302">Horik 2. Barn</a> en grund i Ribe til et kirkebyggeri. Ansgars liv er skildret af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe303" rel="ajax/getReference/pe303">Rimbert</a>, hans medarbejder og efterf\u00f8lger som \u00e6rkebiskop, i helgenbiografien <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a               <span xmlns="" class="italic">Vita Anskarii</span></span> (Ansgars levned).\u000a         </div>\u000a         \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com117','comment')
INSERT INTO TEXTREFERENCE VALUES(10467,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com119">\u000a      <b>com119:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Den Israelit foruden Svig,\u000a      Hvis F\u00e6dreland var Himmerig\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"> jf. <a  class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesu</a> omtale af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe304" rel="ajax/getReference/pe304">Nathanael</a>, da han blev kaldet til discipel: \u201cSe, d\u00e9r er sandelig en israelit, som er uden svig\u201d\u000a         (\u000a         <span class="italic">Bibelen</span> 1992, Joh 1,47; \u000a         <span class="italic">Biblia </span>1787, Joh 1,48). I det f\u00f8lgende lover <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe45" rel="ajax/getReference/pe45">Jesus</a>, at <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe304" rel="ajax/getReference/pe304">Nathanael</a> skal se himlen \u00e5ben og engle stige op og ned (\u000a         <span class="italic">Bibelen</span> 1992, Joh 1,51; \u000a         <span class="italic">Biblia</span> 1787, Joh 1,52). Jesus hentyder her til patriarken <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe305" rel="ajax/getReference/pe305">Jakob</a>s dr\u00f8m om himmelstigen (1 Mos 28,11 f).\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com119','comment')
INSERT INTO TEXTREFERENCE VALUES(10468,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com120">\u000a      <b>com120:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Hvis F\u00e6dreland var Himmerig \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Hebr 11,14-16.</div>\u000a      \u000a   </div>','scrollTarget_com120','comment')
INSERT INTO TEXTREFERENCE VALUES(10469,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com121">\u000a      <b>com121:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Martyr-Old \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">dvs. tidsalder, (her) oldkirkens periode indtil ca. 500, hvor der var martyrer, som\u000a         blev dr\u00e6bt for kristendommens sag.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com121','comment')
INSERT INTO TEXTREFERENCE VALUES(10470,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com122">\u000a      <b>com122:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Morten Luther \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"><a  class="persName" href="ajax/getReference/pe42" rel="ajax/getReference/pe42">Morten Luther</a>s \u00e6ndrede kristendomsforst\u00e5else og brud med den katolske kirke fik markant betydning\u000a         ogs\u00e5 i den danske kirke gennem reformationen i Danmark 1536.\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>\u000a         \u000a         <div class="p">Martin Luther (1483-1546), tysk teolog og reformator. Hans synspunkter p\u00e5 kirkens\u000a            myndighed, sakramenterne og forholdet mellem Gud og menneske f\u00f8rte til et brud med\u000a            den katolske kirke og dannelsen af den evangelisk-lutherske kirkeretning, hvori han\u000a            var en central skikkelse. Luthers overs\u00e6ttelse af Bibelen og hans salmedigtning p\u00e5\u000a            tysk var indflydelsesrige inspirationskilder i brugen af modersm\u00e5let i gudstjenesten.\u000a         </div>\u000a         \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com122','comment')
INSERT INTO TEXTREFERENCE VALUES(10471,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com123">\u000a      <b>com123:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>apostolisk, som <a  class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">\u000a         <span xmlns="" class="italic">Ansgar,</span></a> \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"><a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a> blev kaldt \u201cNordens Apostel\u201d. <a xmlns="http://www.w3.org/1999/xhtml" class="comment_ref">Se indledningen</a>.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com123','comment')
INSERT INTO TEXTREFERENCE VALUES(10472,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com124">\u000a      <b>com124:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Han str\u00f8ede S\u00e6den ud med Graad,\u000a      Og tr\u00f8sted sig, paa Herrens Ord,\u000a      Naar Stormen over Heden foer,\u000a      Med Haabet, dog sin Neg, engang,\u000a      At b\u00e6re hjem med Fryde-Sang!\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Jf. Sl 126,5-6: \u201cDe, som saae med graad, skal h\u00f8ste med fryde-sang. / Den, som b\u00e6r s\u00e6den, [...],\u000a         gaaer flux frem, og gr\u00e6der; [men] han skal visseligen komme med fryde-sang, [naar]\u000a         han b\u00e6r sine neger\u201d (\u000a         <span class="italic">Biblia </span>1787). Billedet af h\u00f8sten er hos <a  class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> brugt i eskatologisk betydning om verdensdommen.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com124','comment')
INSERT INTO TEXTREFERENCE VALUES(10473,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com118">\u000a      <b>com118:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>det Haab,\u000a      Du gav din Tjener ved vor Daab\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">hentydning til de tr\u00f8stende ord biskop <a  class="persName" href="ajax/getReference/pe390" rel="ajax/getReference/pe390">Ebo af Reims</a> i en samtale gav <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe17" rel="ajax/getReference/pe17">Ansgar</a>, da fremtiden for missionsgerningen i Danmark s\u00e5 sortest ud. <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> antager i artiklen \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Ansgars Efterm\u00e6le</span>\u201d, at ordene faldt kort f\u00f8r 850, hvor det lykkedes Ansgar at f\u00e5 kong <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe301" rel="ajax/getReference/pe301">Erik (Horik) 1.</a>s tilladelse til at bygge en kirke i Hedeby (\u000a         <span class="italic"><span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Danne-Virke</span></span> 3 1817, s. 167).\u000a      </div>\u000a      \u000a      <div class="readMore">\u000a         <i>readMore:</i>\u000a         \u000a         <div class="p">I <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a>s overs\u00e6ttelse fra 1817 af <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe303" rel="ajax/getReference/pe303">Rimbert</a>s latinske tekst siger <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe390" rel="ajax/getReference/pe390">Ebo</a>: "derpaa er jeg sikker at det Arbeide, vi i Jesu Navn har begyndt, skal b\u00e6re Frugt\u000a            i Herren, og det er mit Haab, min faste Troe, ja, det veed jeg med Vished, at om ogsaa\u000a            V\u00e6rket, vi hos de Folkef\u00e6rd har begyndt, stundum kan standse for Syndernes Skyld,\u000a            saa skal det dog aldrig aldeles forgaae, men b\u00e6re Frugt i Guds Naade, og lykkes saa,\u000a            at \u000a            <span class="italic">Herrens Navn skal naae til Verdens Ende [...]!</span>" (\u000a            <span class="italic"><span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Danne-Virke</span></span> 3 1817, s. 166 f.). Citatet stammer fra kapitel 29 i <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe303" rel="ajax/getReference/pe303">Rimbert</a>s helgenbiografi \u000a            <span class="italic">Vita Anskarii</span> (Ansgars levned) (<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title"><a class="persName" href="ajax/getReference/pe391" rel="ajax/getReference/pe391">Waitz</a></span> 1884, s. 65).\u000a         </div>\u000a         \u000a      </div>           \u000a      \u000a   </div>','scrollTarget_com118','comment')
INSERT INTO TEXTREFERENCE VALUES(10474,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com125">\u000a      <b>com125:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Kirken \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Matt 16,18, se strofe 5, \u201cKlippe-Grund\u201d.\u000a         \u000a      </div>\u000a      \u000a   </div>','scrollTarget_com125','comment')
INSERT INTO TEXTREFERENCE VALUES(10475,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com126">\u000a      <b>com126:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>F\u00f8r Jord og Himmel f\u00f8lge med \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p"> jf. Luk 21,33 og \u00c5b 21,1.</div>\u000a      \u000a   </div>','scrollTarget_com126','comment')
INSERT INTO TEXTREFERENCE VALUES(10476,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com129">\u000a      <b>com129:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>Marken \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">(her) Danmark.</div>\u000a      \u000a   </div>','scrollTarget_com129','comment')
INSERT INTO TEXTREFERENCE VALUES(10477,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com130">\u000a      <b>com130:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>lukt med B\u00f8lgen blaa,\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">Citat fra 1. strofe af <a  class="persName" href="ajax/getReference/pe221" rel="ajax/getReference/pe221">Laurids Kok</a>s (1634-91) historiske vise \u201c<span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">Om <a class="persName" href="ajax/getReference/pe249" rel="ajax/getReference/pe249">Tyre Danebod</a></span>\u201d: \u201cDanmark, deiligst vang og v\u00e6nge, / Lukt med b\u00f8lgen blaa\u201d (<a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe239" rel="ajax/getReference/pe239">Syv</a> [1695], 4. part nr. 38, s. 584-586). <a xmlns="http://www.w3.org/1999/xhtml" class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> bearbejdede visen i tidsskriftet <span xmlns="http://www.w3.org/1999/xhtml" class="rs_title">\u000a            <span xmlns="" class="italic">Danne-Virke</span></span> 2 1817, s. 1-6.\u000a      </div>\u000a   </div>','scrollTarget_com130','comment')
INSERT INTO TEXTREFERENCE VALUES(10478,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com131">\u000a      <b>com131:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>lukt \u25aa \u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">perfektum participium af ''lukke'', (her) lukket med, omsluttet af.</div>\u000a      \u000a   </div>','scrollTarget_com131','comment')
INSERT INTO TEXTREFERENCE VALUES(10479,-1,'1826_433A_com.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="note" id="scrollTarget_com132">\u000a      <b>com132:\u000a         \u000a      </b>\u000a      <br/>\u000a      <b>lemma: </b>\u000a      <br/>bind i Negen os med dem,\u000a      Han gladelig skal f\u00f8re frem\u000a      <br/>\u000a      <b>kommentar: </b>\u000a      \u000a      <div class="p">jf. Matt 13,30. <a  class="persName" href="ajax/getReference/pe4" rel="ajax/getReference/pe4">Grundtvig</a> bruger h\u00f8stbilledet eskatologisk om de sidste tider, se kommentaren til strofe 8.\u000a      </div>\u000a      \u000a   </div>','scrollTarget_com132','comment')
