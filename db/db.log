INSERT INTO TEXTREFERENCE VALUES(10480,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth146">\u000a      \u000a      <div class="normForm">Afrodite</div>\u000a      \u000a      <div class="popUp">Afrodite er k\u00e6rlighedens gudinde i den gr\u00e6ske mytologi. Hun er mor til Eros.</div>\u000a      \u000a      <div class="encyc">I den romerske mytologi hedder hun Venus og er mor til Amor</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Venushat</span>\u000a   </div>','myth146','myth')
INSERT INTO TEXTREFERENCE VALUES(10481,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth74.a">\u000a      \u000a      <div class="epithet">Age-Thor\u000a         <span class="sameAs">\u000a            <a href="#myth74"> &gt; Thor</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Age-Thor er et tilnavn til Thor, n\u00e5r han ager med sin gedebukkevogn hen over himlen\u000a         og derved skaber tordenen.\u000a      </div>\u000a      \u000a      <span class="orthography">Aka-Thor\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Auka-Thor</span>\u000a      \u000a   </div>','myth74.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10482,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth223">\u000a      \u000a      <div class="normForm">Aiolos</div>\u000a      \u000a      <div class="popUp">Aiolos er vindenes gud i den gr\u00e6ske mytologi.</div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes han \u00c6olus.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>\u00c6olushat</span>\u000a   </div>','myth223','myth')
INSERT INTO TEXTREFERENCE VALUES(10483,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth1">\u000a      \u000a      <div class="normForm">Alfader</div>\u000a      \u000a      <div class="popUp">Alfader er et af Odins mange tilnavne i den nordiske mytologi. Navnet markerer, at\u000a         Odin opfattes som fader til guder (og mennesker).\u000a      </div>\u000a      \u000a      <span class="orthography">Alfaudr</span>\u000a      \u000a      <div class="encyc">For Grundtvig er der dog tale om en betydningsglidning over mod\u000a         den alm\u00e6gtige kristne Gud (se fx \u201cOm Asal\u00e6ren\u201d s. 161,\u000a         hvor Grundtvig skriver \u201cdet Evige\u201d i parentes lige efter Alfader).\u000a         Alfader er s\u00e5ledes et princip, der for Grundtvig st\u00e5r over aserne og\u000a         j\u00e6tterne, og som styrer verdens gang p\u00e5 en s\u00e5dan m\u00e5de,\u000a         at b\u00e5de aser og j\u00e6tter g\u00e5r til grunde ved Ragnarok. Hertil\u000a         kan Grundtvig have ladet sig inspirere af Peter Friedrich Suhms \u000a         <span class="italic">Om Odin og den hedniske Gudel\u00e6re</span> fra 1771 (fx s. 8).\u000a      </div>\u000a      \u000a      <div class="encyc">I Eddaen st\u00e5r der under \u201c\u00d3\u00f0ins heiti ok \u00d3\u00f0inskenninger\u201d (da. Odinsben\u00e6vnelser og Odinskenninger):\u000a         \u201cOdinn heiti Alf\u00f6\u00f0r\u201d (da. Odin hed Alfader).\u000a      </div>\u000a      \u000a      <div class="encyc">I Grundtvigs tidlige afhandling \u201cOm Religion og Liturgie\u201d er Alfader \u201cdet identisk\u000a         Evige\u201d (1807, s. 147).\u000a      </div>\u000a      \u000a      <div class="encyc">M\u00e5ske er Grundtvigs opfattelse dog tydeligst udtrykt i \u000a         <span class="italic">Maskeradeballet i Dannemark 1808,</span> hvor kristendom og asatro optr\u00e6der side om side; og et digt lyder: \u201cH\u00f8je Odin! Hvide\u000a         Krist! / Slettet ud er Eders Tvist, / Begge s\u00f8nner af Alfader (s. 19 ).\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Odinhat</span>\u000a   </div>','myth1','myth')
INSERT INTO TEXTREFERENCE VALUES(10484,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.a">\u000a      \u000a      <div class="pseudoEpithet">Alfader\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth59.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10485,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth225">\u000a      \u000a      <div class="normForm">Alsvinn</div>\u000a      \u000a      <div class="popUp">Alsvinn er den anden af solens to heste i den nordiske mytologi. Navnet betyder \u2018meget\u000a         hurtig\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Alsvi\u00f0r</span>\u000a      \u000a      <div class="encyc"> </div>\u000a      \u000a   </div>','myth225','myth')
INSERT INTO TEXTREFERENCE VALUES(10486,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth263">\u000a      \u000a      <div class="normForm">Alvheim</div>\u000a      \u000a      <div class="popUp">Alvheim var oprindelig alfernes hjem i den nordiske mytologi. P\u00e5 et tidspunkt for\u00e6rer\u000a         alferne det til Frej, hvilket tyder p\u00e5 en n\u00e6r forbindelse mellem vaner og alfer.\u000a      </div>\u000a      \u000a      <span class="orthography">Alfheim</span>\u000a      \u000a   </div>','myth263','myth')
INSERT INTO TEXTREFERENCE VALUES(10487,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth2">\u000a      \u000a      <div class="normForm">Alvider</div>\u000a      \u000a      <div class="popUp">Alvider er den runekyndige blandt j\u00e6tterne.</div>\u000a      \u000a      <span class="orthography">Alsvider,</span>\u000a      \u000a      <span class="orthography">Alsvidr,</span>\u000a      \u000a      <span class="orthography">Alsvier\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Alsvi\u00fer</span>         \u000a      \u000a      <div class="encyc">I \u201cRunekapitlet\u201d, som er en del af eddadigtet \u000a         <span class="italic">H\u00e1vam\u00e1l</span> (da. \u000a         <span class="italic">Den h\u00f8jes Tale</span>) er Alvider den runekyndige blandt j\u00e6tterne.\u000a      </div>\u000a      \u000a   </div>','myth2','myth')
INSERT INTO TEXTREFERENCE VALUES(10488,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.b">\u000a      \u000a      <div class="epithet">Alvider\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth59.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10489,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth3">\u000a      \u000a      <div class="normForm">Alvis</div>        \u000a      \u000a      <div class="popUp">Alvis er en klog dv\u00e6rg fra den nordiske mytologi. Navnet vetyder den al-vise.</div>\u000a      \u000a      <span class="orthography">Alv\u00edss</span>\u000a      \u000a      <div class="encyc">Alvis optr\u00e6der i eddadigtet \u000a         <span class="italic">Alv\u00edsm\u00e1l</span> (da. \u000a         <span class="italic">Sangen om Alvis</span>).\u000a      </div>\u000a      \u000a   </div>','myth3','myth')
INSERT INTO TEXTREFERENCE VALUES(10490,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth147.a">\u000a      \u000a      <div class="latin">Amor\u000a         <span class="sameAs">\u000a            <a href="#myth147"> &gt; Eros</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth147.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10491,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth350">\u000a      \u000a      <div class="normForm">Andhrimner</div>\u000a      \u000a      <div class="popUp">Andhrimner er gudernes kok i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth350','myth')
INSERT INTO TEXTREFERENCE VALUES(10492,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth291">\u000a      \u000a      <div class="normForm">Andlang</div>\u000a      \u000a      <div class="popUp">Andlang er navnet p\u00e5 den anden himmel i den nordiske mytologi. Den befinder sig mod\u000a         syden, oven over den himmel, man ellers kender.\u000a      </div>\u000a      \u000a      <span class="orthography">Aundlangr</span>\u000a      \u000a   </div>','myth291','myth')
INSERT INTO TEXTREFERENCE VALUES(10493,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth189">\u000a      \u000a      <div class="normForm">Angantyr</div>\u000a      \u000a      <div class="popUp">Angantyr er en nordisk sagnhelt og bers\u00e6rk, konge over goterne, kendt fra bl.a. Saxo.\u000a         Han ejer sv\u00e6rdet Tyrfing.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Ongendushat</span>\u000a   </div>','myth189','myth')
INSERT INTO TEXTREFERENCE VALUES(10494,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth394">\u000a      \u000a      <div class="normForm">Angeiia</div>\u000a      \u000a      <div class="popUp">Angeiia er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>    \u000a      \u000a   </div>','myth394','myth')
INSERT INTO TEXTREFERENCE VALUES(10495,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth4">\u000a      \u000a      <div class="normForm">Angrbode</div>\u000a      \u000a      <div class="popUp">Angrbode er en j\u00e6ttekvinde fra den nordiske mytologi. Sammen med Loke har hun b\u00f8rnene\u000a         Fenrisulven, Hel og Midg\u00e5rdsormen.\u000a      </div>\u000a      \u000a      <span class="orthography">Angrboda\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Angurbode</span>\u000a      \u000a   </div>','myth4','myth')
INSERT INTO TEXTREFERENCE VALUES(10496,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth301">\u000a      \u000a      <div class="normForm">Arvaker</div>\u000a      \u000a      <div class="popUp">Arvaker er den ene af solens to heste i den nordiske mytologi. Navnet betyder \u2018den\u000a         \u00e5rv\u00e5gne\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Arvakr</span>\u000a      \u000a   </div>','myth301','myth')
INSERT INTO TEXTREFERENCE VALUES(10497,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth74.b">\u000a      \u000a      <div class="epithet">Asa-Thor\u000a         <span class="sameAs">\u000a            <a href="#myth74"> &gt; Thor</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth74.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10498,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.a">\u000a      \u000a      <div class="epithet">Asaloke\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth48.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10499,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth166.a">\u000a      \u000a      <div class="epithet">Asbro\u000a         <span class="sameAs">\u000a            <a href="#myth166"> &gt; Bifrost</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth166.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10500,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth87">\u000a      \u000a      <div class="normForm">Asg\u00e5rd</div>\u000a      \u000a      <div class="popUp">Asg\u00e5rd er gudernes (asernes) hjem eller bolig i den nordiske mytologi. Det ligger\u000a         enten i himlen eller i verdens centrum. Fra menneskenes verden, Midg\u00e5rd, kan man komme\u000a         til Asg\u00e5rd via regnbuen, Bifrost.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth87','myth')
INSERT INTO TEXTREFERENCE VALUES(10501,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth5">\u000a      \u000a      <div class="normForm">Ask</div>\u000a      \u000a      <div class="popUp">Ask er den f\u00f8rste mand (af menneskesl\u00e6gt) i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Askur</span>\u000a      \u000a   </div>','myth5','myth')
INSERT INTO TEXTREFERENCE VALUES(10502,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth258">\u000a      \u000a      <div class="normForm">Askunge</div>\u000a      \u000a      <div class="popUp">Askunge er en kenning for mennesker, b\u00f8rn af Ask og Embla, det f\u00f8rste menneskepar???.\u000a         Efter Grundtvigs tekst en slags norner eller valkyrier???.\u000a      </div>\u000a      \u000a      <div class="encyc"> Efter Simek m\u00e5ske en sagnkongesl\u00e6gt (u./Askr)??. Schimmelmann n\u00e6vner Askunger, Alfunger\u000a         og dv\u00e6rged\u00f8tre som tre ursl\u00e6gter (s. 158). Samme som alfunge???\u000a      </div>\u000a      \u000a   </div>','myth258','myth')
INSERT INTO TEXTREFERENCE VALUES(10503,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.t">\u000a      \u000a      <div class="epithet">Asodin\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Navnet Asodin understreger, at Odin er en af aserne.</div>\u000a      \u000a   </div>','myth59.t','myth')
INSERT INTO TEXTREFERENCE VALUES(10504,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth398">\u000a      \u000a      <div class="normForm">Atla</div>\u000a      \u000a      <div class="popUp">Atla er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>    \u000a      \u000a   </div>','myth398','myth')
INSERT INTO TEXTREFERENCE VALUES(10505,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth84.a">\u000a      \u000a      <div class="epithet">Augelmer\u000a         <span class="sameAs">\u000a            <a href="#myth84"> &gt; Ymer</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Under navnet Augelmer er Ymer stamfader til rimturserne.</div>\u000a      \u000a      <span class="orthography">Aurgelmer,</span>\u000a      \u000a      <span class="orthography">Aurgelmr,</span>\u000a      \u000a      <span class="orthography">Aurgelmir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Urgelmer</span>\u000a      \u000a   </div>','myth84.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10506,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth150">\u000a      \u000a      <div class="normForm">Aurboda</div>\u000a      \u000a      <div class="popUp">Aurboda er en j\u00e6ttekvinde. Hun er moder til Gerd (eller Gerda), som Frej forelsker\u000a         sig i.\u000a      </div>\u000a      \u000a      <div class="encyc">Frej forelsker sig i Gerd i eddadigtet \u000a         <span class="italic">F\u01ebr Sk\u00edrnis</span> (da. \u000a         <span class="italic">Skirners F\u00e6rd</span>).\u000a      </div>\u000a      \u000a   </div>','myth150','myth')
INSERT INTO TEXTREFERENCE VALUES(10507,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth396">\u000a      \u000a      <div class="normForm">Aurgjafa</div>\u000a      \u000a      <div class="popUp">Aurgjafa er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til\u000a         guden Heimdal.\u000a      </div>    \u000a      \u000a   </div>','myth396','myth')
INSERT INTO TEXTREFERENCE VALUES(10508,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth183.a">\u000a      \u000a      <div class="latin">Bacchus\u000a         <span class="sameAs">\u000a            <a href="#myth183"> &gt; Dionysos</a>\u000a         </span>\u000a      </div>     \u000a      \u000a   </div>','myth183.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10509,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth6">\u000a      \u000a      <div class="normForm">Balder</div>\u000a      \u000a      <div class="popUp">Balder har ogs\u00e5 tilnavnet \u2018den gode\u2019. Han er en lysende, venlig, us\u00e5rlig, klog, mild\u000a         og retf\u00e6rdig gud i den nordiske mytologi, s\u00f8n af Odin og Frigg (if\u00f8lge Snorre).\u000a      </div>\u000a      \u000a      <span class="orthography">Baldr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Baldur</span>\u000a      \u000a      <div class="encyc">Trods us\u00e5rligheden bliver Balder dr\u00e6bt af en pil, lavet af mistelten, det eneste i\u000a         naturen, der ikke havde svoret ikke at skade Balder. Ulykkeligvis bliver pilen oven\u000a         i k\u00f8bet \u2013 ved hj\u00e6lp fra Loke \u2013 afskudt af Balders broder, den blinde H\u00f8der.\u000a      </div>\u000a      \u000a      <div class="encyc">Hos Saxo er Balder han kun en halvgud.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Graadgudenhat</span>\u000a      <span>Hels Herfang (bytte).</span>\u000a   </div>','myth6','myth')
INSERT INTO TEXTREFERENCE VALUES(10510,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.g">\u000a      \u000a      <div class="epithet">Balders baner\u00e5d\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Loke tilskyndede H\u00f8der til at skyde en pil af mistelten efter Balder og derfor dr\u00e6be\u000a         ham.\u000a      </div>\u000a      \u000a   </div>','myth48.g','myth')
INSERT INTO TEXTREFERENCE VALUES(10511,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth155">\u000a      \u000a      <div class="normForm">Barre</div>\u000a      \u000a      <div class="popUp">Barre er den lund i den nordiske mytologi, i hvilken Gerd har lovet Frej et elskovsm\u00f8de</div> \u000a      \u000a      <span class="orthography">Barri</span>\u000a      \u000a      <div class="encyc">Barre optr\u00e6der i eddadigtet \u000a         <span class="italic">F\u01ebr Sk\u00edrnis</span> (da. \u000a         <span class="italic">Skirners F\u00e6rd</span>).\u000a      </div>\u000a      \u000a   </div>','myth155','myth')
INSERT INTO TEXTREFERENCE VALUES(10512,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth18.a">\u000a      \u000a      <div class="epithet">Belas lyse bane (dvs. drabsmand)\u000a         <span class="sameAs">\u000a            <a href="#myth18"> &gt; Frej</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth18.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10513,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth262">\u000a      \u000a      <div class="normForm">Bele</div>\u000a      \u000a      <div class="popUp">Bele er en j\u00e6tte fra den nordiske mytologi, der bliver dr\u00e6bt med Frejs sv\u00e6rd.</div>\u000a      \u000a      <span class="orthography">Bela</span>\u000a      \u000a      <div class="encyc">Drabet p\u00e5 Bele har givet anledning til kenningen (den poetiske omskrivning) \u2018Beles\u000a         lyse bane\u2019, der betyder Frej.\u000a      </div>\u000a      \u000a   </div>','myth262','myth')
INSERT INTO TEXTREFERENCE VALUES(10514,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth7">\u000a      \u000a      <div class="normForm">Bergelmer</div>\u000a      \u000a      <div class="popUp">Bergelmer er den eneste j\u00e6tte, der sammen med sin hustru overlevede verdensoversv\u00f8mmelsen\u000a         af blod, da Ymer blev dr\u00e6bt.\u000a      </div>\u000a      \u000a      <div class="encyc">Bergelmer er i den nordiske mytologi sammen med sin hustru ophav til alle efterf\u00f8lgende\u000a         j\u00e6tter (eller i det mindste til rimturserne).\u000a      </div>\u000a      \u000a   </div>','myth7','myth')
INSERT INTO TEXTREFERENCE VALUES(10515,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth229">\u000a      \u000a      <div class="normForm">Bestla</div>\u000a      \u000a      <div class="popUp">Bestla er mor til Odin i den nordiske mytologi. Hun er af j\u00e6ttesl\u00e6gt og gift med Borr,\u000a         med hvem hun ogs\u00e5 har s\u00f8nnerne Vile og Ve.\u000a      </div>\u000a      \u000a   </div>','myth229','myth')
INSERT INTO TEXTREFERENCE VALUES(10516,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth207">\u000a      \u000a      <div class="normForm">Beyla</div>\u000a      \u000a      <div class="popUp">Beyla er en tjenestepige eller tr\u00e6lkvinde til Frej fra den nordiske mytologi. Hun\u000a         er gift med Byggvir.\u000a      </div>\u000a      \u000a      <span class="orthography">Beila\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">B\u00f8la</span>\u000a      \u000a   </div>','myth207','myth')
INSERT INTO TEXTREFERENCE VALUES(10517,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth166">\u000a      \u000a      <div class="normForm">Bifrost</div>\u000a      \u000a      <div class="popUp">Bifrost er et andet navn for regnbuen. I den nordiske mytologi f\u00f8rer den fra menneskenes\u000a         land, Midg\u00e5rd, op til gudernes rige, Asg\u00e5rd. Broen vogtes af Heimdal.\u000a      </div>\u000a      \u000a      <span class="orthography">Bivr\u00f8st</span>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Asbrohat</span>\u000a   </div>','myth166','myth')
INSERT INTO TEXTREFERENCE VALUES(10518,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth216">\u000a      \u000a      <div class="normForm">Bil</div>\u000a      \u000a      <div class="popUp">Bil er en asynje, dvs. gudinde, fra den nordiske mytologi. Hun anses for at v\u00e6re datter\u000a         af M\u00e5ne, men er dog indimellem blot et billede p\u00e5 m\u00e5nen. I kenninger (poetiske omskrivninger)\u000a         vil hendes navn ofte blot betyde \u2018kvinde\u2019 i al almindelighed.\u000a      </div>\u000a      \u000a   </div>','myth216','myth')
INSERT INTO TEXTREFERENCE VALUES(10519,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth406">\u000a      \u000a      <div class="normForm">Bjovulf</div>\u000a      \u000a      <div class="popUp">Bjovulf er helten i et oldengelsk kvad af samme navn. Bjovulf var en gotlandsk kriger,\u000a         der kom kong Roar i Lejre til hj\u00e6lp i kampen mod trolduhyret Grendel.\u000a      </div>    \u000a      \u000a      <span class="orthography">Beovulf,</span>\u000a      \u000a      <span class="orthography">Beowulf\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Bjowulf</span>\u000a      \u000a      <div class="encyc">Grundtvig oversatte i 1820 kvadet om Bjovulf.</div>\u000a      \u000a   </div>','myth406','myth')
INSERT INTO TEXTREFERENCE VALUES(10520,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth390">\u000a      \u000a      <div class="normForm">Borr</div>\u000a      \u000a      <div class="popUp">Borr er et urv\u00e6sen fra den nordiske mytologi. Sammen med j\u00e6ttekvinden Bestla fik han\u000a         b\u00f8rnene Odin, Vile og Ve.\u000a      </div> \u000a      \u000a      <span class="orthography">Burr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">B\u00f8r</span>\u000a      \u000a   </div>','myth390','myth')
INSERT INTO TEXTREFERENCE VALUES(10521,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.c">\u000a      \u000a      <div class="epithet">Borrs s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth59.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10522,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth8">\u000a      \u000a      <div class="normForm">Brage</div>\u000a      \u000a      <div class="popUp">Brage er digtningens gud i nordisk mytologi. Han er gift med Idun.</div>\u000a      \u000a      <span class="orthography">Braga</span>\u000a      \u000a      \u000a   </div>','myth8','myth')
INSERT INTO TEXTREFERENCE VALUES(10523,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth256">\u000a      \u000a      <div class="normForm">Breidablik</div>\u000a      \u000a      <div class="popUp">Breidablik er Balders bolig i den nordiske mytologi. Det ligger i himlen og er s\u00e6rlig\u000a         rent og helligt.\u000a      </div>\u000a      \u000a      <span class="orthography">Bredeblik\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Breideblik</span>\u000a      \u000a   </div>','myth256','myth')
INSERT INTO TEXTREFERENCE VALUES(10524,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth84.b">\u000a      \u000a      <div class="epithet">Brim\u000a         <span class="sameAs">\u000a            <a href="#myth84"> &gt; Ymer</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth84.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10525,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth314">\u000a      \u000a      <div class="normForm">Brimir</div>\u000a      \u000a      <div class="popUp">Brimir er navnet p\u00e5 en sal, der i den nordiske mytologi findes i den bedre verden\u000a         i tiden efter Ragnarok.\u000a      </div>\u000a      \u000a   </div>','myth314','myth')
INSERT INTO TEXTREFERENCE VALUES(10526,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth238">\u000a      \u000a      <div class="normForm">Brisingamen</div>\u000a      \u000a      <div class="popUp">Brisingamen er navnet p\u00e5 Frejas halssmykke af guld (nordisk mytologi). Hun har f\u00e5et\u000a         det ved at ligge med de fire dv\u00e6rge, der har smedet det.\u000a      </div>\u000a      \u000a      <span class="orthography">Brysinga Men,</span>\u000a      \u000a      <span class="orthography">Brysings halsb\u00e5nd\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Brysings smykke</span>\u000a      \u000a   </div>','myth238','myth')
INSERT INTO TEXTREFERENCE VALUES(10527,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth343">\u000a      \u000a      <div class="normForm">Brok</div>\u000a      \u000a      <div class="popUp">Brok er en dv\u00e6rg (svartalf) i den nordiske mytologi. Sammen med sin bror, Sindre,\u000a         har han smedet ringen Draupner, galten Gyldenb\u00f8rste og hammeren Mj\u00f8lner.\u000a      </div>   \u000a      \u000a      <span class="orthography">Brokur</span>\u000a      \u000a   </div>','myth343','myth')
INSERT INTO TEXTREFERENCE VALUES(10528,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth9">\u000a      \u000a      <div class="normForm">Brynhilde</div>\u000a      \u000a      <div class="popUp">Brynhilde er en valkyrie fra den nordiske sagnhistorie.</div>\u000a      \u000a      <span class="orthography">Brynhild\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Brynhildur</span>\u000a      \u000a      <div class="encyc">Brynhilde var gift med Gunnar Gjukess\u00f8n, men bevarede sin k\u00e6rlighed til Sigurd Fafnersbane,\u000a         som var hendes ungdomselskede.\u000a      </div>\u000a      \u000a   </div>','myth9','myth')
INSERT INTO TEXTREFERENCE VALUES(10529,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth10">\u000a      \u000a      <div class="normForm">Bure</div>\u000a      \u000a      <div class="popUp">Bure er en j\u00e6tte fra den nordiske mytologi, der medvirkede ved verdens skabelse. Hans\u000a         s\u00f8n Borr (af Grundtvig ogs\u00e5 kaldet Bure eller B\u00f8r) fik med j\u00e6ttekvinden Bestla s\u00f8nnerne\u000a         Odin, Vile og Ve.\u000a      </div>\u000a      \u000a      <span class="orthography">Bur\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Burr</span>\u000a      \u000a      \u000a      \u000a   </div>','myth10','myth')
INSERT INTO TEXTREFERENCE VALUES(10530,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth206">\u000a      \u000a      <div class="normForm">Byggvir</div>\u000a      \u000a      <div class="popUp">Byggvir er en af Frejs tjenere eller tr\u00e6lle fra den nordiske mytologi. Han er gift\u000a         med Beyla.\u000a      </div>\u000a      \u000a      <span class="orthography">Beigver\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Byggve</span>\u000a      \u000a      <div class="encyc">Bygvir er meget lille; og i eddadigtet \u000a         <span class="italic">Lokasenna</span> bliver han drillet af Loke.\u000a      </div>\u000a      \u000a   </div>','myth206','myth')
INSERT INTO TEXTREFERENCE VALUES(10531,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth230">\u000a      \u000a      <div class="normForm">B\u00f8ltorn</div>\u000a      \u000a      <div class="popUp">B\u00f8ltorn er en j\u00e6tte fra den nordiske mytologi. Han er far til Bestla og dermed morfar\u000a         til Odin.\u000a      </div>\u000a      \u000a      <span class="orthography">Bolthorn</span>\u000a      \u000a   </div>','myth230','myth')
INSERT INTO TEXTREFERENCE VALUES(10532,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.d">\u000a      \u000a      <div class="epithet">B\u00f8rs s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp"/>\u000a      \u000a   </div>','myth59.d','myth')
INSERT INTO TEXTREFERENCE VALUES(10533,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth157.a">\u000a      \u000a      <div class="latin">Cerberus\u000a         <span class="sameAs">\u000a            <a href="#myth157"> &gt; Kerberos</a>\u000a         </span>\u000a      </div>   \u000a      \u000a   </div>','myth157.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10534,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth151.a">\u000a      \u000a      <div class="latin">Comus\u000a         <span class="sameAs">\u000a            <a href="#myth151"> &gt; Komos</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Komus</span>\u000a      \u000a   </div>','myth151.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10535,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth187">\u000a      \u000a      <div class="normForm">Dain</div>\u000a      \u000a      <div class="popUp">Dain er en af dv\u00e6rgene i den nordiske mytologi. Navnet betyder egentlig \u2018den d\u00f8de\u2019.</div>\u000a      \u000a      <span class="orthography">D\u00e1inn,</span>\u000a      \u000a      <span class="orthography">D\u00e5en\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">D\u00e5in</span>\u000a      \u000a      <div class="encyc">Dain kendes fra \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom,</span> strofe 13.\u000a      </div>\u000a      \u000a   </div>','myth187','myth')
INSERT INTO TEXTREFERENCE VALUES(10536,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth138">\u000a      \u000a      <div class="normForm">Dan</div>\u000a      \u000a      <div class="popUp">Dan er en dansk sagnkonge, if\u00f8lge nogle middelalderlige sagnhistorier Danmarks f\u00f8rste\u000a         konge.\u000a      </div>\u000a      \u000a      \u000a      \u000a   </div>','myth138','myth')
INSERT INTO TEXTREFERENCE VALUES(10537,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth305">\u000a      \u000a      <div class="normForm">Delling</div>\u000a      \u000a      <div class="popUp">Delling er en lys og str\u00e5lende as fra den nordiske mytologi. Sammen med j\u00e6ttedatteren\u000a         Nat har han s\u00f8nnen Dag.\u000a      </div>\u000a      \u000a   </div>','myth305','myth')
INSERT INTO TEXTREFERENCE VALUES(10538,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth183">\u000a      \u000a      <div class="normForm">Dionysos</div>\u000a      \u000a      <div class="popUp">Dionysos er den gr\u00e6ske gud for vin og ekstase.</div>\u000a      \u000a      <div class="encyc">I romersk mytologi kaldes han Bacchus.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Bacchushat</span>\u000a   </div>','myth183','myth')
INSERT INTO TEXTREFERENCE VALUES(10539,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth140">\u000a      \u000a      <div class="normForm">Draupner</div>\u000a      \u000a      <div class="popUp">Navnet Draupner betyder \u2018drypperen\u2019. Det er en kostbar guldring, som tilh\u00f8rer Odin\u000a         og har den egenskab, at der hver niende nat drypper otte lige s\u00e5 kostbare ringe fra\u000a         den.\u000a      </div>\u000a      \u000a      <span class="orthography">Drupner</span>\u000a      \u000a      <div class="encyc">Da Balder blev br\u00e6ndt, kom Draupner med ham til Hel, men han sendte den med Hermod\u000a         tilbage til aserne.\u000a      </div>\u000a      \u000a   </div>','myth140','myth')
INSERT INTO TEXTREFERENCE VALUES(10540,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth260">\u000a      \u000a      <div class="normForm">Drome</div>\u000a      \u000a      <div class="popUp">Drome er navnet p\u00e5 den anden af de tre l\u00e6nker, som aserne fors\u00f8gte at binde Fenrisulven\u000a         med. F\u00f8rst med Gleipner lykkedes det.\u000a      </div>\u000a      \u000a      <div class="encyc">De to andre l\u00e6nker hedder L\u00f8ding og Gleipner.</div>\u000a      \u000a   </div>','myth260','myth')
INSERT INTO TEXTREFERENCE VALUES(10541,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth288">\u000a      \u000a      <div class="normForm">Duneyrr</div>\u000a      \u000a      <div class="popUp">Duneyrr er en af de hjorte i den nordiske mytologi, der gnaver af bladene p\u00e5 verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">Duneir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Duneyr</span>\u000a      \u000a   </div>','myth288','myth')
INSERT INTO TEXTREFERENCE VALUES(10542,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth173">\u000a      \u000a      <div class="normForm">Dvalen</div>\u000a      \u000a      <div class="popUp">Dvalen er en dv\u00e6rg fra den nordiske mytologi. Han opfattes i nogle kilder som fader\u000a         til visse af nornerne.\u000a      </div>\u000a      \u000a      <span class="orthography">Dvalin\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Dvalinn</span>\u000a      \u000a      <div class="encyc">Dvalen n\u00e6vnes i eddadigtet \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>).\u000a      </div>\u000a      \u000a      \u000a   </div>','myth173','myth')
INSERT INTO TEXTREFERENCE VALUES(10543,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth287">\u000a      \u000a      <div class="normForm">Dvalen</div>\u000a      \u000a      <div class="popUp">Dvalen er en af de hjorte i den nordiske mytologi, der spiser af Yggdrasils blade.</div>\u000a      \u000a      <span class="orthography">Dvalin\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Dvalinn</span>\u000a      \u000a   </div>','myth287','myth')
INSERT INTO TEXTREFERENCE VALUES(10544,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth112.a">\u000a      \u000a      <div class="epithet">Dvalins d\u00f8tre\u000a         <span class="sameAs">\u000a            <a href="#myth112"> &gt; norne</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth112.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10545,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth289">\u000a      \u000a      <div class="normForm">Dyratro</div>\u000a      \u000a      <div class="popUp">Dyratro er en af de hjorte i den nordiske mytologi, der gnaver af bladene p\u00e5 verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">Dunathror</span>\u000a      \u000a   </div>','myth289','myth')
INSERT INTO TEXTREFERENCE VALUES(10546,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth208">\u000a      \u000a      <div class="normForm">Eir</div>\u000a      \u000a      <div class="popUp">Eir er en asynje, dvs. gudinde, for l\u00e6gekunst i den nordiske mytologi. Navnet kan\u000a         betyde \u2018hj\u00e6lp\u2019 eller \u2018ro\u2019.\u000a      </div>\u000a      \u000a   </div>','myth208','myth')
INSERT INTO TEXTREFERENCE VALUES(10547,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth351">\u000a      \u000a      <div class="normForm">Eldhrimner</div>\u000a      \u000a      <div class="popUp">Eldhrimner er navnet p\u00e5 gudernes kogekar i den nordiske mytologi. Det betjenes af\u000a         kokken Andhrimner.\u000a      </div>\u000a      \u000a   </div>','myth351','myth')
INSERT INTO TEXTREFERENCE VALUES(10548,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth267">\u000a      \u000a      <div class="normForm">Eldir</div>\u000a      \u000a      <div class="popUp">Eldir er en tr\u00e6l fra den nordiske mytologi. Sammen med Finnafeingur varter han op\u000a         ved et \u00f8lgilde hos \u00c6gir.\u000a      </div>\u000a      \u000a   </div>','myth267','myth')
INSERT INTO TEXTREFERENCE VALUES(10549,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth393">\u000a      \u000a      <div class="normForm">Elgia</div>\u000a      \u000a      <div class="popUp">Elgia er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>    \u000a      \u000a   </div>','myth393','myth')
INSERT INTO TEXTREFERENCE VALUES(10550,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth88">\u000a      \u000a      <div class="normForm">Elivagar</div>\u000a      \u000a      <div class="popUp">Elivagar er floder, der eksisterede i Ginnungagap fra Verdens begyndelse i den nordiske\u000a         mytologi. Navnet kan overs\u00e6ttes til \u2018havstormsb\u00f8lger\u2019 eller \u2018isb\u00f8lger\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Elivaga\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00c9liv\u00e1gar</span>\u000a      \u000a      <div class="encyc">J\u00e6tten Ymer opstod, da rim fra Elivargar m\u00f8dte varmen fra syd.</div>\u000a      \u000a   </div>','myth88','myth')
INSERT INTO TEXTREFERENCE VALUES(10551,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth372">\u000a      \u000a      <div class="normForm">Elvidnir</div>\u000a      \u000a      <div class="popUp">Elvidnir er navnet p\u00e5 en hal hos d\u00f8dsgudinden Hel i den nordiske mytologi. Navnet\u000a         betyder \u2018regnfugtig\u2019.\u000a      </div>    \u000a      \u000a      <span class="orthography">\u00c9lj\u00fa\u00f0nir</span>\u000a      \u000a   </div>','myth372','myth')
INSERT INTO TEXTREFERENCE VALUES(10552,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth11">\u000a      \u000a      <div class="normForm">Embla</div>\u000a      \u000a      <div class="popUp">Embla er den f\u00f8rste kvinde (af menneskesl\u00e6gt) i den nordiske mytologi.</div>\u000a      \u000a      \u000a   </div>','myth11','myth')
INSERT INTO TEXTREFERENCE VALUES(10553,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth317">\u000a      \u000a      <div class="normForm">Erebos</div>\u000a      \u000a      <div class="popUp">Erebos er s\u00f8n af guden Chaos fra den gr\u00e6ske mytologi. </div>\u000a      \u000a      <span class="orthography">Erobos</span>\u000a      \u000a      <div class="encyc">Navnet Erebos kan dog ogs\u00e5 referere til det sted, som de d\u00f8de skal passere umiddelbart\u000a         efter d\u00f8den p\u00e5 deres vej til andre steder i d\u00f8dsriget. Navnet betyder m\u00e5ske \u2018dybt\u000a         m\u00f8rke\u2019 eller \u2018skygge\u2019.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Erobushat</span>\u000a   </div>','myth317','myth')
INSERT INTO TEXTREFERENCE VALUES(10554,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth410">\u000a      \u000a      <div class="normForm">Erik Ordkr\u00e6ng</div>\u000a      \u000a      <div class="popUp">Erik Ordkr\u00e6ng er en norsk sagnhelt.</div>\u000a      \u000a      <span class="orthography">Erik den M\u00e5lspage\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Erik den Veltalende</span>\u000a      \u000a      <div class="encyc">Man kan l\u00e6se om Erik Ordkr\u00e6ng hos Saxo, 5. bog.</div>\u000a      \u000a   </div>','myth410','myth')
INSERT INTO TEXTREFERENCE VALUES(10555,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth317.a">\u000a      \u000a      <div class="latin">Erobus\u000a         <span class="sameAs">\u000a            <a href="#myth317"> &gt; Erebos</a>\u000a         </span>\u000a      </div> \u000a      \u000a   </div>','myth317.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10556,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth147">\u000a      \u000a      <div class="normForm">Eros</div>\u000a      \u000a      <div class="popUp">Eros er k\u00e6rlighedens gud i den gr\u00e6ske mytologi. Han er s\u00f8n af k\u00e6rlighedsgudinden Afrodite\u000a         og krigsguden Ares.\u000a      </div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes han Amor (og er tilsvarende s\u00f8n af Venus og Mars).</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Amorhat</span>\u000a   </div>','myth147','myth')
INSERT INTO TEXTREFERENCE VALUES(10557,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth12">\u000a      \u000a      <div class="normForm">Fafner</div>\u000a      \u000a      <div class="popUp">Fafner var en drage (eller slange) fra den nordiske sagnhistorie. Den vogtede en guldskat\u000a         og dens blod var giftigt. Den blev dr\u00e6bt af Sigurd Fafnersbane, som dog ikke fik gl\u00e6de\u000a         af skatten.\u000a      </div>\u000a      \u000a      <span class="orthography">Fofner</span>\u000a      \u000a   </div>','myth12','myth')
INSERT INTO TEXTREFERENCE VALUES(10558,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth339">\u000a      \u000a      <div class="normForm">Falhovner</div>\u000a      \u000a      <div class="popUp">Falhovner er navnet p\u00e5 en hest i den nordiske mytologi.</div>  \u000a      \u000a      <span class="orthography">Falhrifnir</span>\u000a      \u000a   </div>','myth339','myth')
INSERT INTO TEXTREFERENCE VALUES(10559,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth13">\u000a      \u000a      <div class="normForm">Farbaute</div>\u000a      \u000a      <div class="popUp">Farbaute er en j\u00e6tte fra den nordiske mytologi, fader til Loke. Navnet betyder \u2018den,\u000a         som sl\u00e5r h\u00e5rdt\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Forbaute</span>\u000a      \u000a      \u000a   </div>','myth13','myth')
INSERT INTO TEXTREFERENCE VALUES(10560,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.b">\u000a      \u000a      <div class="epithet">Farbautes s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth48.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10561,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth58.a">\u000a      \u000a      <div class="epithet">Fegjafagud\u000a         <span class="sameAs">\u000a            <a href="#myth58"> &gt; Njord</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Navnet Fegjagud betyder velg\u00f8reren.</div>\u000a      \u000a   </div>','myth58.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10562,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth14">\u000a      \u000a      <div class="normForm">Fenris</div>\u000a      \u000a      <div class="popUp">Fenris er en k\u00e6mpeulv fra den nordiske mytologi, s\u00f8n af Loke og Angrbode. Fenris er\u000a         s\u00e5 st\u00e6rk, at alle aserne frygter den, hvorfor den st\u00e5r bundet indtil Ragnarok, hvor\u000a         den river sig l\u00f8s.\u000a      </div>\u000a      \u000a      <span class="orthography">Fenrir</span>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Frekihat</span>\u000a      <span>Ulv.</span>\u000a   </div>','myth14','myth')
INSERT INTO TEXTREFERENCE VALUES(10563,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth81.b">\u000a      \u000a      <div class="epithet">Fenris bane (drabsmand)\u000a         <span class="sameAs">\u000a            <a href="#myth81"> &gt; Vidar</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth81.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10564,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth170">\u000a      \u000a      <div class="normForm">Fensal</div>\u000a      \u000a      <div class="popUp">Fensal er gudinden Friggs bolig i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Fensale</span>\u000a      \u000a      <div class="encyc">Udtrykket \u2018Fensales ve\u2019 henviser til den sorg, Frigg f\u00f8lte, da hendes elskede s\u00f8n,\u000a         Balder, ved Lokes mellemkomst var blevet dr\u00e6bt af en mistelten.\u000a      </div>\u000a      \u000a   </div>','myth170','myth')
INSERT INTO TEXTREFERENCE VALUES(10565,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.e">\u000a      \u000a      <div class="epithet">Fimbultyr\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth59.e','myth')
INSERT INTO TEXTREFERENCE VALUES(10566,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth130">\u000a      \u000a      <div class="normForm">Fimbulvinter</div>\u000a      \u000a      <div class="popUp">I den nordiske mytologi er der en forestilling om, at der f\u00f8r verdens undergang, Ragnarok,\u000a         kommer en s\u00e6rlig lang og streng vinter, tre vintre i tr\u00e6k uden somre.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth130','myth')
INSERT INTO TEXTREFERENCE VALUES(10567,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth266">\u000a      \u000a      <div class="normForm">Finnafeingur</div>\u000a      \u000a      <div class="popUp">Finnafeingur er en tr\u00e6l fra den nordiske mytologi. Han varter op ved et \u00f8lgilde hos\u000a         \u00c6gir, men g\u00f8r det s\u00e5 godt, at Loke paradoksalt nok sl\u00e5r ham ihjel.\u000a      </div>\u000a      \u000a   </div>','myth266','myth')
INSERT INTO TEXTREFERENCE VALUES(10568,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth387">\u000a      \u000a      <div class="normForm">Fjalar</div>\u000a      \u000a      <div class="popUp">Fjalar er en af de to dv\u00e6rge fra den nordiske mytologi, som dr\u00e6ber Kvaser og danner\u000a         skjaldemj\u00f8d af hans blod.\u000a      </div> \u000a      \u000a   </div>','myth387','myth')
INSERT INTO TEXTREFERENCE VALUES(10569,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth74.c">\u000a      \u000a      <div class="epithet">Fjorgyns s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth74"> &gt; Thor</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp"/>\u000a      \u000a   </div>','myth74.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10570,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.f">\u000a      \u000a      <div class="epithet">Fj\u00f8lner\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>       \u000a      \u000a      <div class="popUp">Fj\u00f8lner er et af Odins mange andre tilnavne i den nordiske mytologi. Det optr\u00e6der\u000a         oftest i forbindelse med Alfader. If\u00f8lge P.F. Suhm (1771) betyder det: \u2018den, der ved\u000a         mange ting\u2019.\u000a      </div> \u000a      \u000a      <span class="orthography">Fi\u00f8lner\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Fjolner</span>\u000a      \u000a   </div>','myth59.f','myth')
INSERT INTO TEXTREFERENCE VALUES(10571,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth134">\u000a      \u000a      <div class="normForm">Fj\u00f8lner</div>\u000a      \u000a      <div class="popUp">Fj\u00f8lner er en svensk sagnkonge, s\u00f8n af vanen Frej og j\u00e6ttekvinden Gerd.</div>\u000a      \u000a      <span class="orthography">Fi\u00f8lner</span>\u000a      \u000a      <div class="encyc">Fj\u00f8lner kan ogs\u00e5 v\u00e6re et andet navn for Odin.</div>  \u000a      \u000a      \u000a   </div>','myth134','myth')
INSERT INTO TEXTREFERENCE VALUES(10572,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.g">\u000a      \u000a      <div class="epithet">Fj\u00f8lsvid\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Fj\u00f8lsvid er Odins d\u00e6knavn i et eddadigt, hvor han svarer p\u00e5 de is\u00e6r mytologiske sp\u00f8rgsm\u00e5l,\u000a         som stilles af digtets andre personer. \u000a      </div>\u000a      \u000a      <span class="orthography">Fiaalsvid,</span>\u000a      \u000a      <span class="orthography">Fj\u00f8lsvid,</span>\u000a      \u000a      <span class="orthography">Fi\u00f6lsvidr,</span>\u000a      \u000a      <span class="orthography">Fj\u00f8lsvin\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Fj\u00e5lsvid</span>\u000a      \u000a   </div>','myth59.g','myth')
INSERT INTO TEXTREFERENCE VALUES(10573,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth16">\u000a      \u000a      <div class="normForm">Fj\u00f8rgyn</div> \u000a      \u000a      <div class="popUp">Fj\u00f8rgyn er jordens gudinde i den nordiske mytologi. Sammen med Odin har hun s\u00f8nnen\u000a         Thor.\u000a      </div> \u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Hlodynhat</span>\u000a      <span>Jorden.</span>\u000a   </div>','myth16','myth')
INSERT INTO TEXTREFERENCE VALUES(10574,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth417">\u000a      \u000a      <div class="normForm">Fj\u00f8rgynn</div>\u000a      \u000a      <div class="popUp">Fj\u00f8rgynn en as fra den nordiske mytologi. Han er far til Frigg</div>     \u000a      \u000a      <span class="orthography">Fjorgyn</span>\u000a      \u000a      <div class="encyc">Fj\u00f8rgynn kan forveksles med Fj\u00f8rgyn (Jorden).</div>\u000a      \u000a      \u000a   </div>','myth417','myth')
INSERT INTO TEXTREFERENCE VALUES(10575,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth324">\u000a      \u000a      <div class="normForm">Folkvang</div>\u000a      \u000a      <div class="popUp">Folkvang er navnet p\u00e5 Frejas sal eller bolig i den nordiske mytologi. Det ligger i\u000a         Asg\u00e5rd.\u000a      </div>\u000a      \u000a      <span class="orthography">Folkvangr</span>\u000a      \u000a   </div>','myth324','myth')
INSERT INTO TEXTREFERENCE VALUES(10576,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth196">\u000a      \u000a      <div class="normForm">Forsete</div>\u000a      \u000a      <div class="popUp">Forsete er en as fra den nordiske mytologi. Han er s\u00f8n af Balder og Nanna og bor i\u000a         Glitner, som er bekl\u00e6dt med guld og s\u00f8lv.\u000a      </div>\u000a      \u000a      <div class="encyc">Forsete optr\u00e6der oftest som forligsmand.</div>\u000a      \u000a   </div>','myth196','myth')
INSERT INTO TEXTREFERENCE VALUES(10577,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth268">\u000a      \u000a      <div class="normForm">Franangerfos</div>\u000a      \u000a      <div class="popUp">Franangarfos er navnet p\u00e5 en elv i den nordiske mytologi. Loke fors\u00f8gte i skikkelse\u000a         af en laks at gemme sig i denne elv, men blev ved hj\u00e6lp af et fiskenet fanget af Thor.\u000a      </div>\u000a      \u000a      <span class="orthography">Franangers Fos</span>\u000a      \u000a   </div>','myth268','myth')
INSERT INTO TEXTREFERENCE VALUES(10578,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth18">\u000a      \u000a      <div class="normForm">Frej</div>\u000a      \u000a      <div class="popUp">Frej er gud for k\u00e6rligheden i den nordiske mytologi. Han er s\u00f8n af Njord; broder til\u000a         Freja.\u000a      </div>\u000a      \u000a      <span class="orthography">Fr\u00f8j,</span>\u000a      \u000a      <span class="orthography">Frei,</span>\u000a      \u000a      <span class="orthography">Freir,</span>\u000a      \u000a      <span class="orthography">Frey\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Fr\u00f8</span>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Belas lyse bane (dvs. drabsmand)hat</span>\u000a      <span>Vanagud eller </span>\u000a      <span>\u00c5rgud.</span>\u000a   </div>','myth18','myth')
INSERT INTO TEXTREFERENCE VALUES(10579,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth19">\u000a      \u000a      <div class="normForm">Freja</div>\u000a      \u000a      <div class="popUp">Freja er gudinde for k\u00e6rligheden i den nordiske mytologi. Hun er datter af Njord;\u000a         s\u00f8ster til Frej.\u000a      </div>\u000a      \u000a      <span class="orthography">Freia\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Freya</span>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>den gr\u00e5dfagre gudindehat</span>\u000a      <span>Hnoss\u2019 moder, </span>\u000a      <span>Ottars hustru eller </span>\u000a      <span>Vanadis.</span>\u000a   </div>','myth19','myth')
INSERT INTO TEXTREFERENCE VALUES(10580,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth17">\u000a      \u000a      <div class="normForm">Freke</div>\u000a      \u000a      <div class="popUp">Freke er en helvedshund, dvs. ulv, i den nordiske mytologi. Navnet betyder \u2018den glubske\u2019.\u000a         Freke og Gere var Odins to ulve.\u000a      </div>\u000a      \u000a      <span class="orthography">Freki</span>\u000a      \u000a      <div class="encyc">Hos Suhm (1771) kaldes de Freki og Geri; i eddadigtet \u000a         <span class="italic">Fj\u00f8lsvinns M\u00e1l</span> (da. \u000a         <span class="italic">Fj\u00f8lsvidkvadet</span>) kaldes de G\u00edfr og Geri.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>G\u00edfrhat</span>\u000a   </div>','myth17','myth')
INSERT INTO TEXTREFERENCE VALUES(10581,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth14.a">\u000a      \u000a      <div class="epithet">Freki\u000a         <span class="sameAs">\u000a            <a href="#myth14"> &gt; Fenris</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth14.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10582,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth133">\u000a      \u000a      <div class="normForm">Frigg</div>\u000a      \u000a      <div class="popUp">Frigg er Odins kloge og elskede hustru. Hun er mor til blandt andre Balder.</div>\u000a      \u000a      <span class="orthography">Frigga</span> \u000a      \u000a      <div class="encyc">Friggs far hedder Fj\u00f8rgynn.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Hlinhat</span>\u000a   </div>','myth133','myth')
INSERT INTO TEXTREFERENCE VALUES(10583,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth137">\u000a      \u000a      <div class="normForm">Frode Fredegod</div>\u000a      \u000a      <div class="popUp">Frode Fredegod var en dansk sagnkonge af skjoldungesl\u00e6gten; if\u00f8lge sagnet var hans\u000a         regeringstid s\u00e5 fredelig, at en guldring kunne ligge i vejsiden i \u00e5revis, uden at\u000a         nogen tog den.\u000a      </div>\u000a      \u000a      <div class="encyc">Frode Fredegod var ogs\u00e5 kendt for sin gode og retf\u00e6rdige lovgivning.</div>   \u000a      \u000a   </div>','myth137','myth')
INSERT INTO TEXTREFERENCE VALUES(10584,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth205">\u000a      \u000a      <div class="normForm">Fulla</div>\u000a      \u000a      <div class="popUp">Fulla er en af asynjerne fra den nordiske mytologi. Hun er Friggs fortrolige og b\u00e6rer\u000a         rundt p\u00e5 bl.a. hendes skrin.\u000a      </div>\u000a      \u000a   </div>','myth205','myth')
INSERT INTO TEXTREFERENCE VALUES(10585,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth20">\u000a      \u000a      <div class="normForm">F\u00f8niks</div>\u000a      \u000a      <div class="popUp">F\u00f8niks er den fabelfuglen fra den gr\u00e6ske mytologi, som i d\u00f8den fort\u00e6res af flammer\u000a         for at rejse sig forynget af asken. Den har r\u00f8dder tilbage til egyptisk solkult. \u000a      </div>\u000a      \u000a      <span class="orthography">F\u00f8nix,</span>\u000a      \u000a      <span class="orthography">Foinix\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Phenix</span>\u000a      \u000a      <div class="encyc">Fugl F\u00f8niks er desuden et alm. bevinget ord for noget, der genopst\u00e5r efter en ulykke\u000a         eller et nederlag.\u000a      </div>\u000a      \u000a      <div class="encyc">I middelalderen fik F\u00f8niks en plads i kristen dyresymbolik som et Kristus-symbol.</div>\u000a      \u000a   </div>','myth20','myth')
INSERT INTO TEXTREFERENCE VALUES(10586,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.h">\u000a      \u000a      <div class="epithet">Gagnrad\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Gagnrad er et d\u00e6knavn for Odin. Det betyder \u2018den, der r\u00e5der for sejr\u2019.</div> \u000a      \u000a   </div>','myth59.h','myth')
INSERT INTO TEXTREFERENCE VALUES(10587,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth388">\u000a      \u000a      <div class="normForm">Galar</div>\u000a      \u000a      <div class="popUp">Galar er en af de to dv\u00e6rge fra den nordiske mytologi, som dr\u00e6ber Kvaser og danner\u000a         skjaldemj\u00f8d af hans blod.\u000a      </div>    \u000a      \u000a   </div>','myth388','myth')
INSERT INTO TEXTREFERENCE VALUES(10588,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth373">\u000a      \u000a      <div class="normForm">Gangdoven</div>\u000a      \u000a      <div class="popUp">Gangdoven er navnet p\u00e5 den mandlige tr\u00e6l hos d\u00f8dsgudinden Hel i den nordiske mytologi.\u000a         Navnet betyder \u2018den, der er doven med hensyn til at g\u00e5\u2019.\u000a      </div> \u000a      \u000a      <span class="orthography">Ganglate\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gangl\u00e1ti</span>\u000a      \u000a   </div>','myth373','myth')
INSERT INTO TEXTREFERENCE VALUES(10589,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth374">\u000a      \u000a      <div class="normForm">Ganglad</div>\u000a      \u000a      <div class="popUp">Ganglad er navnet p\u00e5 den kvindelige tr\u00e6l hos d\u00f8dsgudinden Hel i den nordiske mytologi.\u000a         Navnet betyder \u2018den, der er doven med hensyn til at g\u00e5\u2019.\u000a      </div>   \u000a      \u000a      <span class="orthography">Gangl\u00f8t</span>               \u000a      \u000a   </div>','myth374','myth')
INSERT INTO TEXTREFERENCE VALUES(10590,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth21">\u000a      \u000a      <div class="normForm">Garm</div>\u000a      \u000a      <div class="popUp">Garm er en hund i den nordiske mytologi, der vogter vejen til d\u00f8dsriget (sml. Kerberos\u000a         fra den gr\u00e6ske mytologi). \u000a      </div>\u000a      \u000a      <div class="encyc">Af nogle forskere identificeres Garm med Fenrisulven.</div>\u000a      \u000a      <div class="encyc">I eddadigtet \u000a         <span class="italic">Gr\u00edmnism\u00e1l</span> (da. \u000a         <span class="italic">Grimners Sang</span>) hedder det, at Garm er den ypperste blandt hunde.\u000a      </div>\u000a      \u000a   </div>','myth21','myth')
INSERT INTO TEXTREFERENCE VALUES(10591,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth89">\u000a      \u000a      <div class="normForm">Gastrognies</div>\u000a      \u000a      <div class="popUp">Gastrognies er navnet p\u00e5 et g\u00e6rde i den nordiske mytologi. \u2018Gastr\u2019 betyder \u2018g\u00e6st\u2019;\u000a         \u2018(h)ropa\u2019 betyder \u2018r\u00e5be\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Gastropnir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gast-r\u00f3pnir</span>\u000a      \u000a      <div class="encyc">Gastrognies optr\u00e6der i eddadigtet \u000a         <span class="italic">Fj\u00f8lsvinns M\u00e1l</span> (da. \u000a         <span class="italic">Fj\u00f8lsvidkvadet</span>).\u000a      </div>\u000a      \u000a      <div class="encyc">Grundtvig har kunnet l\u00e6se  \u000a         <span class="italic">Fj\u00f8lsvinns M\u00e1l</span> (da. \u000a         <span class="italic">Fj\u00f8lsvidkvadet</span>) i den arnamagn\u00e6anske kommissions udgave bind 1 1787, s. 279 ff.\u000a      </div>\u000a      \u000a   </div>','myth89','myth')
INSERT INTO TEXTREFERENCE VALUES(10592,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth22">\u000a      \u000a      <div class="normForm">Gefion</div>\u000a      \u000a      <div class="popUp">Gefion er en asynje (dvs. kvindelig as) med tr\u00e6k af frugtbarhedsgudinde. Med sine\u000a         fire s\u00f8nner, omskabt til okser, pl\u00f8jer hun p\u00e5 kun \u00e9t d\u00f8gn Sj\u00e6lland ud af Sverige.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth22','myth')
INSERT INTO TEXTREFERENCE VALUES(10593,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth25">\u000a      \u000a      <div class="normForm">Gefion</div>\u000a      \u000a      <div class="popUp">Gefion er en sk\u00e6bnekyndig kvinde fra den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Gefjun</span>\u000a      \u000a   </div>','myth25','myth')
INSERT INTO TEXTREFERENCE VALUES(10594,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth361">\u000a      \u000a      <div class="normForm">Geirraudel</div>\u000a      \u000a      <div class="popUp">Geirraudel er en valkyrie fra den nordiske mytologi. F\u00f8rste del af navnet betyder\u000a         spyd, mens betydningen af sidste del er uklar.\u000a      </div> \u000a      \u000a      <span class="orthography">Geirahod,</span>\u000a      \u000a      <span class="orthography">Geirraudul,</span>\u000a      \u000a      <span class="orthography">Geirr\u00f8lul,</span>\u000a      \u000a      <span class="orthography">Geirr\u00f8mul,</span>\u000a      \u000a      <span class="orthography">Geirr\u00f8nul\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Geir\u00f8nul</span>\u000a      \u000a      <div class="encyc">Det er usikkert, om navnet har en sammenh\u00e6ng med Odinsnavnet Geir\u00f8lnir.</div>\u000a      \u000a   </div>','myth361','myth')
INSERT INTO TEXTREFERENCE VALUES(10595,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth188">\u000a      \u000a      <div class="normForm">Geirr\u00f8d</div>\u000a      \u000a      <div class="popUp">Geirr\u00f8d er en j\u00e6tte fra den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Geirrod\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Geirr\u00f8dr</span>\u000a      \u000a      <div class="encyc">Hos Snorre fort\u00e6lles om, hvordan han til trods for mange kneb besejres af Thor.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Geruthhat</span>\u000a   </div>','myth188','myth')
INSERT INTO TEXTREFERENCE VALUES(10596,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth191">\u000a      \u000a      <div class="normForm">Geirr\u00f8d</div>\u000a      \u000a      <div class="popUp">Geirr\u00f8d er fosters\u00f8n til Odin og den konge, som Odin bes\u00f8ger i forkl\u00e6dning. Geirr\u00f8d\u000a         piner Odin, da han ikke vil give sig til kende.\u000a      </div>\u000a      \u000a      <span class="orthography">Geirrod</span>\u000a      \u000a      <div class="encyc">Om Odin og Geirr\u00f8d kan man l\u00e6se i eddadigtet \u000a         <span class="italic">Grimners Sang.</span>\u000a      </div>\u000a      \u000a   </div>','myth191','myth')
INSERT INTO TEXTREFERENCE VALUES(10597,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth272">\u000a      \u000a      <div class="normForm">Geirskogul</div>\u000a      \u000a      <div class="popUp">Geirskogul er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018spydkamp\u2019.</div>\u000a      \u000a      <span class="orthography">Geirsk\u00f8gul</span>\u000a      \u000a   </div>','myth272','myth')
INSERT INTO TEXTREFERENCE VALUES(10598,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth24">\u000a      \u000a      <div class="normForm">Gerd</div>\u000a      \u000a      <div class="popUp">Gerd er en j\u00e6ttepige, som Frej forelsker sig i. Hun er datter af Aurboda og Gymer.</div>\u000a      \u000a      <span class="orthography">Gerda</span>\u000a      \u000a      <div class="encyc">Om Frejs forelskelse kan man l\u00e6se i eddadigtet \u000a         <span class="italic">F\u01ebr Sk\u00edrnis</span> (da. \u000a         <span class="italic">Skirners F\u00e6rd</span>).\u000a      </div>\u000a      \u000a   </div>','myth24','myth')
INSERT INTO TEXTREFERENCE VALUES(10599,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth23">\u000a      \u000a      <div class="normForm">Gere</div>\u000a      \u000a      <div class="popUp">Gere er en helvedshund, dvs. ulv, i den nordiske mytologi. Navnet betyder \u2018den gr\u00e5dige\u2019.\u000a         Freke og Gere var Odins to ulve.\u000a      </div>\u000a      \u000a      <span class="orthography">Geri</span>\u000a      \u000a      <div class="encyc">Hos P.F. Suhm (1771) kaldes Odins ulve Geri og Freki.</div>\u000a      \u000a      <div class="encyc">I \u000a         <span class="italic">Fj\u00f8lsvinns M\u00e1l</span> (da. \u000a         <span class="italic">Fj\u00f8lsvidkvadet</span>) kaldes de Geri og G\u00edfr.\u000a      </div>  \u000a      \u000a   </div>','myth23','myth')
INSERT INTO TEXTREFERENCE VALUES(10600,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth209.a">\u000a      \u000a      <div class="epithet">Gersemi\u000a         <span class="sameAs">\u000a            <a href="#myth209"> &gt; Hnoss</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Gersemi er en poetisk variant af Hnoss</div>\u000a      \u000a      <span class="orthography">G\u00f8rsemi</span>\u000a      \u000a   </div>','myth209.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10601,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth188.a">\u000a      \u000a      <div class="latin">Geruth\u000a         <span class="sameAs">\u000a            <a href="#myth188"> &gt; Geirr\u00f8d</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth188.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10602,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth121">\u000a      \u000a      <div class="normForm">Gimle</div>\u000a      \u000a      <div class="popUp">Gimle er en sk\u00f8n og skinnende bolig i nordisk mytologi, hvor de retf\u00e6rdige mennesker\u000a         samles og lever lykkeligt efter Ragnarok. \u000a      </div>\u000a      \u000a      <div class="encyc">\u2018Gim\u2019 kan betyde b\u00e5de \u2018ild\u2019 og \u2018\u00e6delsten\u2019; \u2018le\u2019 er det samme som \u2018l\u00e6\u2019. Boligen giver\u000a         da enten l\u00e6 for ilden eller l\u00e6 under \u00e6delsten, dvs. er en bolig, der er t\u00e6kket med\u000a         noget kostbart. Hos Grundtvig \u2013 og mange siden ham \u2013 har tolkningen mest h\u00e6ldet mod\u000a         de sidste, ogs\u00e5 fordi der herved opst\u00e5r en parallel til det himmelske Jerusalem fra\u000a         Johannes\u2019 \u00c5benbaring (\u00c5b 21, 9-21).\u000a      </div>\u000a      \u000a   </div>','myth121','myth')
INSERT INTO TEXTREFERENCE VALUES(10603,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth338">\u000a      \u000a      <div class="normForm">Gisl</div>\u000a      \u000a      <div class="popUp">Gisl er navnet p\u00e5 en hest i den nordiske mytologi.</div>   \u000a      \u000a   </div>','myth338','myth')
INSERT INTO TEXTREFERENCE VALUES(10604,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth416">\u000a      \u000a      <div class="normForm">Gjallar</div>\u000a      \u000a      <div class="popUp"/>               \u000a      \u000a      <div class="encyc">.</div>\u000a      \u000a   </div>','myth416','myth')
INSERT INTO TEXTREFERENCE VALUES(10605,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth178">\u000a      \u000a      <div class="normForm">Gjallarbro</div>\u000a      \u000a      <div class="popUp">Gjallarbro er i den nordiske mytologi den bro, der f\u00f8rer til d\u00f8dsriget, Hel.</div>\u000a      \u000a   </div>','myth178','myth')
INSERT INTO TEXTREFERENCE VALUES(10606,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth26">\u000a      \u000a      <div class="normForm">Gjallarhorn</div>\u000a      \u000a      <div class="popUp">Gjallarhorn er navnet p\u00e5 det horn, som guden Heimdal bl\u00e6ser i, lige inden Ragnarok\u000a         bryder l\u00f8s (nordisk mytologi). Herved v\u00e5gner ogs\u00e5 de d\u00f8de krigere (einherjer), som\u000a         skal k\u00e6mpe p\u00e5 asernes side.\u000a      </div>\u000a      \u000a      <div class="encyc">Hornet er omtalt b\u00e5de i \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) og i \u000a         <span class="italic">Snorres Edda</span>.\u000a      </div>\u000a      \u000a   </div>','myth26','myth')
INSERT INTO TEXTREFERENCE VALUES(10607,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth392">\u000a      \u000a      <div class="normForm">Gjalp</div>\u000a      \u000a      <div class="popUp">Gjalp er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>\u000a      \u000a   </div>','myth392','myth')
INSERT INTO TEXTREFERENCE VALUES(10608,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth400">\u000a      \u000a      <div class="normForm">Gjalp</div>\u000a      \u000a      <div class="popUp">Gjalp en af j\u00e6tten Geirr\u00f8ds d\u00f8tre i den nordiske mytologi.</div>    \u000a      \u000a   </div>','myth400','myth')
INSERT INTO TEXTREFERENCE VALUES(10609,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth332">\u000a      \u000a      <div class="normForm">Glad</div>\u000a      \u000a      <div class="popUp">Glad er navnet p\u00e5 en hest i den nordiske mytologi. Navnet betyder \u2018den glade\u2019 eller\u000a         \u2018den glinsende\u2019.\u000a      </div> \u000a      \u000a      <span class="orthography">Gla\u00f0r\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Glar</span>\u000a      \u000a   </div>','myth332','myth')
INSERT INTO TEXTREFERENCE VALUES(10610,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth347">\u000a      \u000a      <div class="normForm">Gladsheim</div>\u000a      \u000a      <div class="popUp">Gladsheim er navnet p\u00e5 gudernes bolig i den nordiske mytologi. Det betegner enten\u000a         det hele det landomr\u00e5de, hvor Valhal ligger, eller en bygning af purt guld, som guderne\u000a         bor i. \u000a      </div>\u000a      \u000a      <span class="orthography">Gladheim\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gladhejm</span>\u000a      \u000a      <div class="encyc">Gladsheim kan indimellem opfattes som identisk med Valhal.</div>\u000a      \u000a   </div>','myth347','myth')
INSERT INTO TEXTREFERENCE VALUES(10611,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth248">\u000a      \u000a      <div class="normForm">Gleipner</div>\u000a      \u000a      <div class="popUp">Gleipner er navnet p\u00e5 den sidste af af de tre l\u00e6nker, som aserne fors\u00f8gte at binde\u000a         Fenrisulven med. Med Gleipner, der er fremstillet af lyden af kattepotetrin, kvindesk\u00e6g,\u000a         klipper\u00f8dder, bj\u00f8rnesener, fiske\u00e5nde og fuglespyt, lykkedes det.\u000a      </div>\u000a      \u000a      <div class="encyc">De to andre l\u00e6nker var L\u00f8ding og Drome.</div>\u000a      \u000a   </div>','myth248','myth')
INSERT INTO TEXTREFERENCE VALUES(10612,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth334">\u000a      \u000a      <div class="normForm">Gler</div>\u000a      \u000a      <div class="popUp">Gler er navnet p\u00e5 en hest i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Glen</span>\u000a      \u000a   </div>','myth334','myth')
INSERT INTO TEXTREFERENCE VALUES(10613,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth168">\u000a      \u000a      <div class="normForm">Glitner</div>\u000a      \u000a      <div class="popUp">Glitner er guden Forsetes bolig i Asg\u00e5rd (nordisk mytologi). Navnet betyder \u2018den lysende\u2019\u000a         og henviser til, at salen har v\u00e6gge af guld og tag af s\u00f8lv.\u000a      </div>\u000a      \u000a   </div>','myth168','myth')
INSERT INTO TEXTREFERENCE VALUES(10614,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth217">\u000a      \u000a      <div class="normForm">Gna</div>\u000a      \u000a      <div class="popUp">Gna er en asynje, dvs. gudinde, fra den nordiske mytologi. Hun er budbringer og kan\u000a         f\u00e6rdes overalt, selv i luften. Hun opfattes ofte som en af Friggs tjenestepiger.\u000a      </div>\u000a      \u000a   </div>','myth217','myth')
INSERT INTO TEXTREFERENCE VALUES(10615,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth90">\u000a      \u000a      <div class="normForm">Gnyppa Hule</div>\u000a      \u000a      <div class="popUp">Gnyppa Hule er den hule, uden for hvilken hunden (ulven) Garm st\u00e5r l\u00e6nket i den nordiske\u000a         mytologi. Hulen ligger formentlig ved indgangen til Hel.\u000a      </div>\u000a      \u000a      <span class="orthography">Gnyppahulen\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gnyppas hule</span>\u000a      \u000a      \u000a   </div>','myth90','myth')
INSERT INTO TEXTREFERENCE VALUES(10616,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth270">\u000a      \u000a      <div class="normForm">Goin</div>\u000a      \u000a      <div class="popUp">Goin er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">G\u00f3inn</span>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger i kilden Hvergelmer.</div>\u000a      \u000a   </div>','myth270','myth')
INSERT INTO TEXTREFERENCE VALUES(10617,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth360">\u000a      \u000a      <div class="normForm">Goll</div>\u000a      \u000a      <div class="popUp">Goll er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018kamp\u2019.</div>   \u000a      \u000a      <span class="orthography">Gaut\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">G\u00f8ll</span>\u000a      \u000a   </div>','myth360','myth')
INSERT INTO TEXTREFERENCE VALUES(10618,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth273">\u000a      \u000a      <div class="normForm">Gondul</div>\u000a      \u000a      <div class="popUp">Gondul er en valkyrie fra den nordiske mytologi. Ordet kommer af \u2018gandr\u2019 betyder \u2018tryllestav\u2019.</div>\u000a      \u000a      <span class="orthography">G\u00f8ndul</span>\u000a      \u000a   </div>','myth273','myth')
INSERT INTO TEXTREFERENCE VALUES(10619,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth6.a">\u000a      \u000a      <div class="epithet">Graadguden\u000a         <span class="sameAs">\u000a            <a href="#myth6"> &gt; Balder</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth6.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10620,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth283">\u000a      \u000a      <div class="normForm">Grafvalludur</div>\u000a      \u000a      <div class="popUp">Grafvalludur er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger dog i kilden Hvergelmer.</div>\u000a      \u000a   </div>','myth283','myth')
INSERT INTO TEXTREFERENCE VALUES(10621,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth281">\u000a      \u000a      <div class="normForm">Grafvitnir</div>\u000a      \u000a      <div class="popUp">Grafvitnir er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger i kilden Hvergelmer.</div>\u000a      \u000a   </div>','myth281','myth')
INSERT INTO TEXTREFERENCE VALUES(10622,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth139">\u000a      \u000a      <div class="normForm">Gram</div>\u000a      \u000a      <div class="popUp">Gram er det sv\u00e6rd, hvormed Sigurd Fafnersbane dr\u00e6ber dragen Fafner.</div>\u000a      \u000a   </div>','myth139','myth')
INSERT INTO TEXTREFERENCE VALUES(10623,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth391">\u000a      \u000a      <div class="normForm">Greip</div>\u000a      \u000a      <div class="popUp">Greip er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>\u000a      \u000a   </div>','myth391','myth')
INSERT INTO TEXTREFERENCE VALUES(10624,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth399">\u000a      \u000a      <div class="normForm">Greip</div>\u000a      \u000a      <div class="popUp">Greip er en af j\u00e6tten Geirr\u00f8ds d\u00f8tre i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth399','myth')
INSERT INTO TEXTREFERENCE VALUES(10625,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth411">\u000a      \u000a      <div class="normForm">Grep</div>\u000a      \u000a      <div class="popUp">Vestmar havde 12 s\u00f8nner, hvoraf de tre hed Grep, fordi de var trillinger.</div>\u000a      \u000a      <div class="encyc">Man kan l\u00e6se om de tre Grep\u2019per hos Saxo, 5. bog.</div>\u000a      \u000a   </div>','myth411','myth')
INSERT INTO TEXTREFERENCE VALUES(10626,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth242">\u000a      \u000a      <div class="normForm">Grid</div>\u000a      \u000a      <div class="popUp">Grid er en j\u00e6ttekvinde eller heks fra den nordiske mytologi. Sammen med Odin har hun\u000a         s\u00f8nnen Vidar.\u000a      </div>\u000a      \u000a      <span class="orthography">Gridur\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gri\u00f0r</span>\u000a      \u000a   </div>','myth242','myth')
INSERT INTO TEXTREFERENCE VALUES(10627,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.i">\u000a      \u000a      <div class="epithet">Grimner\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Grimner er et af Odins d\u00e6knavne</div>\u000a      \u000a   </div>','myth59.i','myth')
INSERT INTO TEXTREFERENCE VALUES(10628,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth247">\u000a      \u000a      <div class="normForm">Groa</div>\u000a      \u000a      <div class="popUp">Groa er en v\u00f8lve fra den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Gro</span>\u000a      \u000a      <div class="encyc">Da Thor under kampen med j\u00e6tten Hrungner har f\u00e5et en hv\u00e6ssesten ind i hovedskallen,\u000a         skal Groa med en galder (tryllesang) fors\u00f8ge at synge stenen ud igen. Desv\u00e6rre bliver\u000a         hun ikke f\u00e6rdig, s\u00e5 stenen sidder stadig i Thors hoved.\u000a      </div>\u000a      \u000a   </div>','myth247','myth')
INSERT INTO TEXTREFERENCE VALUES(10629,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth282">\u000a      \u000a      <div class="normForm">Gr\u00e5bag</div>\u000a      \u000a      <div class="popUp">Gr\u00e5bag er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil. Navnet betyder \u2018gr\u00e5ryg\u2019. \u000a      </div>\u000a      \u000a      \u000a      <span class="orthography">Grabadr,</span>\u000a      \u000a      <span class="orthography">Grabak,</span>\u000a      \u000a      <span class="orthography">Gr\u00e1bakr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gr\u00e5ryg</span>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger i kilden Hvergelmer.</div>\u000a      \u000a   </div>','myth282','myth')
INSERT INTO TEXTREFERENCE VALUES(10630,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth32.c">\u000a      \u000a      <div class="epithet">Gudevogter\u000a         <span class="sameAs">\u000a            <a href="#myth32"> &gt; Heimdal</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth32.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10631,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth245">\u000a      \u000a      <div class="normForm">Guldfakse</div>\u000a      \u000a      <div class="popUp">Guldfakse er navnet p\u00e5 j\u00e6tten Hrungners hest i den nordiske mytologi. Navnet betyder\u000a         \u2018guldmanke\u2019. Thor for\u00e6rer hesten til sin s\u00f8n Magni, da Magni har reddet ham ud under\u000a         Hrungners d\u00f8de krop.\u000a      </div>\u000a      \u000a   </div>','myth245','myth')
INSERT INTO TEXTREFERENCE VALUES(10632,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth328">\u000a      \u000a      <div class="normForm">Guldtop</div>\u000a      \u000a      <div class="popUp">Guldtop er navnet p\u00e5 Heimdals hest i den nordiske mytologi.</div>  \u000a      \u000a      <span class="orthography">Gulltop</span>\u000a      \u000a   </div>','myth328','myth')
INSERT INTO TEXTREFERENCE VALUES(10633,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth27">\u000a      \u000a      <div class="normForm">Gullveig</div>\u000a      \u000a      <div class="popUp">Gullveig er en kvindeskikkelse fra den nordiske mytologi. Navnet betyder \u2018guldrus\u2019\u000a         eller \u2018guldkraft\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Gulveig</span>\u000a      \u000a      <div class="encyc">I \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) kommer hun fra vanerne og br\u00e6ndes af aserne, men lever videre som en kvinde, der\u000a         ud\u00f8ver sejd (dvs. kan sp\u00e5 og forhekse).\u000a      </div>\u000a      \u000a      <div class="encyc">Hun opfattes indimellem som identisk med gudinden Freja.</div>\u000a      \u000a   </div>','myth27','myth')
INSERT INTO TEXTREFERENCE VALUES(10634,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth251">\u000a      \u000a      <div class="normForm">Gungner</div>\u000a      \u000a      <div class="popUp">Gungner er navnet p\u00e5 Odins spyd i den nordiske mytologi. Det er smedet af dv\u00e6rge.</div>\u000a      \u000a      <span class="orthography">Gugner</span>\u000a      \u000a   </div>','myth251','myth')
INSERT INTO TEXTREFERENCE VALUES(10635,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth181">\u000a      \u000a      <div class="normForm">Gunl\u00f8de</div>\u000a      \u000a      <div class="popUp">Gunl\u00f8de er datter af j\u00e6tten Suttung fra den nordiske mytologi. Hun vogter skjaldemj\u00f8den.\u000a         Odin forf\u00f8rer hende, s\u00e5 han kan stj\u00e6le drikken og bringe den med sig til Asg\u00e5rd.\u000a      </div>\u000a      \u000a      <span class="orthography">Gunl\u00f8d</span>\u000a      \u000a   </div>','myth181','myth')
INSERT INTO TEXTREFERENCE VALUES(10636,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth237">\u000a      \u000a      <div class="normForm">Gunn</div>\u000a      \u000a      <div class="popUp">Gunn er en valkyrie fra den nordiske mytologi. Navnet betyder kamp.</div>\u000a      \u000a      <span class="orthography">Gunner</span>\u000a      \u000a   </div>','myth237','myth')
INSERT INTO TEXTREFERENCE VALUES(10637,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth365">\u000a      \u000a      <div class="normForm">Gunnr</div>\u000a      \u000a      <div class="popUp">Gunnr er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018kamp\u2019.</div> \u000a      \u000a      <span class="orthography">Gu\u00f0r\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gudur</span>\u000a      \u000a   </div>','myth365','myth')
INSERT INTO TEXTREFERENCE VALUES(10638,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth254">\u000a      \u000a      <div class="normForm">Gyge</div>\u000a      \u000a      <div class="popUp">En gyge er et andet navn for en j\u00e6ttekvinde i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Gygir</span>\u000a      \u000a   </div>','myth254','myth')
INSERT INTO TEXTREFERENCE VALUES(10639,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth190">\u000a      \u000a      <div class="normForm">Gyldenb\u00f8rste</div>\u000a      \u000a      <div class="popUp">Gyldenb\u00f8rste er en galt (kastreret hangris) fra den nordiske mytologi, som tilh\u00f8rer\u000a         Frej. Dens b\u00f8rster er af guld (deraf navnet) og lyser om natten.\u000a      </div>\u000a      \u000a      <span class="orthography">Gullinbursti\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gullinb\u00f8rste</span>\u000a      \u000a      \u000a      <div class="encyc">Gyldenb\u00f8rste kan l\u00f8be hurtigere end en hest og kan desuden flyve.</div>\u000a      \u000a   </div>','myth190','myth')
INSERT INTO TEXTREFERENCE VALUES(10640,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth28">\u000a      \u000a      <div class="normForm">Gyldenkam</div>\u000a      \u000a      <div class="popUp">Gyldenkam er en hane, der i den nordiske mytologi v\u00e6kker Valhals krigere til Ragnarok.</div>\u000a      \u000a      <span class="orthography">Den guldkammede\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gullinkambi</span>\u000a      \u000a      \u000a   </div>','myth28','myth')
INSERT INTO TEXTREFERENCE VALUES(10641,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth32.a">\u000a      \u000a      <div class="epithet">Gyldentand\u000a         <span class="sameAs">\u000a            <a href="#myth32"> &gt; Heimdal</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth32.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10642,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth193">\u000a      \u000a      <div class="normForm">Gylfe</div>\u000a      \u000a      <div class="popUp">Gylfe er en svensk sagnkonge.</div>\u000a      \u000a      <div class="encyc">I eddadigtet \u000a         <span class="italic">Gylfaginning</span> (da. \u000a         <span class="italic">Gylfes Forbl\u00e6ndelse</span>) stiller Gylfe mytologiske sp\u00f8rgsm\u00e5l til den forkl\u00e6dte Odin, der bes\u00f8ger Geirr\u00f8d.\u000a      </div>\u000a      \u000a      <div class="encyc"> I \u000a         <span class="italic">Ynglingesaga</span> giver Gylfe asynjen Gefion det land, som hun p\u00e5 et d\u00f8gn kan pl\u00f8je ud af Sverige.\u000a         Hun skaber sine fire s\u00f8nner om til okser; og de pl\u00f8jer hele Sj\u00e6lland ud. Hullet bliver\u000a         efter traditionen til s\u00f8en M\u00e4laren.\u000a      </div>      \u000a      \u000a      \u000a   </div>','myth193','myth')
INSERT INTO TEXTREFERENCE VALUES(10643,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth333">\u000a      \u000a      <div class="normForm">Gyller</div>\u000a      \u000a      <div class="popUp">Gyller er navnet p\u00e5 en hest i den nordiske mytologi.</div>  \u000a      \u000a      <span class="orthography">Gylden\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Gyllir</span>\u000a      \u000a   </div>','myth333','myth')
INSERT INTO TEXTREFERENCE VALUES(10644,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth29">\u000a      \u000a      <div class="normForm">Gymer</div>\u000a      \u000a      <div class="popUp">Gymer er en j\u00e6tte fra den nordiske mytologi. Han er fader til den smukke Gerd, som\u000a         Frej forelsker sig i.\u000a      </div>\u000a      \u000a      <span class="orthography">Gyme</span>\u000a      \u000a      <div class="encyc">Om Frejs forelskelse kan man l\u00e6se i eddadigtet \u000a         <span class="italic">Skirners F\u00e6rd.</span>\u000a      </div>\u000a      \u000a   </div>','myth29','myth')
INSERT INTO TEXTREFERENCE VALUES(10645,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth85.a">\u000a      \u000a      <div class="epithet">Gymer\u000a         <span class="sameAs">\u000a            <a href="#myth85"> &gt; \u00c6gir</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Gymer et andet navn for \u00c6gir. Ud fra kilderne er der ingen grund til at tro, at der\u000a         er tale om den samme som j\u00e6tten Gymer, der er far til Gerd.\u000a      </div>\u000a      \u000a   </div>','myth85.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10646,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth412">\u000a      \u000a      <div class="normForm">G\u00f8tvor</div>\u000a      \u000a      <div class="popUp">G\u00f8tvor er en ualmindelig grovmundet kvinde fra den danske sagnhistorie. Hun havde\u000a         12 s\u00f8nner med Vestmar, hvoraf de tre hed Grep, fordi de var trillinger.\u000a      </div>\u000a      \u000a      <div class="encyc">Man kan l\u00e6se om G\u00f8tvor hos Saxo, 5. bog.</div>\u000a      \u000a   </div>','myth412','myth')
INSERT INTO TEXTREFERENCE VALUES(10647,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth17.a">\u000a      \u000a      <div class="epithet">G\u00edfr\u000a         <span class="sameAs">\u000a            <a href="#myth17"> &gt; Freke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth17.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10648,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth156">\u000a      \u000a      <div class="normForm">Hades</div>\u000a      \u000a      <div class="popUp">Hades er b\u00e5de navnet p\u00e5 d\u00f8dsrigets gud og p\u00e5 selve d\u00f8dsriget i den gr\u00e6ske mytologi.\u000a         Guden Hades er s\u00f8n af Kronos og bror til Zeus og Poseidon. D\u00f8dsriget opfattes som\u000a         et tr\u00f8stesl\u00f8st m\u00f8rke.\u000a      </div>\u000a      \u000a      <span class="orthography">Ades</span>\u000a      \u000a      <div class="encyc">I den romerske mytologi er de tilsvarende navne: Orcus, s\u00f8n af Saturnus, bror til\u000a         Jupiter og Neptun.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>hat</span>\u000a      <span>Pluton.</span>\u000a   </div>','myth156','myth')
INSERT INTO TEXTREFERENCE VALUES(10649,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth345">\u000a      \u000a      <div class="normForm">Hagbard</div>\u000a      \u000a      <div class="popUp">Hagbard er en norsk konges\u00f8n og helt fra den nordiske sagnhistorie.</div>\u000a      \u000a      <span class="orthography">Hagbart\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Hagbarth</span>\u000a      \u000a      <div class="encyc">Hagbard elsker den danske kongedatter, Signe, men de m\u00e5 ikke f\u00e5 hinanden, da han i\u000a         krig har sl\u00e5et flere af hendes br\u00f8dre ihjel. Det lykkes dog Hagbard og Signe at tilbringe\u000a         en nat sammen, hvor de lover hinanden troskab ud over d\u00f8den. Hagbard forr\u00e5des af en\u000a         af Signes terner og m\u00e5 d\u00f8. Som et sidste \u00f8nske inden d\u00f8den beder han om at se, hvordan\u000a         hans kappe tager sig ud i den galge, hvori han selv skal h\u00e6nges. Da Signe ser kappen\u000a         dingle, tror hun, at det er Hagbard, og stikker ild p\u00e5 sit v\u00e6relse. Da Hagbard \u00f8jner\u000a         r\u00f8gen herfra, ved han, at hun har holdt sit l\u00f8fte, og g\u00e5r selv glad i d\u00f8den.\u000a      </div>\u000a      \u000a   </div>','myth345','myth')
INSERT INTO TEXTREFERENCE VALUES(10650,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth30">\u000a      \u000a      <div class="normForm">Halfdan</div>\u000a      \u000a      <div class="popUp">Halfdan var en dansk sagnkonge, fader til Roar og Helge.</div>\u000a      \u000a      \u000a   </div>','myth30','myth')
INSERT INTO TEXTREFERENCE VALUES(10651,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth32.b">\u000a      \u000a      <div class="epithet">Hallinskide\u000a         <span class="sameAs">\u000a            <a href="#myth32"> &gt; Heimdal</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth32.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10652,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth210">\u000a      \u000a      <div class="normForm">Harald Hildetand</div>\u000a      \u000a      <div class="popUp">Harald Hildetand var en dansk sagnkonge, den sidste af skjoldungerne</div>\u000a      \u000a   </div>','myth210','myth')
INSERT INTO TEXTREFERENCE VALUES(10653,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth302">\u000a      \u000a      <div class="normForm">Hate</div>\u000a      \u000a      <div class="popUp">Hate er en j\u00e6tte i ulveskikkelse fra den nordiske mytologi. Det siges, at den vil\u000a         sluge m\u00e5nen ved Ragnarok. Navnet betyder \u2018foragter\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Hati</span>\u000a      \u000a   </div>','myth302','myth')
INSERT INTO TEXTREFERENCE VALUES(10654,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth31">\u000a      \u000a      <div class="normForm">Hefaistos</div>\u000a      \u000a      <div class="popUp">Hefaistos er ild- og smedegud i den gr\u00e6ske mytologi. Han er gift med k\u00e6rlighedsgudinden\u000a         Afrodite.\u000a      </div>\u000a      \u000a      <div class="encyc">I den romerske mytologi identificeres han med Vulcanus.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Vulcanushat</span>\u000a   </div>','myth31','myth')
INSERT INTO TEXTREFERENCE VALUES(10655,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth319">\u000a      \u000a      <div class="normForm">Heidrik</div>\u000a      \u000a      <div class="popUp">Heidrek er en af hovedpersonerne i den islandske sagalitteratur.</div>\u000a      \u000a      <div class="encyc">I \u000a         <span class="italic">Hervarar Saga ok Hei\u00f0reks</span> (da. \u000a         <span class="italic">Hervars og Heidreks saga</span>) kan man l\u00e6se om Heidrek.)\u000a      </div>\u000a      \u000a   </div>','myth319','myth')
INSERT INTO TEXTREFERENCE VALUES(10656,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth352">\u000a      \u000a      <div class="normForm">Heidrun</div>\u000a      \u000a      <div class="popUp">Heidrun er en ged i den nordiske mytologi, fra hvis yver der str\u00f8mmer mj\u00f8d.</div> \u000a      \u000a      <span class="orthography">Heidrune</span>\u000a      \u000a   </div>','myth352','myth')
INSERT INTO TEXTREFERENCE VALUES(10657,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth32">\u000a      \u000a      <div class="normForm">Heimdal</div>\u000a      \u000a      <div class="popUp">Heimdal er den gud (as), der i den nordiske mytologi vogter regnbuen, som forbinder\u000a         mennesker i Midg\u00e5rd og guder i Asg\u00e5rd. Han blev f\u00f8dt af ni s\u00f8stre, der alle var jomfruer.\u000a      </div>\u000a      \u000a      <span class="orthography">Heimdalur,</span> \u000a      \u000a      <span class="orthography">Heimdalr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Hejmdal</span>\u000a      \u000a      <div class="encyc">Han kan omtales som \u2018den hvide as\u2019, fx i \u000a         <span class="italic">Odins Ravnegalder</span> eller hos Snorre.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Gudevogterhat</span>\u000a      <span>Gyldentand, </span>\u000a      <span>Hallinskide, </span>\u000a      <span>hvide as eller </span>\u000a      <span>ni m\u00f8dres s\u00f8n.</span>\u000a   </div>','myth32','myth')
INSERT INTO TEXTREFERENCE VALUES(10658,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth33">      \u000a      \u000a      <div class="normForm">Hel</div>\u000a      \u000a      <div class="popUp">Hel er d\u00f8dsrigets gudinde i nordisk mytologi, datter af Loke. Betegnelsen bruges ogs\u00e5\u000a         ofte om hendes rige.\u000a      </div>\u000a      \u000a      <span class="orthography">Hela</span>\u000a      \u000a      \u000a   </div>','myth33','myth')
INSERT INTO TEXTREFERENCE VALUES(10659,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth34">\u000a      \u000a      <div class="normForm">Helge</div>\u000a      \u000a      <div class="popUp">Helge var en dansk sagnkonge af skjoldungesl\u00e6gten. Han var bror til Roar, med hvem\u000a         han delte magten, s\u00e5ledes at Roar regerede landet (omkring Lejre), og Helge var s\u00f8konge.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth34','myth')
INSERT INTO TEXTREFERENCE VALUES(10660,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth382">\u000a      \u000a      <div class="normForm">Helge Hundingsbane</div>\u000a      \u000a      <div class="popUp">Helge Hundingsbane er en helt fra den nordiske sagnhistorie, mest kendt fra k\u00e6rlighedshistorien\u000a         med Sigrun.\u000a      </div>\u000a      \u000a      <div class="encyc">Da han er d\u00f8d, f\u00e5r han som d\u00f8dning lov at bes\u00f8ge hende; og de ligger sammen en hel\u000a         nat. Men hun d\u00f8r af sorg over, at han ikke kommer tilbage til hende igen. Han er halvbror\u000a         til Sigurd Fafnersbane.\u000a      </div> \u000a      \u000a      <div class="encyc">Hos Saxo er Helge s\u00f8n af Halvdan og bror til Roar.</div>\u000a      \u000a   </div>','myth382','myth')
INSERT INTO TEXTREFERENCE VALUES(10661,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth6.b">\u000a      \u000a      <div class="epithet">Hels Herfang (bytte)\u000a         <span class="sameAs">\u000a            <a href="#myth6"> &gt; Balder</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth6.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10662,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth91">\u000a      \u000a      <div class="normForm">Hels sale</div>\u000a      \u000a      <div class="popUp">Hels sale er Hels bolig, dvs. d\u00f8dsriget i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Helas v\u00e5ning\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Helas h\u00f8ie Borg</span>\u000a      \u000a      \u000a   </div>','myth91','myth')
INSERT INTO TEXTREFERENCE VALUES(10663,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth172">\u000a      \u000a      <div class="normForm">Hera</div>\u000a      \u000a      <div class="popUp">Hera er overguden Zeus\u2019 hustru i den gr\u00e6ske mytologi.</div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes hun Juno.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Junohat</span>\u000a   </div>','myth172','myth')
INSERT INTO TEXTREFERENCE VALUES(10664,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth35">\u000a      \u000a      <div class="normForm">Herakles</div>\u000a      \u000a      <div class="popUp">Herakles var s\u00f8n af Zeus og Alkmene (gr\u00e6sk mytologi). For at blive ud\u00f8delig m\u00e5tte\u000a         Herakles gennem 12 \u00e5r udf\u00f8re 12 arbejder for kong Eurystheus.\u000a      </div>\u000a      \u000a      <div class="encyc">Som ung blev Herakles stillet over for valget mellem dyd og last og valgte rigtigt.\u000a         Dette motiv er vidt udbredt i europ\u00e6isk litteratur, billedkunst og moralfilosofi som\u000a         sindbillede p\u00e5 det menneske, der opn\u00e5r ud\u00f8delighed gennem dyd og udholdenhed i strabadser.\u000a      </div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes Herakles for Herkules.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Herkuleshat</span>\u000a   </div>','myth35','myth')
INSERT INTO TEXTREFERENCE VALUES(10665,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth359">\u000a      \u000a      <div class="normForm">Herfjotur</div>\u000a      \u000a      <div class="popUp">Herfjotur er en valkyrie fra den nordiske mytologi. Navnet er ligner en kenninge (poetisk\u000a         omskrivning) og betyder \u2018h\u00e6rens l\u00e6nke\u2019.\u000a      </div>\u000a      \u000a   </div>','myth359','myth')
INSERT INTO TEXTREFERENCE VALUES(10666,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.k">\u000a      \u000a      <div class="epithet">Herjan\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Herian</span>\u000a      \u000a   </div>','myth59.k','myth')
INSERT INTO TEXTREFERENCE VALUES(10667,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth117.a">\u000a      \u000a      <div class="epithet">Herjans M\u00f8er\u000a         <span class="sameAs">\u000a            <a href="#myth117"> &gt; valkyrie</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Herians M\u00f8er</span>\u000a      \u000a   </div>','myth117.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10668,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth35.a">\u000a      \u000a      <div class="latin">Herkules\u000a         <span class="sameAs">\u000a            <a href="#myth35"> &gt; Herakles</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Hercules</span>\u000a      \u000a      \u000a   </div>','myth35.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10669,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth129">\u000a      \u000a      <div class="normForm">Hermod</div>\u000a      \u000a      <div class="popUp">Hermod er s\u00f8n af Odin og Frigg og bror til Balder. Efter Balders d\u00f8d er det Hermod,\u000a         der rider til d\u00f8dsriget for at f\u00e5 ham udfriet.\u000a      </div>\u000a      \u000a      <div class="encyc">.</div>\u000a      \u000a   </div>','myth129','myth')
INSERT INTO TEXTREFERENCE VALUES(10670,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth381">\u000a      \u000a      <div class="normForm">Hervor</div>\u000a      \u000a      <div class="popUp">Hervor er en skjoldm\u00f8 fra den nordiske sagnhistorie. Hun er s\u00f8ster (eller datter)\u000a         til goterkongen Angantyr og mor til Heidrek.\u000a      </div>\u000a      \u000a   </div>','myth381','myth')
INSERT INTO TEXTREFERENCE VALUES(10671,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth274">\u000a      \u000a      <div class="normForm">Hild</div>\u000a      \u000a      <div class="popUp">Hild er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018kamp\u2019.</div>\u000a      \u000a      <span class="orthography">Hildir</span>\u000a      \u000a   </div>','myth274','myth')
INSERT INTO TEXTREFERENCE VALUES(10672,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth366">\u000a      \u000a      <div class="normForm">Hildr</div>\u000a      \u000a      <div class="popUp">Hildr er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018kamp\u2019.</div>\u000a      \u000a   </div>','myth366','myth')
INSERT INTO TEXTREFERENCE VALUES(10673,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth326">\u000a      \u000a      <div class="normForm">Himmelbjerg</div>\u000a      \u000a      <div class="popUp">Himmelbjerg er navnet p\u00e5 Heimdals bolig i den nordiske mytologi. Den ligger ved foden\u000a         af regnbuen, Bifrost.\u000a      </div>\u000a      \u000a   </div>','myth326','myth')
INSERT INTO TEXTREFERENCE VALUES(10674,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth408">\u000a      \u000a      <div class="normForm">Hjarne</div>\u000a      \u000a      <div class="popUp">Efter den danske konge Frode Fredegods d\u00f8d tilb\u00f8d folket hans krone til den, der kunne\u000a         give den afd\u00f8de den bedste efterm\u00e6le i digtform; den veltalende Hjarne vandt.\u000a      </div>\u000a      \u000a      <div class="encyc">Denne fort\u00e6lling kan man l\u00e6se hos Saxo.</div>    \u000a      \u000a   </div>','myth408','myth')
INSERT INTO TEXTREFERENCE VALUES(10675,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth85.b">\u000a      \u000a      <div class="epithet">Hler\u000a         <span class="sameAs">\u000a            <a href="#myth85"> &gt; \u00c6gir</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth85.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10676,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth145">\u000a      \u000a      <div class="normForm">Hlidskjalv</div>\u000a      \u000a      <div class="popUp">Hlidskjalv er Odins h\u00f8js\u00e6de i den nordiske mytologi. Herfra kan man overskue hele\u000a         verden.\u000a      </div>\u000a      \u000a      <span class="orthography">Hlidskjalf,</span> \u000a      \u000a      <span class="orthography">Hlidskialf,</span>\u000a      \u000a      <span class="orthography">Hli\u00f0skj\u00e1lf\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Lidskjalv</span>\u000a      \u000a   </div>','myth145','myth')
INSERT INTO TEXTREFERENCE VALUES(10677,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth133.a">\u000a      \u000a      <div class="epithet">Hlin\u000a         <span class="sameAs">\u000a            <a href="#myth133"> &gt; Frigg</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Det alternative navn for Frigg, Hlin, opfattes af nogle som navnet p\u00e5 en af Friggs\u000a         tjenestepiger.\u000a      </div>\u000a      \u000a      <span class="orthography">Hlyn</span>\u000a      \u000a   </div>','myth133.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10678,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth16.a">\u000a      \u000a      <div class="epithet">Hlodyn\u000a         <span class="sameAs">\u000a            <a href="#myth16"> &gt; Fj\u00f8rgyn</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Hlodynia</span>\u000a      \u000a   </div>','myth16.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10679,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth74.d">\u000a      \u000a      <div class="epithet">Hlodyns s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth74"> &gt; Thor</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp"/>\u000a      \u000a      <span class="orthography">Hlodynia s\u00f8n,</span>\u000a      \u000a      <span class="orthography">Hloride (s\u00f8n af Hlodyn),</span>\u000a      \u000a      <span class="orthography">Hlorrida (s\u00f8n af Hlodyn)\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Hlorridi (s\u00f8n af Hlodyn)</span>\u000a      \u000a      \u000a      \u000a   </div>','myth74.d','myth')
INSERT INTO TEXTREFERENCE VALUES(10680,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth358">\u000a      \u000a      <div class="normForm">Hlokk</div>\u000a      \u000a      <div class="popUp">Hlokk er en valkyrie fra den nordiske mytologi.</div>    \u000a      \u000a      <span class="orthography">Hlauk</span>\u000a      \u000a      \u000a   </div>','myth358','myth')
INSERT INTO TEXTREFERENCE VALUES(10681,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth296">\u000a      \u000a      <div class="normForm">Hlymnir</div>\u000a      \u000a      <div class="popUp">Hlymnir er navnet p\u00e5 en af himlene i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth296','myth')
INSERT INTO TEXTREFERENCE VALUES(10682,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth209">\u000a      \u000a      <div class="normForm">Hnoss</div>\u000a      \u000a      <div class="popUp">Hnoss er datter af Freja og Od fra den nordiske mytologi. Hendes navn betyder \u2018kostbarhed\u2019\u000a         eller \u2018klenodie\u2019.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Gersemihat</span>\u000a   </div>','myth209','myth')
INSERT INTO TEXTREFERENCE VALUES(10683,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth19.b">\u000a      \u000a      <div class="epithet">Hnoss\u2019 moder\u000a         <span class="sameAs">\u000a            <a href="#myth19"> &gt; Freja</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth19.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10684,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth36">\u000a      \u000a      <div class="normForm">Hoddmimer</div>\u000a      \u000a      <div class="popUp">Hoddmimer er ejer af den skov, hvor menneskeparret Liv og Livtraser skjuler sig efter\u000a         Fimbulvinteren i den nordiske mytologi.\u000a      </div>\u000a      \u000a      <span class="orthography">Hoddm\u00edmer,</span>\u000a      \u000a      <span class="orthography">Hoddmime\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Hodd Mimer</span>\u000a      \u000a      <div class="encyc">Navnet kan muligvis betyde noget i retning af \u2018skattekiste-mimer\u2019, hvilket knytter\u000a         ham til den Mimer, der ejer visdommens kilde ved Yggdrasils fod; se ogs\u00e5 Mimamejd.\u000a      </div>\u000a      \u000a   </div>','myth36','myth')
INSERT INTO TEXTREFERENCE VALUES(10685,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth92">\u000a      \u000a      <div class="normForm">Hoddmimers H\u00f8je</div>\u000a      \u000a      <div class="popUp">Hoddmimers H\u00f8je er det sted, hvor menneskeparret Liv og Livtraser skjuler sig efter\u000a         Fimbulvinteren i den nordiske mytologi. M\u00e5ske er stedet identisk med asken Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">Hoddmimers Skov</span>\u000a      \u000a      \u000a   </div>','myth92','myth')
INSERT INTO TEXTREFERENCE VALUES(10686,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth37">\u000a      \u000a      <div class="normForm">Holger Danske</div>\u000a      \u000a      <div class="popUp">Holger Danske er en europ\u00e6isk sagnfigur med s\u00e6rlig betydning som dansk nationalhelt.</div>\u000a      \u000a      <div class="encyc">Han optr\u00e6der tidligst som en af Karl den Stores krigere i \u000a         <span class="italic">La Chanson de Roland</span> (da. \u000a         <span class="italic">Rolandskvadet</span>, ca. 1060). Her er han en bifigur, hvis eneste tilknytning til Danmark er navnet\u000a         Oger le Danois (danskeren). I nordiske kilder kendes han tidligst fra \u000a         <span class="italic">Karlamagn\u00fassaga</span>, en vestnordisk prosagengivelse fra sidste halvdel af 1200-tallet, hovedsagelig af\u000a         franske digte om Karl den Store og hans m\u00e6nd; tredje del handler om Oddgeir danski.\u000a         Den vestnordiske tekst blev oversat til dansk i 1400-tallet, hvor den kendes som \u000a         <span class="italic">Karl Magnus\u2019</span> kr\u00f8nike, trykt f\u00f8rste gang 1509.\u000a      </div>\u000a      \u000a      <div class="encyc">Da Christiern Pedersen (1480-1554) sprogligt gennemrettede det \u00e6ldre tryk og udgav\u000a         teksten 1534, \u00e6ndrede han heltens navn fra Udger til Olger, og samme \u00e5r udgav han\u000a         \u000a         <span class="italic">Kong Olger Danskis Kr\u00f8nicke</span>, en overs\u00e6ttelse af en fransk roman om Ogier, tidligst kendt fra en trykt bog 1496.\u000a         Det var sidstn\u00e6vnte version af beretningerne om Holger Danske, der levede videre som\u000a         en ofte optrykt, meget yndet dansk folkebog langt op i 1800-tallet. Den er hovedkilden\u000a         til danskernes kendskab til heltens bedrifter og til sagnene om hans genkomst fra\u000a         sine underjordiske opholdssteder (bl.a. Bulbjerg, Lovns, Nonnebakken i Odense og Kronborg)\u000a         for at frelse nationen i yderste n\u00f8d.\u000a      </div>\u000a      \u000a      <div class="encyc">Holger Danske blev en central figur i dansk litter\u00e6r og musikalsk romantik, kulminerende\u000a         med B.S. Ingemanns episke digtcyklus \u000a         <span class="italic">Holger Danske. Et Digt</span> (1837); nogle af digtene heri \u2013 bl.a. \u201cHolger Danskes M\u00e6rke\u201d (\u201cI alle de Riger og\u000a         Lande\u201d) \u2013 sattes i musik og fik siden stor betydning i nationale krisetider.\u000a      </div>   \u000a      \u000a   </div>','myth37','myth')
INSERT INTO TEXTREFERENCE VALUES(10687,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth128.a">\u000a      \u000a      <div class="epithet">Hother\u000a         <span class="sameAs">\u000a            <a href="#myth128"> &gt; H\u00f8der</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">I Saxos fort\u00e6lling om Balder og hans d\u00f8d er det Hother, der uforvarende dr\u00e6ber Balder\u000a         uden Lokes mellemkomst.\u000a      </div>\u000a      \u000a      <span class="orthography">H\u00f8der</span>\u000a      \u000a   </div>','myth128.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10688,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth142">\u000a      \u000a      <div class="normForm">Hrimgrimner</div>\u000a      \u000a      <div class="popUp">Hrimgrimner er en j\u00e6tte fra den nordiske mytologi, der holder til i d\u00f8dsriget. Navnet\u000a         antyder, at han h\u00f8rer til rimturserne.\u000a      </div>\u000a      \u000a   </div>','myth142','myth')
INSERT INTO TEXTREFERENCE VALUES(10689,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth141">\u000a      \u000a      <div class="normForm">Hrimner</div>\u000a      \u000a      <div class="popUp">Hrimner er en j\u00e6tte fra den nordiske mytologi, hvis navn betyder, at han er d\u00e6kket\u000a         af rim.\u000a      </div>\u000a      \u000a   </div>','myth141','myth')
INSERT INTO TEXTREFERENCE VALUES(10690,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth295">\u000a      \u000a      <div class="normForm">Hriodr</div>\u000a      \u000a      <div class="popUp">Hriodr er navnet p\u00e5 en af himlene i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth295','myth')
INSERT INTO TEXTREFERENCE VALUES(10691,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth354">\u000a      \u000a      <div class="normForm">Hrist</div>\u000a      \u000a      <div class="popUp">Hrist er en ung opvarterske i den nordiske mytologi. Hun b\u00e6rer drikkehorn til Odin.</div>\u000a      \u000a   </div>','myth354','myth')
INSERT INTO TEXTREFERENCE VALUES(10692,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.l">\u000a      \u000a      <div class="epithet">Hropt\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Hroptatyr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Hroptur</span>\u000a      \u000a   </div>','myth59.l','myth')
INSERT INTO TEXTREFERENCE VALUES(10693,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth243">\u000a      \u000a      <div class="normForm">Hrungner</div>\u000a      \u000a      <div class="popUp">Hrungner er en pralende og fordrukken j\u00e6tte fra den nordiske mytologi. Han sl\u00e5s ihjel\u000a         af Thor, da han ved en fejltagelse har forvildet sig ind i Asg\u00e5rd. Thor for\u00e6rer Hrungners\u000a         hest, Guldfakse, til sin s\u00f8n, halvj\u00e6tten Magni.\u000a      </div>\u000a      \u000a      <span class="orthography">Hrugner\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Rungner</span>\u000a      \u000a   </div>','myth243','myth')
INSERT INTO TEXTREFERENCE VALUES(10694,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth383">\u000a      \u000a      <div class="normForm">Hr\u00e6sv\u00e6lgr</div>\u000a      \u000a      <div class="popUp">Hr\u00e6sv\u00e6lgr er en j\u00e6tte i \u00f8rneskikkelse fra den nordiske mytologi. Dens v\u00e6ldige vingeslag\u000a         skaber m\u00e6gtige storme. Navnet betyder \u2018\u00e5dsels- eller ligsluger\u2019.\u000a      </div>\u000a      \u000a   </div>','myth383','myth')
INSERT INTO TEXTREFERENCE VALUES(10695,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth312">\u000a      \u000a      <div class="normForm">Hugin</div>\u000a      \u000a      <div class="popUp">Hugin er den ene af Odins to ravne i den nordiske mytologi. Navnet betyder \u2018tanken\u2019.</div>\u000a      \u000a      <div class="encyc">Ravnene, Hugin og Munin, sidder p\u00e5 Odins skuldre og fort\u00e6ller Odin alt, hvad de h\u00f8rer\u000a         og ser ske i verden.\u000a      </div>\u000a      \u000a   </div>','myth312','myth')
INSERT INTO TEXTREFERENCE VALUES(10696,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth370">\u000a      \u000a      <div class="normForm">Hunger</div>\u000a      \u000a      <div class="popUp">Hunger er navnet p\u00e5 spisebordet hos d\u00f8dsgudinden Hel i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth370','myth')
INSERT INTO TEXTREFERENCE VALUES(10697,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth278">\u000a      \u000a      <div class="normForm">Hvergelmer</div>\u000a      \u000a      <div class="popUp">Hvergelmer er navnet p\u00e5 en kilde fra den nordiske mytologi. Den ligger i enten Niflheim\u000a         eller er en af de kilder, der str\u00f8mmer under Yggdrasils r\u00f8dder.\u000a      </div>\u000a      \u000a   </div>','myth278','myth')
INSERT INTO TEXTREFERENCE VALUES(10698,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth239">\u000a      \u000a      <div class="normForm">Hymer</div>\u000a      \u000a      <div class="popUp">Hymer er en j\u00e6tte fra den nordiske mytologi. Han ejer en stor kedel, der bruges til\u000a         at brygge \u00f8l i til et gilde hos havguden \u00c6gir.\u000a      </div>\u000a      \u000a      <div class="encyc">Det er Hymer, der tager Thor p\u00e5 fisketur for at fange Midg\u00e5rdsormen.</div>\u000a      \u000a   </div>','myth239','myth')
INSERT INTO TEXTREFERENCE VALUES(10699,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth38">\u000a      \u000a      <div class="normForm">Hyndla</div>\u000a      \u000a      <div class="popUp">Hyndla er en troldkvinde af j\u00e6ttesl\u00e6gt. Navnet betyder egentlig \u2018den lille hund\u2019.</div>\u000a      \u000a      <span class="orthography">Hyndle</span>\u000a      \u000a      <div class="encyc">I eddadigtet \u000a         <span class="italic">Hyndlas Sang</span> fort\u00e6lles, hvordan Hyndla hjalp Freja med at udrede sl\u00e6gtsskabsforholdene for den\u000a         ellers ukendte Ottar.\u000a      </div>\u000a      \u000a   </div>','myth38','myth')
INSERT INTO TEXTREFERENCE VALUES(10700,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth39">\u000a      \u000a      <div class="normForm">Hyrrokin</div>\u000a      \u000a      <div class="popUp">Hyrrokin er en j\u00e6ttekvinde i den nordiske mytologi.</div>\u000a      \u000a      <div class="encyc">Efter Balders d\u00f8d skulle hans lig br\u00e6ndes p\u00e5 Ringhorne, det st\u00f8rste af asernes krigsskibe,\u000a         men selv ikke Thor kunne skubbe skibet fra land. Aserne tilkaldte da Hyrrokin, som\u000a         gjorde det med st\u00f8rste lethed. Thor blev rasende ved at se sig sl\u00e5et i styrke og ville\u000a         dr\u00e6be hende, men p\u00e5 asernes opfordring undlod han at g\u00f8re det. F\u00f8rst da dv\u00e6rgen Lit\u000a         tilf\u00e6ldigvis gik ind foran Thor, kunne han ikke l\u00e6ngere styre sin vrede, men sparkede\u000a         til dv\u00e6rgen, der uheldigvis styrtede ind i b\u00e5let og d\u00f8de.\u000a      </div>\u000a      \u000a   </div>','myth39','myth')
INSERT INTO TEXTREFERENCE VALUES(10701,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.m">\u000a      \u000a      <div class="epithet">H\u00e6rfader\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">H\u00e6rfader er et af Odins tilnavne. Det karakteriserer ham som krigsgud.</div>\u000a      \u000a   </div>','myth59.m','myth')
INSERT INTO TEXTREFERENCE VALUES(10702,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth128">\u000a      \u000a      <div class="normForm">H\u00f8der</div>\u000a      \u000a      <div class="popUp">H\u00f8der er Odins blinde s\u00f8n, der ved Lokes mellemkomst kommer til at skyde og dr\u00e6be\u000a         sin bror, Balder.\u000a      </div>\u000a      \u000a      <span class="orthography">Hother\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">H\u00f8dur</span>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Hotherhat</span>\u000a   </div>','myth128','myth')
INSERT INTO TEXTREFERENCE VALUES(10703,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth40">\u000a      \u000a      <div class="normForm">H\u00f8ner</div>\u000a      \u000a      <div class="popUp">H\u00f8ner er en as i den nordiske mytologi. Han g\u00e5r for at v\u00e6re tavs og m\u00e5ske lettere\u000a         ubegavet.\u000a      </div>\u000a      \u000a      <span class="orthography">H\u00e6ner</span>\u000a      \u000a      <div class="encyc">I \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) giver han menneskeparret Ask og Embla \u00e5nd. Grundtvig opfatter ham efter Suhm (1771,\u000a         s. 221) som meget smuk.\u000a      </div>\u000a      \u000a   </div>','myth40','myth')
INSERT INTO TEXTREFERENCE VALUES(10704,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.j">\u000a      \u000a      <div class="epithet">H\u00e1r\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>    \u000a      \u000a   </div>','myth59.j','myth')
INSERT INTO TEXTREFERENCE VALUES(10705,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth231">\u000a      \u000a      <div class="normForm">H\u00e1rs sale</div>\u000a      \u000a      <div class="popUp">H\u00e1r er et af Odins mange tilnavne i den nordiske mytologi. H\u00e1rs sale er derfor Odins\u000a         borg i Valhal eller blot en kenning (poetisk omskrivning) for hele Valhal.\u000a      </div>\u000a      \u000a      <span class="orthography">Hars sale</span>\u000a      \u000a   </div>','myth231','myth')
INSERT INTO TEXTREFERENCE VALUES(10706,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth93">\u000a      \u000a      <div class="normForm">Idavangen</div>\u000a      \u000a      <div class="popUp">Idavangen er det idealsted, hvor guderne i den nordiske mytologi samles b\u00e5de f\u00f8r og\u000a         efter Ragnarok. Her afholder de ting eller spiller br\u00e6tspil.\u000a      </div>\u000a      \u000a      <span class="orthography">Idasletten,</span>\u000a      \u000a      <span class="orthography">Idavolden\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Idas Marke</span>\u000a      \u000a      \u000a   </div>','myth93','myth')
INSERT INTO TEXTREFERENCE VALUES(10707,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth41">\u000a      \u000a      <div class="normForm">Idun</div>\u000a      \u000a      <div class="popUp">Idun er en asynje i den nordiske mytologi, som ejer og vogter de \u00e6bler, der giver\u000a         guderne evig ungdom. Hun er gift med digterguden Brage.\u000a      </div>\u000a      \u000a      <span class="orthography">Idunna\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ydun</span>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>\u00c6blernes vogterhat</span>\u000a   </div>','myth41','myth')
INSERT INTO TEXTREFERENCE VALUES(10708,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth264">\u000a      \u000a      <div class="normForm">Ifing</div>\u000a      \u000a      <div class="popUp">Floden Ifing er et bundl\u00f8st sv\u00e6lg som skiller asernes omr\u00e5de fra j\u00e6tternes. Navnet\u000a         betyder \u2018isslag\u2018, men det bel\u00e6gges aldrig med is.\u000a      </div>\u000a      \u000a      <div class="encyc">Floden er omtalt i \u000a         <span class="italic">Vaftrudners Sang.</span>  \u000a      </div>\u000a      \u000a   </div>','myth264','myth')
INSERT INTO TEXTREFERENCE VALUES(10709,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth407.a">\u000a      \u000a      <div class="latin">Ingellus\u000a         <span class="sameAs">\u000a            <a href="#myth407"> &gt; Ingjald</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth407.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10710,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth407">\u000a      \u000a      <div class="normForm">Ingjald</div>\u000a      \u000a      <div class="popUp">Ingjald er en nordisk sagnfigur, s\u00f8n af den danske kong Frode. Hos Saxo optr\u00e6der han\u000a         i forbindelse med St\u00e6rkodder, der h\u00e5ner ham, fordi han hellere vil leve det s\u00f8de liv\u000a         med sin hustru end h\u00e6vne sin faders d\u00f8d.\u000a      </div> \u000a      \u000a      <span class="orthography">Inge,</span>\u000a      \u000a      <span class="orthography">Ingeld\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ingild</span>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Ingellushat</span>\u000a   </div>','myth407','myth')
INSERT INTO TEXTREFERENCE VALUES(10711,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth342">\u000a      \u000a      <div class="normForm">Ivald</div>\u000a      \u000a      <div class="popUp">Ivald er en dv\u00e6rg (svartalf) i den nordiske mytologi. Hans s\u00f8nner Brok og Sindre har\u000a         skabt skibet Skidbladner.\u000a      </div>  \u000a      \u000a      <span class="orthography">Ivalde</span>\u000a      \u000a   </div>','myth342','myth')
INSERT INTO TEXTREFERENCE VALUES(10712,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth414">\u000a      \u000a      <div class="normForm">Ivar Benl\u00f8s</div>\u000a      \u000a      <div class="popUp">Ivar var s\u00f8n af Regnar Lodbrog.</div>               \u000a      \u000a      <div class="encyc">Tilnavnet Benl\u00f8s skyldes, at Ivar havde en tyk, brusket hud i stedet for knogler.</div>\u000a      \u000a   </div>','myth414','myth')
INSERT INTO TEXTREFERENCE VALUES(10713,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth244">\u000a      \u000a      <div class="normForm">Jarnsaxa</div>\u000a      \u000a      <div class="popUp">Jarnsaxa er en j\u00e6ttekvinde fra den nordiske mytologi, der sammen med Thor har s\u00f8nnen\u000a         Magni.\u000a      </div>\u000a      \u000a      <span class="orthography">Jarn Saksa,</span>\u000a      \u000a      <span class="orthography">Jarnsakse\u000a         <i> eller </i>\u000a      </span>       \u000a      \u000a      <span class="orthography">Jernsakse</span>\u000a      \u000a      <div class="encyc">Navnet betyder \u2018hende med jernsv\u00e6rdet\u2019.</div>\u000a      \u000a      \u000a   </div>','myth244','myth')
INSERT INTO TEXTREFERENCE VALUES(10714,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth95">\u000a      \u000a      <div class="normForm">Jernsakse</div>\u000a      \u000a      <div class="popUp">Jernsakse er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til\u000a         guden Heimdal.\u000a      </div> \u000a      \u000a      <span class="orthography">Jarn Saksa,</span>\u000a      \u000a      <span class="orthography">Jarnsakse\u000a         <i> eller </i>\u000a      </span>       \u000a      \u000a      <span class="orthography">Jernsakse</span>\u000a      \u000a      <div class="encyc">Navnet betyder \u2018hende med jernsv\u00e6rdet\u2019.</div>\u000a      \u000a   </div>','myth95','myth')
INSERT INTO TEXTREFERENCE VALUES(10715,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth279">\u000a      \u000a      <div class="normForm">Jernskoven</div>\u000a      \u000a      <div class="popUp">Jernskoven er et skummelt sted i den nordiske mytologi, hvor gygen (dvs. en j\u00e6ttekvinde)\u000a         opfostrer ulve.\u000a      </div>\u000a      \u000a      <div class="encyc">Jernskoven n\u00e6vnes i eddadigtet \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom.</span>\u000a      </div>\u000a      \u000a   </div>','myth279','myth')
INSERT INTO TEXTREFERENCE VALUES(10716,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.n">\u000a      \u000a      <div class="epithet">Jolmer\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>   \u000a      \u000a      <div class="popUp">Jolmer eller Jolner er et andet navn for Odin. Under dette navn forbindes han hos\u000a         historiskeren P.F. Suhm (1771, s. 24) is\u00e6r med julen. Nyere forskning g\u00e5r dog ud fra,\u000a         at \u2018Jolner\u2019 blot er et andet ord for \u2018gud\u2019.\u000a      </div> \u000a      \u000a      <span class="orthography">Jolner</span>\u000a      \u000a   </div>','myth59.n','myth')
INSERT INTO TEXTREFERENCE VALUES(10717,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth16.b">\u000a      \u000a      <div class="epithet">Jorden\u000a         <span class="sameAs">\u000a            <a href="#myth16"> &gt; Fj\u00f8rgyn</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth16.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10718,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth105.a">\u000a      \u000a      <div class="epithet">Jordtorn\u000a         <span class="sameAs">\u000a            <a href="#myth105"> &gt; Midg\u00e5rdsormen</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Jordtorn eller Muldtorn er andre navne for Midg\u00e5rdsormen efter oldislandsk \u2018mold\u00feinvr\u2019.\u000a         Hvad ang\u00e5r disse navne, s\u00e5 overs\u00e6ttes det oldislandske ord derfor ofte blot med Midg\u00e5rdsormen.\u000a      </div>\u000a      \u000a   </div>','myth105.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10719,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth105.b">\u000a      \u000a      <div class="epithet">Jormungandr\u000a         <span class="sameAs">\u000a            <a href="#myth105"> &gt; Midg\u00e5rdsormen</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp"> Jormungardr betyder egl. \u2018Jordstaven\u2019 eller \u2018den v\u00e6ldige stav\u2019.</div>\u000a      \u000a      <span class="orthography">Jormungardr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Jordmundgander</span>\u000a      \u000a   </div>','myth105.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10720,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth99.a">\u000a      \u000a      <div class="epithet">Jotunheim\u000a         <span class="sameAs">\u000a            <a href="#myth99"> &gt; Udg\u00e5rd</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">egl. \u2018j\u00e6ttehjem\u2019</div>\u000a      \u000a   </div>','myth99.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10721,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth172.a">\u000a      \u000a      <div class="latin">Juno\u000a         <span class="sameAs">\u000a            <a href="#myth172"> &gt; Hera</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth172.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10722,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth171.a">\u000a      \u000a      <div class="latin">Jupiter\u000a         <span class="sameAs">\u000a            <a href="#myth171"> &gt; Zeus</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">I det antikke Rom havde Jupiter et tempel p\u00e5 Capitol.</div> \u000a      \u000a   </div>','myth171.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10723,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth375">\u000a      \u000a      <div class="normForm">Kar</div>\u000a      \u000a      <div class="popUp">Kar er navnet p\u00e5 sengen hos d\u00f8dsgudinden Hel i den nordiske mytologi. Navnet betyder\u000a         \u2018sygeleje\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Kavr</span>\u000a      \u000a   </div>','myth375','myth')
INSERT INTO TEXTREFERENCE VALUES(10724,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth157">\u000a      \u000a      <div class="normForm">Kerberos</div>\u000a      \u000a      <div class="popUp">Kerberos er den hund, der vogter indgangen til d\u00f8dsriget Hades i den gr\u00e6ske mytologi.\u000a         Den har mange ligheder med Garm fra den nordiske mytologi.\u000a      </div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes hunden Cerberus.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Cerberushat</span>\u000a   </div>','myth157','myth')
INSERT INTO TEXTREFERENCE VALUES(10725,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth151">\u000a      \u000a      <div class="normForm">Komos</div>\u000a      \u000a      <div class="popUp">Komos er en gr\u00e6sk gud for bordets gl\u00e6der og selskabelig munterhed.</div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes han Comus.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Comushat</span>\u000a   </div>','myth151','myth')
INSERT INTO TEXTREFERENCE VALUES(10726,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth43">\u000a      \u000a      <div class="normForm">Kvaser</div>\u000a      \u000a      <div class="popUp">Kvaser er en mand fra den nordiske mytologi. Han er opst\u00e5et ved, at aser og vaner\u000a         ved fredsslutningen spyttede i et kar og dannede Kvaser af spyttet. Senere dr\u00e6btes\u000a         han, og af hans blod blandet med honning opstod skjaldemj\u00f8den.\u000a      </div>\u000a      \u000a      <span class="orthography">Kvasier</span>\u000a      \u000a      <div class="encyc">\u2018Kvasers blod\u2019 er en kenning (poetisk omskrivning) for digterevnen.</div>\u000a      \u000a      <div class="encyc">Kvaser var den, der i asken fandt resterne af det net, som Loke selv havde opfundet,\u000a         men som han br\u00e6ndte for ikke at give andre ideen. Guderne kunne dog godt genneskue\u000a         den tilt\u00e6nkte brug og fangede s\u00e5ledes Loke, da han havde omskabt sig til en laks.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth43','myth')
INSERT INTO TEXTREFERENCE VALUES(10727,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth413">\u000a      \u000a      <div class="normForm">Ladgerd</div>\u000a      \u000a      <div class="popUp">Ladgerd var en krigerisk kvinde og Regnar Lodbrogs f\u00f8rste dronning.</div>\u000a      \u000a      <span class="orthography">Lagertha,</span>\u000a      \u000a      <span class="orthography">Latgerthe\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Lathgertha</span>\u000a      \u000a      <div class="encyc">Lodbrog forlod Ladgerd til fordel for den svenske prinsesse Tore.</div>\u000a      \u000a   </div>','myth413','myth')
INSERT INTO TEXTREFERENCE VALUES(10728,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth44">\u000a      \u000a      <div class="normForm">Laufey</div>\u000a      \u000a      <div class="popUp">Lokes mor hedder enten Laufey eller N\u00e1l og er formentlig af j\u00e6ttesl\u00e6gt.</div>\u000a      \u000a      <span class="orthography">L\u00f8v\u00f8</span>\u000a      \u000a      \u000a      <div class="encyc">Om Lokes sl\u00e6gtskab med Laufey kan man l\u00e6se i \u000a         <span class="italic">Snorres Edda,</span> \u000a         <span class="italic">Gylfaginning</span> (da. \u000a         <span class="italic">Gylfe Forbl\u00e6ndelse</span>), kapitel 32.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>N\u00e1lhat</span>\u000a   </div>','myth44','myth')
INSERT INTO TEXTREFERENCE VALUES(10729,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.e">\u000a      \u000a      <div class="epithet">Laufeys s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth48.e','myth')
INSERT INTO TEXTREFERENCE VALUES(10730,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth353">\u000a      \u000a      <div class="normForm">Lerad</div>\u000a      \u000a      <div class="popUp">Lerad er et tr\u00e6/busk i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth353','myth')
INSERT INTO TEXTREFERENCE VALUES(10731,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth340">\u000a      \u000a      <div class="normForm">Letfod</div>\u000a      \u000a      <div class="popUp">Letfod er navnet p\u00e5 en hest i den nordiske mytologi.</div> \u000a      \u000a      <span class="orthography">Letfete</span>\u000a      \u000a   </div>','myth340','myth')
INSERT INTO TEXTREFERENCE VALUES(10732,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth47">\u000a      \u000a      <div class="normForm">Lit</div>\u000a      \u000a      <div class="popUp">Lit er en dv\u00e6rg i den nordiske mytologi.</div>\u000a      \u000a      <div class="encyc">Efter Balders d\u00f8d skulle hans lig br\u00e6ndes p\u00e5 Ringhorne, det st\u00f8rste af asernes krigsskibe,\u000a         men selv ikke Thor kunne skubbe skibet fra land. Aserne tilkaldte da den st\u00e6rke j\u00e6ttekvinde\u000a         Hyrrokin, som gjorde det med st\u00f8rste lethed. Thor blev rasende ved at se sig sl\u00e5et\u000a         i styrke og ville dr\u00e6be hende, men p\u00e5 asernes opfordring undlod han at g\u00f8re det. F\u00f8rst\u000a         da Lit tilf\u00e6ldigvis gik ind foran Thor, kunne han ikke l\u00e6ngere styre sin vrede, men\u000a         sparkede til dv\u00e6rgen, der uheldigvis styrtede ind i b\u00e5let og d\u00f8de.\u000a      </div>\u000a      \u000a   </div>','myth47','myth')
INSERT INTO TEXTREFERENCE VALUES(10733,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth45">\u000a      \u000a      <div class="normForm">Liv</div>\u000a      \u000a      <div class="popUp">Liv er den kvindelige halvdel af det menneskepar, der overlever Fimbulvinteren i den\u000a         nordiske mytologi. Livtraser er den mandlige.\u000a      </div>\u000a      \u000a      <span class="orthography">Lif</span>\u000a      \u000a      \u000a   </div>','myth45','myth')
INSERT INTO TEXTREFERENCE VALUES(10734,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth46">\u000a      \u000a      <div class="normForm">Livtraser</div>\u000a      \u000a      <div class="popUp">Livtraser er den mandlige halvdel af det menneskepar, der overlever Fimbulvinterenden\u000a         i den nordiske mytologi. Liv er den kvindelige.\u000a      </div>\u000a      \u000a      <span class="orthography">Lifthrase,</span>\u000a      \u000a      <span class="orthography">Lifthrasir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Livtrase</span>\u000a      \u000a      \u000a   </div>','myth46','myth')
INSERT INTO TEXTREFERENCE VALUES(10735,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth49">\u000a      \u000a      <div class="normForm">Lodur</div>\u000a      \u000a      <div class="popUp">Lodur er en af de tre guder i den nordiske mytologi, der er med til at skabe det f\u00f8rste\u000a         menneskepar, Ask og Embla.\u000a      </div>\u000a      \u000a      <span class="orthography">L\u00f8dur,</span>\u000a      \u000a      <span class="orthography">Lo\u00f0urr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">L\u00e6dur</span>\u000a      \u000a      <div class="encyc">En tolkning af Lodur som enten Loke eller Frej er meget usikker.</div>\u000a      \u000a   </div>','myth49','myth')
INSERT INTO TEXTREFERENCE VALUES(10736,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth212">\u000a      \u000a      <div class="normForm">Lofn</div>\u000a      \u000a      <div class="popUp">Lofn er en asynje, dvs. gudinde, fra den nordiske mytologi. Hun er is\u00e6r knyttet til\u000a         vanskelig k\u00e6rlighed og opfattes som tr\u00f8stende.\u000a      </div>\u000a      \u000a   </div>','myth212','myth')
INSERT INTO TEXTREFERENCE VALUES(10737,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48">\u000a      \u000a      <div class="normForm">Loke</div>\u000a      \u000a      <div class="popUp">Loke er den mest tvetydige figur i den nordiske mytologi. Sk\u00f8nt af j\u00e6ttesl\u00e6gt, s\u00f8n\u000a         af Farbaute og Laufey (eller N\u00e1l), omg\u00e5s han aserne og redder dem ofte, nogle gange\u000a         endda fra ulykker, han selv har for\u00e5rsaget.\u000a      </div>\u000a      \u000a      <span class="orthography">Loki</span>\u000a      \u000a      <div class="encyc">Lokes st\u00f8rste br\u00f8de er, at han er medskyldig i  Balders d\u00f8d.</div>\u000a      \u000a      <div class="encyc">Han kan n\u00e6ppe betragtes som en gud, og der eksisterer heller ikke spor af kult i tilknytning\u000a         til ham. Ved Ragnarok er han utvetydigt p\u00e5 j\u00e6tternes side.\u000a      </div>\u000a      \u000a      <div class="encyc">Med Angrbode har Loke b\u00f8rnene Fenrisulven, Midg\u00e5rdsormen og Hel; med Sigyn har han\u000a         s\u00f8nnerne Vale og Narfe.\u000a      </div>  \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Asalokehat</span>\u000a      <span>Balders baner\u00e5d, </span>\u000a      <span>Farbautes s\u00f8n, </span>\u000a      <span>Laufeys s\u00f8n, </span>\u000a      <span>Lopt, </span>\u000a      <span>Nals s\u00f8n eller </span>\u000a      <span>Sleipners fr\u00e6nde.</span>\u000a   </div>','myth48','myth')
INSERT INTO TEXTREFERENCE VALUES(10738,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.c">\u000a      \u000a      <div class="epithet">Lopt\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Loptr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Loptur</span>\u000a      \u000a   </div>','myth48.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10739,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth259">\u000a      \u000a      <div class="normForm">L\u00f8ding</div>\u000a      \u000a      <div class="popUp">L\u00f8ding er navnet p\u00e5 den f\u00f8rste af de tre l\u00e6nker, som aserne fors\u00f8gte at binde Fenrisulven\u000a         med. F\u00f8rst med Gleipner lykkedes det.\u000a      </div>\u000a      \u000a      <span class="orthography">L\u00e6ding</span>\u000a      \u000a      <div class="encyc">De to andre l\u00e6nker hedder Drome og Gleipner.</div>\u000a      \u000a   </div>','myth259','myth')
INSERT INTO TEXTREFERENCE VALUES(10740,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth197">\u000a      \u000a      <div class="normForm">Magni</div>\u000a      \u000a      <div class="popUp">Magni er en halvgud fra den nordiske mytologi. Han er s\u00f8n af Thor og j\u00e6ttekvinden\u000a         Jarnsaxa og kendt for sin store styrke. Navnet betyder da ogs\u00e5 \u2018den st\u00e6rke\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Magne</span>\u000a      \u000a   </div>','myth197','myth')
INSERT INTO TEXTREFERENCE VALUES(10741,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth70">\u000a      \u000a      <div class="normForm">Megingjord</div>\u000a      \u000a      <div class="popUp">Megingjord er navnet p\u00e5 Thors styrkeb\u00e6lte. </div>\u000a      \u000a      <span class="orthography">Megingjardur</span>\u000a      \u000a      <div class="encyc">Gjord betyder \u2018livrem\u2019.</div>\u000a      \u000a   </div>','myth70','myth')
INSERT INTO TEXTREFERENCE VALUES(10742,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth200">\u000a      \u000a      <div class="normForm">Meile</div>\u000a      \u000a      <div class="popUp">Meile er en af Thors br\u00f8dre i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth200','myth')
INSERT INTO TEXTREFERENCE VALUES(10743,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth153">\u000a      \u000a      <div class="normForm">Mengl\u00f8d</div>\u000a      \u000a      <div class="popUp">Mengl\u00f8d er den st\u00e6rkt bevogtede jomfru, som helten Svipdag s\u00f8ger efter og pr\u00f8ver at\u000a         befri (nordisk mytologi).\u000a      </div> \u000a      \u000a      <div class="encyc">Mengl\u00f8d optr\u00e6der i \u000a         <span class="italic">Svipdagskvadet</span>. Hun er i senere forskning blevet identificeret med Freja.\u000a      </div>           \u000a      \u000a   </div>','myth153','myth')
INSERT INTO TEXTREFERENCE VALUES(10744,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth94">\u000a      \u000a      <div class="normForm">Midg\u00e5rd</div>\u000a      \u000a      <div class="popUp">Midg\u00e5rd er menneskenes hjem eller bolig i den nordiske mytologi. Det ligger midt i\u000a         verden, deraf navnet, og er omkranset af j\u00e6tternes hjem, Udg\u00e5rd.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth94','myth')
INSERT INTO TEXTREFERENCE VALUES(10745,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth105">\u000a      \u000a      <div class="normForm">Midg\u00e5rdsormen</div>\u000a      \u000a      <div class="popUp">Midg\u00e5rdsormen er et af Lokes b\u00f8rn. Den slynger sig rundt om hele jorden og er en fjende\u000a         af mennesker og guder.\u000a      </div>\u000a      \u000a      <span class="orthography">Midgarsorm\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ormen</span>\u000a      \u000a      <div class="encyc">Den har en r\u00e6kke andre navne.</div>\u000a      \u000a      <div class="encyc">\u000a         <span class="italic">Fritzners Ordbog</span> (1867) giver dog ogs\u00e5 mulighed for, at \u2018mold\u00feinvr\u2019 kan v\u00e6re verdenstr\u00e6et Yggdrasil.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Jordtornhat</span>\u000a      <span>Jormungandr eller </span>\u000a      <span>Muldtorn.</span>\u000a   </div>','myth105','myth')
INSERT INTO TEXTREFERENCE VALUES(10746,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth103.a">\u000a      \u000a      <div class="epithet">Mimamejd\u000a         <span class="sameAs">\u000a            <a href="#myth103"> &gt; Yggdragsil</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Mimamejd er i den nordiske mytologi det store asketr\u00e6, der b\u00e6rer verden og g\u00e5r gennem\u000a         himmel, jord og underverden.\u000a      </div>\u000a      \u000a      <span class="orthography">Mimamejds Quiste\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Mimers tr\u00e6</span>\u000a      \u000a   </div>','myth103.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10747,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth50">\u000a      \u000a      <div class="normForm">Mimer</div>\u000a      \u000a      <div class="popUp">Mimer er en j\u00e6tte i den nordiske mytologi, som vogter over visdommens kilde. Odin\u000a         s\u00e6tter sit ene \u00f8je i pant for en drik af kilden.\u000a      </div>\u000a      \u000a      <span class="orthography">Mymer,</span>\u000a      \u000a      <span class="orthography">Mir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Mimring</span>\u000a      \u000a      <div class="encyc">P\u00e5 et tidspunkt halshugger vanerne Mimer og sender hans hoved til aserne. Odin pr\u00e6parerer\u000a         det med urter, s\u00e5 han kan tale med det og dermed fortsat kan nyde godt af Mimers visdom.\u000a         Odin sp\u00f8rger ofte hovedet til r\u00e5ds.\u000a      </div>\u000a      \u000a   </div>','myth50','myth')
INSERT INTO TEXTREFERENCE VALUES(10748,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth96">\u000a      \u000a      <div class="normForm">Mimers br\u00f8nd</div>\u000a      \u000a      <div class="popUp">Mimers br\u00f8nd er navnet p\u00e5 en kilde, der springer ved Yggdrasils r\u00f8dder, ejet af j\u00e6tten\u000a         Mimer. Vandet er fyldt med visdom; og Odin har givet sit ene \u00f8je i pant for en drik\u000a         af kilden.\u000a      </div>\u000a      \u000a      <span class="orthography">Mimers kilde\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Mimers vande</span>\u000a      \u000a      \u000a   </div>','myth96','myth')
INSERT INTO TEXTREFERENCE VALUES(10749,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth355">\u000a      \u000a      <div class="normForm">Mist</div>\u000a      \u000a      <div class="popUp">Mist er en ung opvarterske i den nordiske mytologi. Hun b\u00e6rer drikkehorn til Odin.</div>\u000a      \u000a   </div>','myth355','myth')
INSERT INTO TEXTREFERENCE VALUES(10750,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth51">\u000a      \u000a      <div class="normForm">Mistelten</div>\u000a      \u000a      <div class="popUp">Mistelten er navnet p\u00e5 en b\u00f8jelig snylteplante. I nordisk mytologi var misteltenen\u000a         det eneste i naturen, der ikke havde afgivet l\u00f8fte om ikke at skade Balder.\u000a      </div>\u000a      \u000a      <div class="encyc"> Loke kunne derfor lave en pil af misteltenen; og denne pil dr\u00e6bte Balder, idet Loke\u000a         hjalp den blinde H\u00f8der med at affyre pilen.\u000a      </div>\u000a      \u000a   </div>','myth51','myth')
INSERT INTO TEXTREFERENCE VALUES(10751,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth52">\u000a      \u000a      <div class="normForm">Mj\u00f8lner</div>\u000a      \u000a      <div class="popUp">Mj\u00f8lner er navnet p\u00e5 Thors magiske hammer, der kommer tilbage til den, som har kastet\u000a         den (nordisk mytologi).\u000a      </div>\u000a      \u000a      <span class="orthography">Mj\u01ebllnir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Mi\u01eblner</span>\u000a      \u000a      \u000a   </div>','myth52','myth')
INSERT INTO TEXTREFERENCE VALUES(10752,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth376">\u000a      \u000a      <div class="normForm">Modgudur</div>\u000a      \u000a      <div class="popUp">Modgudur er en ung pige eller valkyrie fra den nordiske mytologi. Hun vogter broen\u000a         til Hel. Navnet betyder \u2018vred kamp.\u2019\u000a      </div>  \u000a      \u000a   </div>','myth376','myth')
INSERT INTO TEXTREFERENCE VALUES(10753,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth198">\u000a      \u000a      <div class="normForm">Modi</div>\u000a      \u000a      <div class="popUp">Modi er en halvgud fra den nordiske mytologi. Han er s\u00f8n af Thor og j\u00e6ttekvinden Jarnsaxa.\u000a         Navnet betyder \u2018den vrede\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Mode</span>\u000a      \u000a   </div>','myth198','myth')
INSERT INTO TEXTREFERENCE VALUES(10754,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth307">\u000a      \u000a      <div class="normForm">Modsogner</div>\u000a      \u000a      <div class="popUp">Modsogner er en af dv\u00e6rgerne i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Modsagner,</span>\u000a      \u000a      <span class="orthography">Motsognir,</span>\u000a      \u000a      <span class="orthography">M\u00f3tsognir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">M\u00f3\u00f0sognir</span>\u000a      \u000a      <div class="encyc">Modsogner n\u00e6vnes b\u00e5de i \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) og i \u000a         <span class="italic">Gylfes Forbl\u00e6ndelse.</span>\u000a      </div>\u000a      \u000a   </div>','myth307','myth')
INSERT INTO TEXTREFERENCE VALUES(10755,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth280">\u000a      \u000a      <div class="normForm">Moin</div>\u000a      \u000a      <div class="popUp">Moin er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">M\u00f3inn</span>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger i kilden Hvergelmer.</div>\u000a      \u000a   </div>','myth280','myth')
INSERT INTO TEXTREFERENCE VALUES(10756,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth105.c">\u000a      \u000a      <div class="epithet">Muldtorn\u000a         <span class="sameAs">\u000a            <a href="#myth105"> &gt; Midg\u00e5rdsormen</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Muldtorn eller Jordtorn er andre navne for Midg\u00e5rdsormen efter oldislandsk \u2018mold\u00feinvr\u2019.\u000a         Hvad ang\u00e5r disse navne, s\u00e5 overs\u00e6ttes det oldislandske ord derfor ofte blot med Midg\u00e5rdsormen.\u000a      </div>\u000a      \u000a   </div>','myth105.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10757,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth220">\u000a      \u000a      <div class="normForm">Mundilfare</div>\u000a      \u000a      <div class="popUp">Mundilfare er en skikkelse fra den nordiske mytologi. Han er far til Sol og M\u00e5ne.</div>\u000a      \u000a      <span class="orthography">Mundilfar\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Mundilf\u00f8r</span>\u000a      \u000a      <div class="encyc">Egentlig havde Mundilfare blot opkaldt sine b\u00f8rn efter solen og m\u00e5nen, men guderne\u000a         straffede ham for dette hovmod ved at s\u00e6tte b\u00f8rnene til at k\u00f8re henholdsvis solens\u000a         og m\u00e5nens vogne hen over himlen.\u000a      </div>\u000a      \u000a   </div>','myth220','myth')
INSERT INTO TEXTREFERENCE VALUES(10758,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth313">\u000a      \u000a      <div class="normForm">Munin</div>\u000a      \u000a      <div class="popUp">Munin er den ene af Odins to ravne i den nordiske mytologi. Navnet betyder \u2018erindringen\u2019.</div>\u000a      \u000a      <div class="encyc">Ravnene, Hugin og Munin, sidder p\u00e5 Odins skuldre og fort\u00e6ller Odin alt, hvad de h\u00f8rer\u000a         og ser ske i verden.\u000a      </div>\u000a      \u000a   </div>','myth313','myth')
INSERT INTO TEXTREFERENCE VALUES(10759,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth53">\u000a      \u000a      <div class="normForm">Muspel</div>\u000a      \u000a      <div class="popUp">Muspel er anf\u00f8rer af (ild)j\u00e6tterne ved Ragnarok i den nordiske mytologi. Navnet betyder\u000a         m\u00e5ske \u2018verdensbrand\u2019. Hans f\u00f8lge af j\u00e6tter betegnes ofte som muspels\u00f8nnerne.\u000a      </div>\u000a      \u000a      \u000a      \u000a   </div>','myth53','myth')
INSERT INTO TEXTREFERENCE VALUES(10760,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth304">\u000a      \u000a      <div class="normForm">Muspelheim</div>\u000a      \u000a      <div class="popUp">Muspelheim er ildens verden i den nordiske mytologi. Ildj\u00e6tten Surt omtales som den,\u000a         der styrer Muspelheim og muspels\u00f8nnerne. Ved Ragnarok kommer Surt sejlende fra Muspelheim.\u000a      </div>\u000a      \u000a   </div>','myth304','myth')
INSERT INTO TEXTREFERENCE VALUES(10761,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth158">\u000a      \u000a      <div class="normForm">M\u00f8rkeskoven</div>\u000a      \u000a      <div class="popUp">M\u00f8rkeskoven er den skov, som Muspels s\u00f8nner, dvs. Muspels f\u00f8lge, rider igennem ved\u000a         Ragnarok.\u000a      </div>\u000a      \u000a      <span class="orthography">My\u00f8rkvi\u00f0r,</span>\u000a      \u000a      <span class="orthography">M\u00f8rkved\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">M\u00f8rkvid</span>\u000a      \u000a   </div>','myth158','myth')
INSERT INTO TEXTREFERENCE VALUES(10762,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth97.a">\u000a      \u000a      <div class="epithet">M\u00f8rkesletten\u000a         <span class="sameAs">\u000a            <a href="#myth97"> &gt; Nidavolden</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth97.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10763,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth55">\u000a      \u000a      <div class="normForm">M\u00e5negarm</div>\u000a      \u000a      <div class="popUp">M\u00e5negarm er en ulv, der under Ragnarok sluger solen.</div>      \u000a      \u000a   </div>','myth55','myth')
INSERT INTO TEXTREFERENCE VALUES(10764,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth54">\u000a      \u000a      <div class="normForm">Naglfar</div>\u000a      \u000a      <div class="popUp">Naglfar er navnet p\u00e5 et skib fra den nordiske mytologi, bygget af d\u00f8de m\u00e6nds negle.\u000a         Det slippes l\u00f8s ved Ragnarok.\u000a      </div>\u000a      \u000a      <span class="orthography">Naglfare\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Naglfari</span>\u000a      \u000a      \u000a   </div>','myth54','myth')
INSERT INTO TEXTREFERENCE VALUES(10765,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth143">\u000a      \u000a      <div class="normForm">Nagrind</div>\u000a      \u000a      <div class="popUp">Nagrind betyder egentlig \u2018d\u00f8dshegn\u2019 og er det g\u00e6rde, som i den nordiske mytologi omgiver\u000a         underverdenen Hel.\u000a      </div>\u000a      \u000a   </div>','myth143','myth')
INSERT INTO TEXTREFERENCE VALUES(10766,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.d">\u000a      \u000a      <div class="epithet">Nals s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth48.d','myth')
INSERT INTO TEXTREFERENCE VALUES(10767,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth56">\u000a      \u000a      <div class="normForm">Nanna</div>\u000a      \u000a      <div class="popUp">Nanna er den nordiske gud Balders elskede hustru, datter af Nef. Hun f\u00f8lger Balder\u000a         p\u00e5 ligb\u00e5let, da han er d\u00f8d.\u000a      </div>\u000a      \u000a      <span class="orthography">Nanne</span>\u000a      \u000a      <div class="encyc">I \u000a         <span class="italic">Snorres Edda</span> h\u00f8rer Nanna til aserne, mens hun hos Saxo er et menneske.\u000a      </div>\u000a      \u000a      <div class="encyc">Navnet Nanna optr\u00e6der dog ogs\u00e5 andre steder i de oldislandske kilder, hvor der ikke\u000a         er nogen sammenh\u00e6ng med Baldermyten.\u000a      </div>\u000a      \u000a   </div>','myth56','myth')
INSERT INTO TEXTREFERENCE VALUES(10768,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth269.a">\u000a      \u000a      <div class="epithet">Nar\u000a         <span class="sameAs">\u000a            <a href="#myth269"> &gt; Vale</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Nare</span>\u000a      \u000a   </div>','myth269.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10769,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth127">\u000a      \u000a      <div class="normForm">Narfe</div>\u000a      \u000a      <div class="popUp">Narfe er s\u00f8n af Loke og Sigyn i den nordiske mytologi.</div>\u000a      \u000a      \u000a   </div>','myth127','myth')
INSERT INTO TEXTREFERENCE VALUES(10770,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth235">\u000a      \u000a      <div class="normForm">Nastrand</div>\u000a      \u000a      <div class="popUp">Nastrand er en del af d\u00f8dsriget Hel i den nordiske mytologi. I Nastrand pines de d\u00f8de,\u000a         is\u00e6r med slangegift. Ordet betyder \u2018ligstrand\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Nastrond</span>\u000a      \u000a   </div>','myth235','myth')
INSERT INTO TEXTREFERENCE VALUES(10771,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth221.a">\u000a      \u000a      <div class="latin">Neptun\u000a         <span class="sameAs">\u000a            <a href="#myth221"> &gt; Poseidon</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth221.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10772,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth97">\u000a      \u000a      <div class="normForm">Nidavolden</div>\u000a      \u000a      <div class="popUp">Nidavolden er et fjeld i den nordiske mytologi, evt. en h\u00f8jslette ved Idabjerget.</div>\u000a      \u000a      <span class="orthography">Nida,</span>\u000a      \u000a      <span class="orthography">Nidav\u01ebllum\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Nidefjelde</span> \u000a      \u000a      <div class="encyc">I en moderne, oldislandsk udgave af \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) finder man i 0 \u201cNidav\u01ebllum"; i den arnamagn\u00e6anske kommisions udgave (1787): \u201cNIDA\u000a         fiollom".\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>M\u00f8rkeslettenhat</span>\u000a      <span>N\u00e6sletten.</span>\u000a   </div>','myth97','myth')
INSERT INTO TEXTREFERENCE VALUES(10773,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth57">\u000a      \u000a      <div class="normForm">Nidhug</div>\u000a      \u000a      <div class="popUp">Nidhug er en d\u00f8dsdrage i den nordiske mytologi, der dukker op ved Ragnarok, hvor den\u000a         ved indgangen til Hel suger de d\u00f8des blod. F\u00f8rste del af navnet betyder nid (h\u00e5n);\u000a         anden del \u2018den, som hugger\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Nidh\u00f8g,</span>\u000a      \u000a      <span class="orthography">Nidh\u00f8ge\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ni\u00f0h\u00f6ggr</span>\u000a      \u000a      \u000a   </div>','myth57','myth')
INSERT INTO TEXTREFERENCE VALUES(10774,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth227">\u000a      \u000a      <div class="normForm">Niflheim</div>\u000a      \u000a      <div class="popUp">I den nordiske mytologi er det kolde og t\u00e5gede Niflheim i nord mods\u00e6tningen til det\u000a         gl\u00f8dende varme Muspelheim i syd. Niflheim kan ogs\u00e5 v\u00e6re den dybeste del af d\u00f8dsriget,\u000a         Hel.\u000a      </div>\u000a      \u000a      <span class="orthography">Niffelhejm\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Niffelhjem</span>\u000a      \u000a   </div>','myth227','myth')
INSERT INTO TEXTREFERENCE VALUES(10775,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth369">\u000a      \u000a      <div class="normForm">Niflhel</div>\u000a      \u000a      <div class="popUp">Niflhel er den m\u00f8rkeste del af d\u00f8dsriget Hel i den nordiske mytologi, den niende verden\u000a         under jorden.\u000a      </div>\u000a      \u000a      <div class="encyc">Stedet kan opfattes som identisk med Niflheim.</div>\u000a      \u000a   </div>','myth369','myth')
INSERT INTO TEXTREFERENCE VALUES(10776,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth389">\u000a      \u000a      <div class="normForm">Nitbjerg</div>\u000a      \u000a      <div class="popUp">Nitbjerg er navnet p\u00e5 det bjerg, hvori j\u00e6tten Suttung lader sin datter Gunl\u00f8de vogte\u000a         digtermj\u00f8den.\u000a      </div>\u000a      \u000a      <span class="orthography">Hnitbj\u00f8rg\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Hvytbj\u00f8rg</span>\u000a      \u000a   </div>','myth389','myth')
INSERT INTO TEXTREFERENCE VALUES(10777,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth58">\u000a      \u000a      <div class="normForm">Njord</div>\u000a      \u000a      <div class="popUp">Njord er en frugtbarhedsgud, en af vanerne i den nordiske mytologi. Han er fader til\u000a         Frej og Freja.\u000a      </div>\u000a      \u000a      <span class="orthography">Njardur,</span>\u000a      \u000a      <span class="orthography">Niord\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Nj\u01eb\u00f0r</span>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Fegjafagudhat</span>\u000a      <span>Vananidr.</span>\u000a   </div>','myth58','myth')
INSERT INTO TEXTREFERENCE VALUES(10778,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth325">\u000a      \u000a      <div class="normForm">Noatun</div>\u000a      \u000a      <div class="popUp">Noatun er navnet p\u00e5 Njords bolig i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth325','myth')
INSERT INTO TEXTREFERENCE VALUES(10779,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth311">\u000a      \u000a      <div class="normForm">Nord</div>\u000a      \u000a      <div class="popUp">Nord er en af de fire dv\u00e6rge i den nordiske mytologi, der b\u00e6rer himmelhv\u00e6lvingen.</div>\u000a      \u000a   </div>','myth311','myth')
INSERT INTO TEXTREFERENCE VALUES(10780,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth97.b">\u000a      \u000a      <div class="epithet">N\u00e6sletten\u000a         <span class="sameAs">\u000a            <a href="#myth97"> &gt; Nidavolden</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth97.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10781,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth306">\u000a      \u000a      <div class="normForm">N\u00f8rr</div>\u000a      \u000a      <div class="popUp">N\u00f8rr er far til natten i den nordiske mytologi. Navnet betyder \u2018smal\u2019 eller \u2018tynd\u2019.</div>\u000a      \u000a      <span class="orthography">Narfi,</span>\u000a      \u000a      <span class="orthography">Njorve\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">N\u00f8rfi</span>\u000a      \u000a   </div>','myth306','myth')
INSERT INTO TEXTREFERENCE VALUES(10782,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth44.a">\u000a      \u000a      <div class="epithet">N\u00e1l\u000a         <span class="sameAs">\u000a            <a href="#myth44"> &gt; Laufey</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">N\u00e1l er et andet navn for Laufey. Hun er mor til Loke i den nordiske mytologi.</div>\u000a   </div>','myth44.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10783,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth61">\u000a      \u000a      <div class="normForm">Od</div>\u000a      \u000a      <div class="popUp">Od er Frejas h\u00f8jt elskede og indimellem h\u00e5rdt savnede \u00e6gtef\u00e6lle i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">\u00d3\u00f0r</span>\u000a      \u000a      <div class="encyc">Freja gr\u00e6der guldt\u00e5rer, n\u00e5r hun s\u00f8ger efer Od.</div>\u000a      \u000a      <div class="encyc">Forskningen ser indimellem parret Od-Freja som en udspaltning af parret Odin-Frigg.</div>   \u000a      \u000a   </div>','myth61','myth')
INSERT INTO TEXTREFERENCE VALUES(10784,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth386">\u000a      \u000a      <div class="normForm">Oddbj\u00f8rg</div>\u000a      \u000a      <div class="popUp">Oddbj\u00f8rg er en sp\u00e5domskyndig v\u00f8lve fra den islandske sagalitteratur.</div>\u000a      \u000a      <div class="encyc">Man kan l\u00e6se om Oddbj\u00f8rg og Saldis i \u000a         <span class="italic">Viga-Glums Saga.</span>\u000a      </div>    \u000a      \u000a   </div>','myth386','myth')
INSERT INTO TEXTREFERENCE VALUES(10785,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth1.a">\u000a      \u000a      <div class="pseudoEpithet">Odin\u000a         <span class="sameAs">\u000a            <a href="#myth1"> &gt; Alfader</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth1.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10786,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59">\u000a      \u000a      <div class="normForm">Odin</div>\u000a      \u000a      <div class="popUp">Odin er asernes overhoved i den nordiske mytologi. Han er gud for bl.a. visdom, krigslykke,\u000a         skjaldekunst, runer og magi. Han er s\u00f8n af Borr (af Grundtvig ogs\u00e5 kaldet Bure eller\u000a         B\u00f8r) og Bestla.\u000a      </div>\u000a      \u000a      <span class="orthography">\u00d3\u00f0inn\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00d3dinn</span>\u000a      \u000a      \u000a      <div class="encyc"> Hans s\u00f8nner er f\u00f8rst og fremmest Thor, Balder, Vidar og Vale. Dertil kommer Heimdal,\u000a         H\u00f8der og Brage, ligesom Tyr, Nef og Hermod regnes til s\u00f8nnerne. Endelig n\u00e6vnes ogs\u00e5\u000a         kongerne Vegdeg, Sigi, Skjold, S\u00e6mingr og Yngve som hans s\u00f8nner.\u000a      </div>\u000a      \u000a      <div class="encyc">Odin har mange tilnavne.</div>         \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>hat</span>\u000a      <span>Alvider, </span>\u000a      <span>Borrs s\u00f8n, </span>\u000a      <span>B\u00f8rs s\u00f8n, </span>\u000a      <span>Fimbultyr, </span>\u000a      <span>Fj\u00f8lner, </span>\u000a      <span>Fj\u00f8lsvid, </span>\u000a      <span>Gagnrad, </span>\u000a      <span>Grimner, </span>\u000a      <span>H\u00e1r, </span>\u000a      <span>Herjan, </span>\u000a      <span>Hropt, </span>\u000a      <span>H\u00e6rfader, </span>\u000a      <span>Jolmer, </span>\u000a      <span>Sejrfader, </span>\u000a      <span>Svipdag, </span>\u000a      <span>Thund, </span>\u000a      <span>Valfader, </span>\u000a      <span>Vegtam, </span>\u000a      <span>Asodin eller </span>\u000a      <span>Yggur.</span>\u000a   </div>','myth59','myth')
INSERT INTO TEXTREFERENCE VALUES(10787,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth117.b">\u000a      \u000a      <div class="epithet">Odins M\u00f8er\u000a         <span class="sameAs">\u000a            <a href="#myth117"> &gt; valkyrie</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth117.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10788,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth284">\u000a      \u000a      <div class="normForm">Ofnir</div>\u000a      \u000a      <div class="popUp">Ofnir er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">Ovnir</span>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger i kilden Hvergelmer.</div>\u000a      \u000a   </div>','myth284','myth')
INSERT INTO TEXTREFERENCE VALUES(10789,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth60">\u000a      \u000a      <div class="normForm">Olympen</div>\u000a      \u000a      <div class="popUp">Olympen er navnet p\u00e5 gudernes bolig i den gr\u00e6ske mytologi.</div>\u000a      \u000a      <span class="orthography">Olymp\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Olympos</span>\u000a      \u000a      <div class="encyc">Trods det mytologiske indhold kan Olympen geografisk placeres i bjergomr\u00e5det mellem\u000a         Thessalien og Makedonien i det nordlige Gr\u00e6kenland.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Olympushat</span>\u000a   </div>','myth60','myth')
INSERT INTO TEXTREFERENCE VALUES(10790,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth60.a">\u000a      \u000a      <div class="latin">Olympus\u000a         <span class="sameAs">\u000a            <a href="#myth60"> &gt; Olympen</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">I romersk mytologi er navnet p\u00e5 gudernes bolig Olympus, men den geografiske placering\u000a         er den samme som i den gr\u00e6ske.\u000a      </div>\u000a      \u000a   </div>','myth60.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10791,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth189.a">\u000a      \u000a      <div class="latin">Ongendus\u000a         <span class="sameAs">\u000a            <a href="#myth189"> &gt; Angantyr</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth189.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10792,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth156.a">\u000a      \u000a      <div class="latin">Orcus\u000a         <span class="sameAs">\u000a            <a href="#myth156"> &gt; Hades</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth156.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10793,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth149">\u000a      \u000a      <div class="normForm">Orestes</div>\u000a      \u000a      <div class="popUp">Orestes er en helt fra den gr\u00e6ske sagnhistorie. Han er s\u00f8n af kong Agamemnon og h\u00e6vner\u000a         drabet p\u00e5 sin far ved at dr\u00e6be faderens morder, Aigisthos.\u000a      </div>\u000a      \u000a   </div>','myth149','myth')
INSERT INTO TEXTREFERENCE VALUES(10794,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth182">\u000a      \u000a      <div class="normForm">Orion</div>\u000a      \u000a      <div class="popUp">Orion var en helt fra den gr\u00e6ske mytologi. Han var en j\u00e6ger, der blev dr\u00e6bt af jagtgudinden\u000a         Artemis.\u000a      </div>\u000a      \u000a      <div class="encyc">P\u00e5 stjernehimlen har Orion givet navn til \u2018Orions b\u00e6lte\u2019.</div>\u000a      \u000a   </div>','myth182','myth')
INSERT INTO TEXTREFERENCE VALUES(10795,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth201">\u000a      \u000a      <div class="normForm">Ottar</div>\u000a      \u000a      <div class="popUp">I et eddadigt fort\u00e6lles, hvordan Hyndla hjalp Freja med at udrede sl\u00e6gtskabsforholdene\u000a         for Ottar. Han er ellers ukendt i den nordiske mytologi.\u000a      </div>\u000a      \u000a      <span class="orthography">Otar,</span>\u000a      \u000a      <span class="orthography">Othar,</span>\u000a      \u000a      <span class="orthography">\u00d3ttarr\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Otter</span>\u000a      \u000a      <div class="encyc">Hyndla, Freja og Ottar optr\u00e6der i eddadigtet \u000a         <span class="italic">Hyndlas Sang</span>.\u000a      </div>\u000a      \u000a   </div>','myth201','myth')
INSERT INTO TEXTREFERENCE VALUES(10796,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth19.c">\u000a      \u000a      <div class="epithet">Ottars hustru\u000a         <span class="sameAs">\u000a            <a href="#myth19"> &gt; Freja</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Ottars kone</span>\u000a      \u000a   </div>','myth19.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10797,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth378">\u000a      \u000a      <div class="normForm">Persefone</div>\u000a      \u000a      <div class="popUp">Persefone er underverdenens dronning i den gr\u00e6ske mytologi.</div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes hun Proserpina.</div>               \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Proserpinahat</span>\u000a   </div>','myth378','myth')
INSERT INTO TEXTREFERENCE VALUES(10798,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth156.b">\u000a      \u000a      <div class="epithet">Pluton\u000a         <span class="sameAs">\u000a            <a href="#myth156"> &gt; Hades</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Pluton er et tilnavn til d\u00f8dsguden Hades, som betyder \u2018den rige\u2019.</div>\u000a      \u000a      <span class="orthography">Pluto,</span>\u000a      \u000a      <span class="orthography">Plutos\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Plutus</span>\u000a      \u000a   </div>','myth156.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10799,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth221">\u000a      \u000a      <div class="normForm">Poseidon</div>\u000a      \u000a      <div class="popUp">Poseidon er en gr\u00e6sk havgud, s\u00f8n af Rheia og Kronos. Hans vrede g\u00e5r ofte ud over mennesker.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Neptunhat</span>\u000a   </div>','myth221','myth')
INSERT INTO TEXTREFERENCE VALUES(10800,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth378.a">\u000a      \u000a      <div class="latin">Proserpina\u000a         <span class="sameAs">\u000a            <a href="#myth378"> &gt; Persefone</a>\u000a         </span>\u000a      </div>\u000a      \u000a      \u000a   </div>','myth378.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10801,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth148">\u000a      \u000a      <div class="normForm">Pylades</div>\u000a      \u000a      <div class="popUp">Pylades er en helt fra den gr\u00e6ske sagnhistorie. Han er ven med Orestes og s\u00f8n af kong\u000a         Strofios.\u000a      </div>\u000a      \u000a      <span class="orthography">Pylad</span>\u000a      \u000a   </div>','myth148','myth')
INSERT INTO TEXTREFERENCE VALUES(10802,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth363">\u000a      \u000a      <div class="normForm">Radgrid</div>\u000a      \u000a      <div class="popUp">Radgrid er en valkyrie fra den nordiske mytologi. Navnet betyder muligvis \u2018den herskesyge\u2019.</div>\u000a      \u000a   </div>','myth363','myth')
INSERT INTO TEXTREFERENCE VALUES(10803,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth104">\u000a      \u000a      <div class="normForm">Ragnarok</div>\u000a      \u000a      <div class="popUp">Ragnarok er Jordens undergang i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Ragnaroke\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ragnarokr</span>\u000a      \u000a      <div class="encyc">P\u00e5 Grundtvigs tid gjaldt stadig en fejlfortolkning af \u2018ragna r\u00f6kr\u2019 fra Snorre. Det blev l\u00e6st som  Ragnarok (da. gudernes dunkelhed, jf. det ty. G\u00f6tterd\u00e4mmerung).\u000a         Grundtvig taler derfor om \u2018asernes tusm\u00f8rke\u2019. I dag l\u00e6ses udtrykket \u2018Ragnar\u00f6k\u2019, og\u000a         \u2018gudernes sk\u00e6bne\u2019 regnes for den korrekte overs\u00e6ttelse.\u000a      </div>\u000a      \u000a   </div>','myth104','myth')
INSERT INTO TEXTREFERENCE VALUES(10804,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth362">\u000a      \u000a      <div class="normForm">Rangrid</div>\u000a      \u000a      <div class="popUp">Rangrid er en valkyrie fra den nordiske mytologi. Navnet betyder muligvis \u2018skjold\u00f8del\u00e6gger\u2019.</div>\u000a      \u000a   </div>','myth362','myth')
INSERT INTO TEXTREFERENCE VALUES(10805,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth290">\u000a      \u000a      <div class="normForm">Ratatosk</div>\u000a      \u000a      <div class="popUp">Ratatosk er et egern fra den nordiske mytologi. Det l\u00f8ber op og ned ad verdenstr\u00e6et\u000a         Yggdrasil som n\u00e6svis budbringer mellem \u00f8rnen i toppen og dragen ved tr\u00e6ets r\u00f8dder.\u000a         Navnet betyder \u2018gnavtand\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Ratatoskur</span>\u000a      \u000a   </div>','myth290','myth')
INSERT INTO TEXTREFERENCE VALUES(10806,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth364">\u000a      \u000a      <div class="normForm">Reginleiv</div>\u000a      \u000a      <div class="popUp">Reginleiv er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018gudedatter\u2019.</div>    \u000a      \u000a      <span class="orthography">Reginleif</span>\u000a      \u000a   </div>','myth364','myth')
INSERT INTO TEXTREFERENCE VALUES(10807,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth405">\u000a      \u000a      <div class="normForm">Regnar Lodbrog</div>\u000a      \u000a      <div class="popUp">Regnar Lodbrog er en dansk sagnkonge.</div>    \u000a      \u000a   </div>','myth405','myth')
INSERT INTO TEXTREFERENCE VALUES(10808,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth246">\u000a      \u000a      <div class="normForm">Rimfakse</div>\u000a      \u000a      <div class="popUp">Rimfakse er nattens hest i den nordiske mytologi. Navnet betyder \u2018rimmanke\u2019.</div>\u000a      \u000a      <span class="orthography">Hrimfakse\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Rimfaxe</span>\u000a      \u000a      <div class="encyc">Riddende p\u00e5 Rimfakse tr\u00e6kker j\u00e6ttekvinden Nat m\u00f8rket hen over himlen.</div>\u000a      \u000a   </div>','myth246','myth')
INSERT INTO TEXTREFERENCE VALUES(10809,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth203">\u000a      \u000a      <div class="normForm">Rind</div>\u000a      \u000a      <div class="popUp">Rind er en gudinde fra den nordiske mytologi. Sammen med Odin har hun s\u00f8nnen Vale.</div>\u000a      \u000a      <span class="orthography">Rinda</span>\u000a      \u000a   </div>','myth203','myth')
INSERT INTO TEXTREFERENCE VALUES(10810,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth253">\u000a      \u000a      <div class="normForm">Ringhorne</div>\u000a      \u000a      <div class="popUp">Ringhorne er Balders skib i den nordiske mytologi. Da han er d\u00f8d, bliver hans lig\u000a         br\u00e6ndt p\u00e5 dette skib.\u000a      </div>\u000a      \u000a      <span class="orthography">Hringhorne</span>\u000a      \u000a   </div>','myth253','myth')
INSERT INTO TEXTREFERENCE VALUES(10811,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth62">\u000a      \u000a      <div class="normForm">Roar</div>\u000a      \u000a      <div class="popUp">Roar er en dansk sagnkonge af skjoldungesl\u00e6gten. Han er broder til Helge, med hvem\u000a         han deler magten, s\u00e5ledes at Roar regerer landet (omkring Lejre), og Helge hersker\u000a         p\u00e5 havet som s\u00f8konge.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth62','myth')
INSERT INTO TEXTREFERENCE VALUES(10812,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth415">\u000a      \u000a      <div class="normForm">Rolf</div>\u000a      \u000a      <div class="popUp">.</div>               \u000a      \u000a      <div class="encyc">.</div>\u000a      \u000a   </div>','myth415','myth')
INSERT INTO TEXTREFERENCE VALUES(10813,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth63">\u000a      \u000a      <div class="normForm">Rolf Krake</div>\u000a      \u000a      <div class="popUp">Rolf Krake er en dansk sagnkonge af skjoldungesl\u00e6gten. Han beskrives som en idealkonge,\u000a         der overg\u00e5r alle i godhed, mod og naturlighed.\u000a      </div>\u000a      \u000a      <div class="encyc">Rolf Krake kendes fra \u000a         <span class="italic">SnorresEdda.</span>\u000a      </div>\u000a      \u000a   </div>','myth63','myth')
INSERT INTO TEXTREFERENCE VALUES(10814,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth367">\u000a      \u000a      <div class="normForm">Rota</div>\u000a      \u000a      <div class="popUp">Rota er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018regnfuldt vejr\u2019.</div>\u000a      \u000a   </div>','myth367','myth')
INSERT INTO TEXTREFERENCE VALUES(10815,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth64">\u000a      \u000a      <div class="normForm">Saga</div>\u000a      \u000a      <div class="popUp">Saga er en gudinde (asynje) fra den nordiske mytologi. M\u00e5ske er hun identisk med Odins\u000a         hustru, Frigg.\u000a      </div>\u000a      \u000a      <div class="encyc">For Grundtvig bliver Saga en slags muse, der kan \u00e5bne hans \u00f8jne for betydningen af\u000a         og sammenh\u00e6ngen i den nordiske mytologi.\u000a      </div>\u000a      \u000a   </div>','myth64','myth')
INSERT INTO TEXTREFERENCE VALUES(10816,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth385">\u000a      \u000a      <div class="normForm">Saldis</div>\u000a      \u000a      <div class="popUp">Saldis er en kvinde fra den islandske sagalitteratur. Da hun er meget utilfreds med\u000a         en sp\u00e5dom om hendes b\u00f8rneb\u00f8rn, som bliver fremsagt af en v\u00f8lve, smider hun v\u00f8lven\u000a         p\u00e5 porten.\u000a      </div>\u000a      \u000a      <div class="encyc">Man kan l\u00e6se fort\u00e6llingen om Saldis og v\u00f8lven i \u000a         <span class="italic">Viga-Glums Saga.</span>\u000a      </div>\u000a      \u000a   </div>','myth385','myth')
INSERT INTO TEXTREFERENCE VALUES(10817,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.o">\u000a      \u000a      <div class="epithet">Sejrfader\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Seirfader</span>\u000a      \u000a   </div>','myth59.o','myth')
INSERT INTO TEXTREFERENCE VALUES(10818,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth337">\u000a      \u000a      <div class="normForm">Sene</div>\u000a      \u000a      <div class="popUp">Sene er navnet p\u00e5 en hest i den nordiske mytologi.</div> \u000a      \u000a      <span class="orthography">Sinir</span>\u000a      \u000a   </div>','myth337','myth')
INSERT INTO TEXTREFERENCE VALUES(10819,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth204">\u000a      \u000a      <div class="normForm">Sif</div>\u000a      \u000a      <div class="popUp">Sif er en af asynjerne fra den nordiske mytologi. Hun er den smukkeste af gudinderne\u000a         og gift med Thor. Hun er is\u00e6r kendt for sit guldh\u00e5r.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>den h\u00e5rfagre gudindehat</span>\u000a   </div>','myth204','myth')
INSERT INTO TEXTREFERENCE VALUES(10820,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth318">\u000a      \u000a      <div class="normForm">Sigmund</div>\u000a      \u000a      <div class="popUp">Sigmund er en af Odins s\u00f8nner fra den nordiske mytologi.</div>\u000a      \u000a      <div class="encyc">Om Sigmund kan man l\u00e6se i \u000a         <span class="italic">Hyndlas Sang</span>.\u000a      </div>\u000a      \u000a   </div>','myth318','myth')
INSERT INTO TEXTREFERENCE VALUES(10821,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth65">\u000a      \u000a      <div class="normForm">Sigurd Fafnersbane</div>\u000a      \u000a      <div class="popUp">Sigurd Fafnersbane er en helt fra den nordiske sagnhistorie, af v\u00f8lsungernes sl\u00e6gt.\u000a         Som det fremg\u00e5r af hans tilnavn, dr\u00e6ber han bl.a. dragen Fafner. Hans elskede hedder\u000a         Brynhilde.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth65','myth')
INSERT INTO TEXTREFERENCE VALUES(10822,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth126">\u000a      \u000a      <div class="normForm">Sigyn</div>\u000a      \u000a      <div class="popUp">Sigyn er Lokes trofaste hustru i den nordiske mytologi.</div>\u000a      \u000a      <div class="encyc">Da Loke straffes efter Ragnarok, opsamler Sigyn i en sk\u00e5l den gift, der ellers skal\u000a         dryppe ned over ham som straf. Kun n\u00e5r hun vender sig for at t\u00f8mme sk\u00e5len, drypper\u000a         giften ned p\u00e5 Loke, der s\u00e5 vrider sig i smerte.\u000a      </div>\u000a      \u000a   </div>','myth126','myth')
INSERT INTO TEXTREFERENCE VALUES(10823,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth252">\u000a      \u000a      <div class="normForm">Sildetand</div>\u000a      \u000a      <div class="popUp">Sildetand er Frejs ridedyr fra den nordiske mytologi. </div>\u000a      \u000a   </div>','myth252','myth')
INSERT INTO TEXTREFERENCE VALUES(10824,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth344">\u000a      \u000a      <div class="normForm">Sindre</div>\u000a      \u000a      <div class="popUp">Sindre er en dv\u00e6rg (svartalf) i den nordiske mytologi. Sammen med sin bror, Brok,\u000a         har han smedet ringen Draupner, galten Gyldenb\u00f8rste og hammeren Mj\u00f8lner.\u000a      </div>   \u000a      \u000a   </div>','myth344','myth')
INSERT INTO TEXTREFERENCE VALUES(10825,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth397">\u000a      \u000a      <div class="normForm">Sindur</div>\u000a      \u000a      <div class="popUp">Sindur er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>    \u000a      \u000a   </div>','myth397','myth')
INSERT INTO TEXTREFERENCE VALUES(10826,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth211">\u000a      \u000a      <div class="normForm">Sjofn</div>\u000a      \u000a      <div class="popUp">Sjofn er en asynje, dvs. gudinde, fra den nordiske mytologi. Hun er is\u00e6r knyttet til\u000a         k\u00e6rlighed.\u000a      </div>\u000a      \u000a      <div class="encyc">\u2018Sjafni\u2019 er ogs\u00e5 et andet ord for elskov.</div>\u000a      \u000a   </div>','myth211','myth')
INSERT INTO TEXTREFERENCE VALUES(10827,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth202">\u000a      \u000a      <div class="normForm">Skade</div>\u000a      \u000a      <div class="popUp">Skade er en gudinde for jagt og skil\u00f8b i den nordiske mytologi. Hun er datter af j\u00e6tten\u000a         Tjasse og gift med frugtbarhedsguden Njord, men deres \u00e6gteskab er ikke lykkeligt;\u000a         og de har hver sin bolig.\u000a      </div>\u000a      \u000a      <div class="encyc">Hun er m\u00e5ske mor til Frej.</div>\u000a      \u000a   </div>','myth202','myth')
INSERT INTO TEXTREFERENCE VALUES(10828,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth298">\u000a      \u000a      <div class="normForm">Skatyrnir</div>\u000a      \u000a      <div class="popUp">Skatyrnir er navnet p\u00e5 en af himlene i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth298','myth')
INSERT INTO TEXTREFERENCE VALUES(10829,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth356">\u000a      \u000a      <div class="normForm">Skeggold</div>\u000a      \u000a      <div class="popUp">Skeggold er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018strids\u00f8kse\u2019.</div>  \u000a      \u000a      <span class="orthography">Skeggiauld\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Skegg\u00f8ld</span>\u000a      \u000a   </div>','myth356','myth')
INSERT INTO TEXTREFERENCE VALUES(10830,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth341">\u000a      \u000a      <div class="normForm">Skidbladner</div>\u000a      \u000a      <div class="popUp">Skidbladner er navnet p\u00e5 et skib i den nordiske mytologi. Det tilh\u00f8rer Frej og har\u000a         den egenskab, at det kan foldes sammen og puttes i en lomme, n\u00e5r man er kommet p\u00e5\u000a         land. Til vands kan det rumme alle guder. \u000a      </div>  \u000a      \u000a      <span class="orthography">Skibladnir,</span>\u000a      \u000a      <span class="orthography">Skidblaner\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Skidblanir</span>\u000a      \u000a      <div class="encyc">Navnet hentyder til, at plankerne er tynde som knivsblade.</div>\u000a      \u000a   </div>','myth341','myth')
INSERT INTO TEXTREFERENCE VALUES(10831,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth214">\u000a      \u000a      <div class="normForm">Skinfakse</div>\u000a      \u000a      <div class="popUp">Skinfakse er den hest, som Dag rider hen over himlen p\u00e5 i den nordiske mytologi. Navnet\u000a         betyder \u2018den skinnende manke\u2019.\u000a      </div>\u000a      \u000a   </div>','myth214','myth')
INSERT INTO TEXTREFERENCE VALUES(10832,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth66">\u000a      \u000a      <div class="normForm">Skirner</div>\u000a      \u000a      <div class="popUp">Navnet betyder \u2018den skinnende\u2019. Han er en af Frejs tjenere (en skosvend).</div>\u000a      \u000a      <span class="orthography">Skierner\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Skirnir</span>\u000a      \u000a      <div class="encyc">Skirner optr\u00e6der i eddadigtet \u000a         <span class="italic">Sk\u00edrnism\u00e1l</span> (da. \u000a         <span class="italic">Skirners F\u00e6rd</span>).\u000a      </div>\u000a      \u000a   </div>','myth66','myth')
INSERT INTO TEXTREFERENCE VALUES(10833,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth67">\u000a      \u000a      <div class="normForm">Skjold</div>\u000a      \u000a      <div class="popUp">Skjold var dansk sagnkonge, der blev stamfader til skjoldungerne. If\u00f8lge sagnet er\u000a         han s\u00f8n af Odin og Rind. Hans konges\u00e6de l\u00e5 i Lejre. Mange af Danmarks senere konger\u000a         f\u00f8rer deres sl\u00e6gt tilbage til ham.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth67','myth')
INSERT INTO TEXTREFERENCE VALUES(10834,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth271">\u000a      \u000a      <div class="normForm">Skogul</div>\u000a      \u000a      <div class="popUp">Skogul er en valkyrie fra den nordiske mytologi. Navnet betyder \u2018kamp\u2019.</div>\u000a      \u000a      <span class="orthography">Skaugul,</span>\u000a      \u000a      <span class="orthography">Sk\u00f8gul\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Sk\u00f8gull</span>\u000a      \u000a   </div>','myth271','myth')
INSERT INTO TEXTREFERENCE VALUES(10835,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth303">\u000a      \u000a      <div class="normForm">Skoll</div>\u000a      \u000a      <div class="popUp">Skoll er en j\u00e6tte i ulveskikkelse fra den nordiske mytologi. Det siges, at den vil\u000a         sluge solen ved Ragnarok. Navnet betyder \u2018spot\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Skol</span>\u000a      \u000a   </div>','myth303','myth')
INSERT INTO TEXTREFERENCE VALUES(10836,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth335">\u000a      \u000a      <div class="normForm">Skridbrimer</div>\u000a      \u000a      <div class="popUp">Skridbrimer er navnet p\u00e5 en hest i den nordiske mytologi.</div>   \u000a      \u000a      <span class="orthography">Skeidbrimer\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Skridbrimir</span>\u000a      \u000a   </div>','myth335','myth')
INSERT INTO TEXTREFERENCE VALUES(10837,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth226.a">\u000a      \u000a      <div class="epithet">Skrymer\u000a         <span class="sameAs">\u000a            <a href="#myth226"> &gt; Udg\u00e5rdsloke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth226.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10838,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth68">\u000a      \u000a      <div class="normForm">Skuld</div>\u000a      \u000a      <div class="popUp">Skuld er en af de tre norner (sk\u00e6bnegudinder) fra nordisk mytologi. De to andre er\u000a         Verdande og Urd. Navnet er afledt af \u2018skulu\u2019 (skulle), \u2018det, der skal komme\u2019, og er\u000a         i familie med ordet skyld.\u000a      </div>\u000a      \u000a      <span class="orthography">Skulde\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Skulda</span>\u000a      \u000a      <div class="encyc">I eddadigtet \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) er Skuld ogs\u00e5 en valkyrie.\u000a      </div>\u000a      \u000a   </div>','myth68','myth')
INSERT INTO TEXTREFERENCE VALUES(10839,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth169">\u000a      \u000a      <div class="normForm">Sleipner</div>\u000a      \u000a      <div class="popUp">Sleipner er guden Odins ottefodede hest i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth169','myth')
INSERT INTO TEXTREFERENCE VALUES(10840,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth48.f">\u000a      \u000a      <div class="epithet">Sleipners fr\u00e6nde\u000a         <span class="sameAs">\u000a            <a href="#myth48"> &gt; Loke</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth48.f','myth')
INSERT INTO TEXTREFERENCE VALUES(10841,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth218">\u000a      \u000a      <div class="normForm">Snotra</div>\u000a      \u000a      <div class="popUp">Snotra er en asynje, dvs. gudinde, fra den nordiske mytologi. Navnet betyder \u2018den\u000a         kloge\u2019, og hun er kendt som meget velopdragen.\u000a      </div>\u000a      \u000a   </div>','myth218','myth')
INSERT INTO TEXTREFERENCE VALUES(10842,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth69">\u000a      \u000a      <div class="normForm">Sodr\u00f8d</div>\u000a      \u000a      <div class="popUp">Sodr\u00f8d er den sodm\u00f8rke (sorte) hane fra den nordiske mytologi, som galer ved Ragnarok.</div>\u000a      \u000a      <span class="orthography">Sor\u00f8d\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">den sodbrune</span>\u000a      \u000a      \u000a   </div>','myth69','myth')
INSERT INTO TEXTREFERENCE VALUES(10843,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth219">\u000a      \u000a      <div class="normForm">Sol</div>\u000a      \u000a      <div class="popUp">Sol er i den nordiske mytologi datter af Mundilfare og s\u00f8ster til drengen M\u00e5ne. Guderne\u000a         har anbragt dem p\u00e5 himlen, for at de p\u00e5 hver sin vogn skal styre henholdsvis solen\u000a         og m\u00e5nen.\u000a      </div>\u000a      \u000a      <div class="encyc">Egentlig havde Mundilfare blot opkaldt sine b\u00f8rn efter solen og m\u00e5nen, men guderne\u000a         straffede ham for dette hovmod ved at s\u00e6tte b\u00f8rnene til at k\u00f8re henholdsvis solens\u000a         og m\u00e5nens vogne hen over himlen.\u000a      </div>\u000a      \u000a   </div>','myth219','myth')
INSERT INTO TEXTREFERENCE VALUES(10844,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth323">\u000a      \u000a      <div class="normForm">Sonargalt</div>\u000a      \u000a      <div class="popUp">Sonargalt er en galt (kastreret hangris) fra den nordiske mytologi.</div>\u000a      \u000a      <div class="encyc">Sonargalt tilh\u00f8rer Freja.</div>\u000a      \u000a   </div>','myth323','myth')
INSERT INTO TEXTREFERENCE VALUES(10845,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth185">\u000a      \u000a      <div class="normForm">Starkad</div>\u000a      \u000a      <div class="popUp">Starkad er en nordisk sagnhelt, kriger og skjald, som kendes fra Saxo og de islandske\u000a         sagaer. Navnet betyder \u2018den st\u00e6rke\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">St\u00e6rkodder</span>\u000a      \u000a      <div class="encyc">Han er kendt for sin styrke og sin fork\u00e6rlighed for spartansk, nordisk levevis i mods\u00e6tning\u000a         til tyskinspireret forfinelse og vellevned.\u000a      </div>\u000a      \u000a   </div>','myth185','myth')
INSERT INTO TEXTREFERENCE VALUES(10846,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth186">\u000a      \u000a      <div class="normForm">Stridsk\u00f8gul</div>\u000a      \u000a      <div class="popUp">Stridsk\u00f8gul er en af valkyrierne i den nordiske mytologi.</div>\u000a      \u000a      <div class="encyc">Hun kendes fra \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom.</span>\u000a      </div>\u000a      \u000a   </div>','myth186','myth')
INSERT INTO TEXTREFERENCE VALUES(10847,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth249">\u000a      \u000a      <div class="normForm">Styx</div>\u000a      \u000a      <div class="popUp">Styx er i gr\u00e6sk mytologi navnet p\u00e5 floden ved indgangen til d\u00f8dsriget (Hades). F\u00e6rgemanden\u000a         Charos fragter de d\u00f8de sj\u00e6le over Styx.\u000a      </div>\u000a      \u000a      <span class="orthography">de stygiske vande</span>\u000a      \u000a   </div>','myth249','myth')
INSERT INTO TEXTREFERENCE VALUES(10848,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth371">\u000a      \u000a      <div class="normForm">Sult</div>\u000a      \u000a      <div class="popUp">Sult er navnet p\u00e5 kniven hos d\u00f8dsgudinden Hel i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth371','myth')
INSERT INTO TEXTREFERENCE VALUES(10849,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth71">\u000a      \u000a      <div class="normForm">Surt</div>\u000a      \u000a      <div class="popUp">Surt betyder egl. \u2018den sorte\u2019. Han er en ildj\u00e6tte fra den nordiske mytologi og regerer\u000a         i ildlandet Muspelheim, der ligger mod syd. Under Ragnarok rider han ud fra Muspelheim\u000a         og afbr\u00e6nder jorden.\u000a      </div>\u000a      \u000a      <span class="orthography">Surtur\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Surtr</span>\u000a      \u000a      \u000a   </div>','myth71','myth')
INSERT INTO TEXTREFERENCE VALUES(10850,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth72">\u000a      \u000a      <div class="normForm">Suttung</div>\u000a      \u000a      <div class="popUp">Suttung er en j\u00e6tte fra den nordiske mytologi. Han ejer skjaldemj\u00f8den, der er brygget\u000a         af Kvasers blod. \u2018Suttungs mj\u00f8d\u2019 er en kenning (fast poetisk omskrivning) for skjalde-\u000a         eller digterkunsten.\u000a      </div>\u000a      \u000a      <span class="orthography">Suttungr</span>\u000a      \u000a      <div class="encyc">At Suttungs mj\u00f8d havnede hos aserne, hentydes der til i eddadigtet \u000a         <span class="italic">H\u00e1vam\u00e1l</span> (da. \u000a         <span class="italic">Den h\u00f8jes Tale</span>).\u000a      </div>\u000a      \u000a   </div>','myth72','myth')
INSERT INTO TEXTREFERENCE VALUES(10851,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth241">\u000a      \u000a      <div class="normForm">Svadilfare</div>\u000a      \u000a      <div class="popUp">Svadilfare er en us\u00e6dvanlig st\u00e6rk arbejdshingst fra den nordiske mytologi. Den tilh\u00f8rer\u000a         den j\u00e6tte, der har p\u00e5taget sig at bygge et g\u00e6rde omkring gudernes bolig, Asg\u00e5rd.\u000a      </div>\u000a      \u000a      <span class="orthography">Svadilfar</span>\u000a      \u000a      <div class="encyc">J\u00e6tten forlanger solen, m\u00e5nen og Freja for sit arbejde, hvis han kan gennemf\u00f8re byggeriet\u000a         p\u00e5 \u00e9n vinter. Da det ser ud til at lykkes, tvinger aserne Loke til at forvandle sig\u000a         til en hoppe, der kan lokke Svadilfare v\u00e6k fra byggeriet. Resultatet af hoppens fristende\u000a         opf\u00f8rsel bliver Odins ottebenede hest, Sleipner.\u000a      </div>\u000a      \u000a   </div>','myth241','myth')
INSERT INTO TEXTREFERENCE VALUES(10852,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth285">\u000a      \u000a      <div class="normForm">Svafner</div>\u000a      \u000a      <div class="popUp">Svafner er en af de slanger fra den nordiske mytologi, der ligger under verdenstr\u00e6et\u000a         Yggdrasil.\u000a      </div>\u000a      \u000a      <span class="orthography">Sv\u00e1fnir,</span>\u000a      \u000a      <span class="orthography">Svofnir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Sv\u00e5fnir</span>\u000a      \u000a      <div class="encyc">Hos Snorre lever disse slanger i kilden Hvergelmer.</div>\u000a      \u000a      \u000a   </div>','myth285','myth')
INSERT INTO TEXTREFERENCE VALUES(10853,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth261">\u000a      \u000a      <div class="normForm">Svartalfheim</div>\u000a      \u000a      <div class="popUp">Svartalfheim er svart- eller sortalfernes hjem i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth261','myth')
INSERT INTO TEXTREFERENCE VALUES(10854,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth403">\u000a      \u000a      <div class="normForm">Svarth\u00f8fud</div>\u000a      \u000a      <div class="popUp">Svarth\u00f8fud er en k\u00e6mpe i den nordiske mytologi. Han opfattes som stamfader til mennesker,\u000a         der kan \u00f8ve seid. Navnet betyder \u2018sorthoved\u2019.\u000a      </div>    \u000a      \u000a      <span class="orthography">Svarth\u00f8f\u00f0i\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Svarth\u00f8vud</span>\u000a      \u000a   </div>','myth403','myth')
INSERT INTO TEXTREFERENCE VALUES(10855,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.p">\u000a      \u000a      <div class="epithet">Svipdag\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp"> I flere af sagaerne er Svipdag et d\u00e6knavn for Odin.</div>\u000a      \u000a   </div>','myth59.p','myth')
INSERT INTO TEXTREFERENCE VALUES(10856,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth152">\u000a      \u000a      <div class="normForm">Svipdag</div>\u000a      \u000a      <div class="popUp">Svipdag er en helt fra den nordiske mytologi. Han drager ud for at finde og vinde\u000a         sin elskede, Mengl\u00f8d.\u000a      </div>\u000a      \u000a      <div class="encyc">Svipdag optr\u00e6der i eddadigtet \u000a         <span class="italic">Svipdagskvadet</span>.\u000a      </div>\u000a      \u000a   </div>','myth152','myth')
INSERT INTO TEXTREFERENCE VALUES(10857,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth299">\u000a      \u000a      <div class="normForm">Sv\u00f8l</div>\u000a      \u000a      <div class="popUp">Sv\u00f8l er navnet p\u00e5 det skjold, der i den nordiske mytologi befinder sig foran solen\u000a         og hindrer verden i at br\u00e6nde op.\u000a      </div>\u000a      \u000a      <span class="orthography">Svalin</span>\u000a      \u000a   </div>','myth299','myth')
INSERT INTO TEXTREFERENCE VALUES(10858,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth215">\u000a      \u000a      <div class="normForm">Syn</div>\u000a      \u000a      <div class="popUp">Syn er en asynje, dvs. gudinde, fra den nordiske mytologi. Hun vogter d\u00f8ren til Valhal.</div>\u000a      \u000a   </div>','myth215','myth')
INSERT INTO TEXTREFERENCE VALUES(10859,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth349">\u000a      \u000a      <div class="normForm">S\u00e6rimner</div>\u000a      \u000a      <div class="popUp">S\u00e6rimner er en galt (kastreret hangris) fra den nordiske mytologi. Den bor i Valhal\u000a         og har den egenskab, at den hver morgen bliver levende igen, efter at den er blevet\u000a         slagtet og fort\u00e6ret om aftenen.\u000a      </div>        \u000a      \u000a      <span class="orthography">S\u00e6hrimner</span>\u000a      \u000a   </div>','myth349','myth')
INSERT INTO TEXTREFERENCE VALUES(10860,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth327">\u000a      \u000a      <div class="normForm">S\u00f8kkvabek</div>\u000a      \u000a      <div class="popUp">S\u00f8kkvabek er en gudebolig i den nordiske mytologi. Den er knyttet til Saga og indimellem\u000a         ogs\u00e5 til Odin. Navnet betyder \u2018den sunkne banke\u2019 eller m\u00e5ske \u2018skyggebanken\u2019.\u000a      </div>       \u000a      \u000a      <span class="orthography">S\u00f8knabek\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">S\u00f8kkvabekkr</span>\u000a      \u000a   </div>','myth327','myth')
INSERT INTO TEXTREFERENCE VALUES(10861,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth336">\u000a      \u000a      <div class="normForm">S\u00f8lvtop</div>\u000a      \u000a      <div class="popUp">S\u00f8lvtop er navnet p\u00e5 en hest i den nordiske mytologi.</div>   \u000a      \u000a   </div>','myth336','myth')
INSERT INTO TEXTREFERENCE VALUES(10862,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth310">\u000a      \u000a      <div class="normForm">S\u00f8nder</div>\u000a      \u000a      <div class="popUp">S\u00f8nder er en af de fire dv\u00e6rge i den nordiske mytologi, der b\u00e6rer himmelhv\u00e6lvingen.</div>\u000a      \u000a   </div>','myth310','myth')
INSERT INTO TEXTREFERENCE VALUES(10863,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth320">\u000a      \u000a      <div class="normForm">Tandgnost</div>\u000a      \u000a      <div class="popUp">Tandgnost er en af de to bukke, som Thor har sp\u00e6ndt for sin tordenvogn.</div>\u000a      \u000a      <span class="orthography">Tangnjost\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Tangnjostr</span>\u000a      \u000a   </div>','myth320','myth')
INSERT INTO TEXTREFERENCE VALUES(10864,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth321">\u000a      \u000a      <div class="normForm">Tangrisner</div>\u000a      \u000a      <div class="popUp">Tangrisner er en af de to bukke, som Thor har sp\u00e6ndt for sin tordenvogn.</div>\u000a      \u000a      <span class="orthography">Tandgrisnir</span>\u000a      \u000a   </div>','myth321','myth')
INSERT INTO TEXTREFERENCE VALUES(10865,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth74">\u000a      \u000a      <div class="normForm">Thor</div>\u000a      \u000a      <div class="popUp">Thor er himmelgud og den fysisk st\u00e6rkeste gud i nordisk mytologi.</div>\u000a      \u000a      <span class="orthography">Tor\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00de\u00f3rr</span>\u000a      \u000a      <div class="encyc">Thor er s\u00f8n af Odin og Fj\u00f8rgyn (Jorden, Hlodyn) og gift med den h\u00e5rfagre Sif. Thor\u000a         ejer den magiske hammer, Mj\u00f8lner, og bek\u00e6mper j\u00e6tterne med h\u00e5rd h\u00e5nd.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Age-Thorhat</span>\u000a      <span>Asa-Thor, </span>\u000a      <span>Fjorgyns s\u00f8n, </span>\u000a      <span>Hlodyns s\u00f8n eller </span>\u000a      <span>Tordner.</span>\u000a   </div>','myth74','myth')
INSERT INTO TEXTREFERENCE VALUES(10866,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth330">\u000a      \u000a      <div class="normForm">Thrymheim</div>\u000a      \u000a      <div class="popUp">Thrymheim er navnet p\u00e5 j\u00e6ttekvinden Skades bolig i den nordiske mytologi. Hun er gift\u000a         med Njord, men \u00e6gteskabet er ikke lykkeligt; og de bor derfor ikke sammen.\u000a      </div>   \u000a      \u000a      <span class="orthography">Thrymsheim</span>\u000a      \u000a   </div>','myth330','myth')
INSERT INTO TEXTREFERENCE VALUES(10867,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.q">\u000a      \u000a      <div class="epithet">Thund\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      \u000a      <div class="popUp">I et af eddadigtene optr\u00e6der Odin under navnet Thund.</div>\u000a      \u000a      <span class="orthography">Thundr</span>\u000a      \u000a   </div>','myth59.q','myth')
INSERT INTO TEXTREFERENCE VALUES(10868,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth228">\u000a      \u000a      <div class="normForm">Tjalfe</div>\u000a      \u000a      <div class="popUp">Tjalfe er et ungt menneske fra den nordiske mytologi. Han bliver tjener for Thor p\u00e5\u000a         dennes rejse til Udg\u00e5rdsloke og deltager her i v\u00e6ddekampene mod Udg\u00e5rdslokes folk.\u000a         Hans s\u00f8ster, der ogs\u00e5 var med p\u00e5 turen, hedder R\u00f8skva.\u000a      </div>\u000a      \u000a      <span class="orthography">Tjalve</span>\u000a      \u000a   </div>','myth228','myth')
INSERT INTO TEXTREFERENCE VALUES(10869,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth240">\u000a      \u000a      <div class="normForm">Tjasse </div>\u000a      \u000a      <div class="popUp">Tjasse er en j\u00e6tte fra den nordiske mytologi. Han er far til Skade. Aserne har kastet\u000a         hans \u00f8jne p\u00e5 p\u00e5 himlen, hvor de er blevet til to stjerner.\u000a      </div>\u000a      \u000a   </div>','myth240','myth')
INSERT INTO TEXTREFERENCE VALUES(10870,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth74.e">\u000a      \u000a      <div class="epithet">Tordner\u000a         <span class="sameAs">\u000a            <a href="#myth74"> &gt; Thor</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth74.e','myth')
INSERT INTO TEXTREFERENCE VALUES(10871,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth357">\u000a      \u000a      <div class="normForm">Trud</div>\u000a      \u000a      <div class="popUp">Trud er en valkyrie fra den nordiske mytologi.</div>    \u000a      \u000a      <span class="orthography">Trudi</span>\u000a      \u000a      \u000a   </div>','myth357','myth')
INSERT INTO TEXTREFERENCE VALUES(10872,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth322">\u000a      \u000a      <div class="normForm">Trudvang</div>\u000a      \u000a      <div class="popUp">Trudvang er navnet p\u00e5 Thors bolig i Asg\u00e5rd (nordisk mytologi).</div>\u000a      \u000a      <span class="orthography">Thrudheim,</span>\u000a      \u000a      <span class="orthography">Thrudvang\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Thrudvangr</span>\u000a      \u000a   </div>','myth322','myth')
INSERT INTO TEXTREFERENCE VALUES(10873,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth75">\u000a      \u000a      <div class="normForm">Trym</div>\u000a      \u000a      <div class="popUp">Trym er konge over turserne (en slags j\u00e6tter) i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Thrymer,</span>\u000a      \u000a      <span class="orthography">Thrym,</span>\u000a      \u000a      <span class="orthography">Trymer\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00derymr</span>\u000a      \u000a      <div class="encyc">Han optr\u00e6der i \u000a         <span class="italic">Trymskvadet.</span>\u000a      </div>\u000a      \u000a   </div>','myth75','myth')
INSERT INTO TEXTREFERENCE VALUES(10874,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth98">\u000a      \u000a      <div class="normForm">Tursernes land</div>\u000a      \u000a      <div class="popUp">Tursernes land er den nordlige del af Udg\u00e5rd, j\u00e6tternes land i den nordiske mytologi.\u000a         Det ligger uden for menneskenes land, Midg\u00e5rd.\u000a      </div>\u000a      \u000a      <span class="orthography">Hrimthussers sale\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Rimtursers sale</span>\u000a      \u000a      <div class="encyc">N\u00e5r der tales om rimtursernes land, hersker der m\u00e5ske en forestilling om, at der er\u000a         meget koldt.\u000a      </div>\u000a      \u000a   </div>','myth98','myth')
INSERT INTO TEXTREFERENCE VALUES(10875,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth73">\u000a      \u000a      <div class="normForm">Tyr</div>\u000a      \u000a      <div class="popUp">Tyr er krigsgud i den nordiske mytologi og desuden gud for tingsteder, aftaler og\u000a         edsafl\u00e6ggelser. Navnet betyder \u2018larm\u2019 eller ligefrem \u2018gud\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Thir\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Thyr</span>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Ulvs fosterfaderhat</span>\u000a   </div>','myth73','myth')
INSERT INTO TEXTREFERENCE VALUES(10876,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth255">\u000a      \u000a      <div class="normForm">T\u00f8k</div>\u000a      \u000a      <div class="popUp">T\u00f8k er en gammel kone fra den nordiske mytologi. Efter manges overbevisning er T\u00f8k\u000a         blot Loke i forkl\u00e6dning, der vil hindre den d\u00f8de Balder i at vende levende tilbage\u000a         til aserne.\u000a      </div>\u000a      \u000a      <span class="orthography">Thok\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Toke</span>\u000a      \u000a      <div class="encyc">Da Balder er d\u00f8d, og Frigg g\u00e5r rundt i hele verden for at f\u00e5 alle til at gr\u00e6de for\u000a         Balder, s\u00e5 han kan blive sluppet ud af d\u00f8dsriget igen, svarer T\u00f8k, at hun kun vil\u000a         gr\u00e6de t\u00f8rre t\u00e5rer. Herefter m\u00e5 Balder forblive hos Hel.\u000a      </div>\u000a      \u000a   </div>','myth255','myth')
INSERT INTO TEXTREFERENCE VALUES(10877,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth99">\u000a      \u000a      <div class="normForm">Udg\u00e5rd</div>\u000a      \u000a      <div class="popUp">Udg\u00e5rd er j\u00e6tternes land i den nordiske mytologi. Det ligger uden for menneskenes\u000a         land, Midg\u00e5rd.\u000a      </div>\u000a      \u000a      <div class="encyc">.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Jotunheimhat</span>\u000a   </div>','myth99','myth')
INSERT INTO TEXTREFERENCE VALUES(10878,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth226">\u000a      \u000a      <div class="normForm">Udg\u00e5rdsloke</div>\u000a      \u000a      <div class="popUp">Udg\u00e5rdsloke er en j\u00e6ttekonge fra den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Udgarde-Loke</span>\u000a      \u000a      <div class="encyc">Udg\u00e5rdsloke er mest kendt fra fort\u00e6llingen om \u201cThors rejse til Udg\u00e5rd\u201d, hvor Thor\u000a         og hans ledsagere deltager i en r\u00e6kke fysiske v\u00e6ddekampe, som de taber. Det viser\u000a         sig dog, at Udg\u00e5rdsloke har snydt og ladet dem k\u00e6mpe mod fx tanken, ilden og alderdommen.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Skrymerhat</span>\u000a   </div>','myth226','myth')
INSERT INTO TEXTREFERENCE VALUES(10879,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth395">\u000a      \u000a      <div class="normForm">Ulfrun</div>\u000a      \u000a      <div class="popUp">Ulfrun er en af de ni s\u00f8stre fra den nordiske mytologi, der alle blev m\u00f8dre til guden\u000a         Heimdal.\u000a      </div>    \u000a      \u000a   </div>','myth395','myth')
INSERT INTO TEXTREFERENCE VALUES(10880,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth199">\u000a      \u000a      <div class="normForm">Ull</div>\u000a      \u000a      <div class="popUp">Ull er gud for jagt og idr\u00e6t i den nordiske mytologi. Han er s\u00f8n af Thors hustru Sif\u000a         og er som hun smuk af ydre.\u000a      </div>\u000a      \u000a      <span class="orthography">Oller,</span>\u000a      \u000a      <span class="orthography">Uller\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ullr</span>\u000a      \u000a      <div class="encyc">Ull kendes b\u00e5de fra Saxo og \u000a         <span class="italic">Snorres Edda</span>.\u000a      </div>\u000a      \u000a   </div>','myth199','myth')
INSERT INTO TEXTREFERENCE VALUES(10881,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth14.b">\u000a      \u000a      <div class="epithet">Ulv\u000a         <span class="sameAs">\u000a            <a href="#myth14"> &gt; Fenris</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth14.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10882,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth73.a">\u000a      \u000a      <div class="epithet">Ulvs fosterfader\u000a         <span class="sameAs">\u000a            <a href="#myth73"> &gt; Tyr</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Tyr lagde sin ene arm i gabet p\u00e5 Fenrisulven for at berolige den, da den skulle bindes.\u000a         Derfor er Tyr enarmet.\u000a      </div>\u000a      \u000a   </div>','myth73.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10883,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth77">\u000a      \u000a      <div class="normForm">Urd</div>\u000a      \u000a      <div class="popUp">Urd er en af de tre norner (sk\u00e6bnegudinder) i den nordiske mytologi. De to andre er\u000a         Verdande og Skuld. Urd betyder \u2018det, der var\u2019 og svarer til pr\u00e6teritum af ordet verda\u000a         (blive).\u000a      </div>\u000a      \u000a      <span class="orthography">Urda,</span>\u000a      \u000a      <span class="orthography">Urdur\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ur\u00f0r</span>\u000a      \u000a      \u000a   </div>','myth77','myth')
INSERT INTO TEXTREFERENCE VALUES(10884,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth100">\u000a      \u000a      <div class="normForm">Urds kilde</div>\u000a      \u000a      <div class="popUp">Urds Kilde er navnet p\u00e5 en kilde under Yggdrasils r\u00f8dder. Her sidder de tre navngivne\u000a         norner (sk\u00e6bnegudinder), Urd, Verdande og Skuld. Kilden vander Yggdrasil med hvidt,\u000a         frugtbarg\u00f8rende slam.\u000a      </div>\u000a      \u000a      <span class="orthography">Urdas br\u00f8nd</span>\u000a      \u000a      <div class="encyc">Der hersker en forestilling om, at asernes tingsted l\u00e5 t\u00e6t p\u00e5 Urds Kilde. </div>\u000a      \u000a   </div>','myth100','myth')
INSERT INTO TEXTREFERENCE VALUES(10885,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth78">\u000a      \u000a      <div class="normForm">Vaftrudner</div>\u000a      \u000a      <div class="popUp">Vaftrudner er en vis j\u00e6tte fra den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Vafthrudner\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Vaf\u00feru\u00f0nir</span>\u000a      \u000a      <div class="encyc"> Han optr\u00e6der i digtet \u000a         <span class="italic">Vaf\u00fer\u00fa\u00f0nis M\u00e1l</span>  (da. \u000a         <span class="italic">Vaftrudners Sang</span>), hvor han dyster med Odin i viden.\u000a      </div>\u000a      \u000a   </div>','myth78','myth')
INSERT INTO TEXTREFERENCE VALUES(10886,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth195">\u000a      \u000a      <div class="normForm">Valaskjalv</div>\u000a      \u000a      <div class="popUp">Valaskjalv er en hal i Valhal eller Valhal selv. Den omtales som en gudebolig, der\u000a         er d\u00e6kket af s\u00f8lv. Navnet kan associeres til Odins s\u00f8n Vale.\u000a      </div>\u000a      \u000a      <span class="orthography">Valaskjalf</span>\u000a      \u000a      <div class="encyc">Om Valaskjalv kan man l\u00e6se i eddadigtet \u000a         <span class="italic">Gr\u00edmnism\u00e1l.</span>\u000a      </div>\u000a      \u000a   </div>','myth195','myth')
INSERT INTO TEXTREFERENCE VALUES(10887,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth79">\u000a      \u000a      <div class="normForm">Vale</div>\u000a      \u000a      <div class="popUp">Vale er s\u00f8n af Odin og Rind. Kun \u00e9n dag gammel dr\u00e6ber han Balders banemand, H\u00f8der.</div>\u000a      \u000a      <span class="orthography">Ale\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Vala</span>\u000a      \u000a   </div>','myth79','myth')
INSERT INTO TEXTREFERENCE VALUES(10888,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth269">\u000a      \u000a      <div class="normForm">Vale</div>\u000a      \u000a      <div class="popUp">Vale er s\u00f8n af Loke og Sigyn (nordisk mytologi). Efter at Loke har forvoldt Balders\u000a         d\u00f8d, bliver Vale omskabt til en ulv, der s\u00f8nderriver sin bror, Narfe.\u000a      </div>\u000a      \u000a      <span class="orthography">Ale,</span>\u000a      \u000a      <span class="orthography">V\u00e5le\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00c5le</span>\u000a      \u000a      <div class="encyc">Undertiden kaldes Vale ogs\u00e5 Nar.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Narhat</span>\u000a   </div>','myth269','myth')
INSERT INTO TEXTREFERENCE VALUES(10889,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.r">\u000a      \u000a      <div class="epithet">Valfader\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth59.r','myth')
INSERT INTO TEXTREFERENCE VALUES(10890,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth348">\u000a      \u000a      <div class="normForm">Valgrind</div>\u000a      \u000a      <div class="popUp">Valgrind er navnet p\u00e5 en port eller l\u00e5ge i den nordiske mytologi. Den sidder formentlig\u000a         i de d\u00f8des g\u00e6rde, Nagrind. Navnet betyder \u2018de faldnes g\u00e6rde\u2019 eller \u2018d\u00f8del\u00e5ge\u2019.\u000a      </div>\u000a      \u000a      <div class="encyc">Valgrind kan dog ogs\u00e5 opfattes som selve g\u00e6rdet om d\u00f8dsriget.</div>\u000a      \u000a   </div>','myth348','myth')
INSERT INTO TEXTREFERENCE VALUES(10891,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth101">\u000a      \u000a      <div class="normForm">Valhal</div>\u000a      \u000a      <div class="popUp">Valhal er gudernes bolig i den nordiske mytologi.</div>\u000a      \u000a      <span class="orthography">Valhalla</span>\u000a      \u000a      <div class="encyc">Historikeren P.F. Suhm kalder ogs\u00e5 Valhalla for \u201cde Lyksaliges Boliger\u201d (1771, s.\u000a         29).\u000a      </div>\u000a      \u000a   </div>','myth101','myth')
INSERT INTO TEXTREFERENCE VALUES(10892,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth19.a">\u000a      \u000a      <div class="epithet">Vanadis\u000a         <span class="sameAs">\u000a            <a href="#myth19"> &gt; Freja</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Vanadis er til tilnavn til Freja</div>\u000a      \u000a   </div>','myth19.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10893,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth18.b">\u000a      \u000a      <div class="epithet">Vanagud\u000a         <span class="sameAs">\u000a            <a href="#myth18"> &gt; Frej</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth18.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10894,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth58.b">\u000a      \u000a      <div class="epithet">Vananidr\u000a         <span class="sameAs">\u000a            <a href="#myth58"> &gt; Njord</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Navnet Vanadir hentyder til, at Njord er af vanernes sl\u00e6gt.</div>\u000a      \u000a   </div>','myth58.b','myth')
INSERT INTO TEXTREFERENCE VALUES(10895,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth123">\u000a      \u000a      <div class="normForm">Ve</div>\u000a      \u000a      <div class="popUp">Ve er en as fra den nordiske mytologi, der optr\u00e6der sammen med Odin og Vile. Disse\u000a         tre dr\u00e6ber j\u00e6tten Ymer og skaber verden af hans krop.\u000a      </div>\u000a      \u000a      <div class="encyc">Om Vile og Ve kan man l\u00e6se i \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>).\u000a      </div>\u000a      \u000a   </div>','myth123','myth')
INSERT INTO TEXTREFERENCE VALUES(10896,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.s">\u000a      \u000a      <div class="epithet">Vegtam\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">I et af eddadigtene kaldes Odin ogs\u00e5 Vegtam.</div> \u000a      \u000a   </div>','myth59.s','myth')
INSERT INTO TEXTREFERENCE VALUES(10897,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth146.a">\u000a      \u000a      <div class="latin">Venus\u000a         <span class="sameAs">\u000a            <a href="#myth146"> &gt; Afrodite</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth146.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10898,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth80">\u000a      \u000a      <div class="normForm">Verdande</div>\u000a      \u000a      <div class="popUp">Verdande er en af de tre norner (sk\u00e6bnegudinder) fra nordisk mytologi; de to andre\u000a         er Skuld og Urd. Navnet betyder \u2018det v\u00e6rende\u2019 eller \u2018det, der sker\u2019.\u000a      </div>\u000a      \u000a      <span class="orthography">Verand,</span>\u000a      \u000a      <span class="orthography">Vardandi,</span>\u000a      \u000a      <span class="orthography">Verdand\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Verdandi</span>\u000a      \u000a      \u000a   </div>','myth80','myth')
INSERT INTO TEXTREFERENCE VALUES(10899,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth309">\u000a      \u000a      <div class="normForm">Vester</div>\u000a      \u000a      <div class="popUp">Vester er en af de fire dv\u00e6rge i den nordiske mytologi, der b\u00e6rer himmelhv\u00e6lvingen.</div>\u000a      \u000a   </div>','myth309','myth')
INSERT INTO TEXTREFERENCE VALUES(10900,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth222">\u000a      \u000a      <div class="normForm">Vid</div>\u000a      \u000a      <div class="popUp">Vid er en flod i den nordiske mytologi, evt. en af floderne Elivagar.</div>\u000a      \u000a      <span class="orthography">Vi\u00f0\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">V\u00edl</span>\u000a      \u000a   </div>','myth222','myth')
INSERT INTO TEXTREFERENCE VALUES(10901,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth81">\u000a      \u000a      <div class="normForm">Vidar</div>\u000a      \u000a      <div class="popUp">Vidar er en tavs as i den nordiske mytologi, s\u00f8n af Odin og j\u00e6ttekvinden Grid. \u00c9n\u000a         dag gammel h\u00e6vner han mordet p\u00e5 sin fader, Odin, som ved Ragnarok er blevet dr\u00e6bt\u000a         af Fenrisulven. Vidar f\u00e5r f\u00f8rst m\u00e6le, da Odin er d\u00f8d.\u000a      </div>\u000a      \u000a      <span class="orthography">Vedar\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Vi\u00f0arr</span>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>asers broderhat</span>\u000a      <span>Fenris bane (drabsmand) eller </span>\u000a      <span>den tavse as.</span>\u000a   </div>','myth81','myth')
INSERT INTO TEXTREFERENCE VALUES(10902,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth292">\u000a      \u000a      <div class="normForm">Vidblain</div>\u000a      \u000a      <div class="popUp">Vidblain er navnet p\u00e5 den tredie himmel i den nordiske mytologi. Navnet betyder \u2018det\u000a         vidstrakte bl\u00e5\u2019.\u000a      </div>\u000a      \u000a      <div class="encyc">If\u00f8lge Snorre er det i Vidblain, Gimle befinder sig, og hvor lysalferne bor.</div>\u000a      \u000a   </div>','myth292','myth')
INSERT INTO TEXTREFERENCE VALUES(10903,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth294">\u000a      \u000a      <div class="normForm">Vidfadm</div>\u000a      \u000a      <div class="popUp">Vidfadm er navnet p\u00e5 en af himlene i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth294','myth')
INSERT INTO TEXTREFERENCE VALUES(10904,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth401">\u000a      \u000a      <div class="normForm">Vidolf</div>\u000a      \u000a      <div class="popUp">Vidolf er stamfader til alle sandsigere og sp\u00e5m\u00e6nd i den nordiske mytologi. V\u00f8lverne\u000a         stammer fra ham. Navnet betyder \u2018skovulv\u2019.\u000a      </div> \u000a      \u000a      <span class="orthography">Vittd\u00f3lfr</span>\u000a      \u000a   </div>','myth401','myth')
INSERT INTO TEXTREFERENCE VALUES(10905,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth102">\u000a      \u000a      <div class="normForm">Vigrids Marke</div>\u000a      \u000a      <div class="popUp">Vigrids Marke er den slette (100 mil p\u00e5 hver led) i den nordiske mytologi, hvor aser\u000a         og j\u00e6tter k\u00e6mper mod hinanden ved Ragnarok.\u000a      </div>\u000a      \u000a      <span class="orthography">Vidrids Marke\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Vigridris Dale</span>\u000a      \u000a      <div class="encyc">Den omtales i eddadigtet \u000a         <span class="italic">Vaftrudners Sang.</span>\u000a      </div>\u000a      \u000a   </div>','myth102','myth')
INSERT INTO TEXTREFERENCE VALUES(10906,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth122">\u000a      \u000a      <div class="normForm">Vile</div>\u000a      \u000a      <div class="popUp">Vile er en as fra den nordiske mytologi, der optr\u00e6der sammen med Odin og Ve. Disse\u000a         tre dr\u00e6ber j\u00e6tten Ymer og skaber verden af hans krop.\u000a      </div>\u000a      \u000a      <span class="orthography">Vil</span>\u000a      \u000a      <div class="encyc">Om Vile og Ve kan man l\u00e6se i \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>).\u000a      </div>\u000a      \u000a   </div>','myth122','myth')
INSERT INTO TEXTREFERENCE VALUES(10907,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth402">\u000a      \u000a      <div class="normForm">Vilmeid</div>\u000a      \u000a      <div class="popUp">Vilmeid er stamfader til alle troldm\u00e6nd i den nordiske mytologi. Vitkerne stammer\u000a         fra ham.\u000a      </div>    \u000a      \u000a      <span class="orthography">Vilmei\u00f0r</span>\u000a      \u000a   </div>','myth402','myth')
INSERT INTO TEXTREFERENCE VALUES(10908,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth293">\u000a      \u000a      <div class="normForm">Vindblain</div>\u000a      \u000a      <div class="popUp">Vindblain er navnet p\u00e5 en af himlene i den nordiske mytologi. Navnet betyder \u2018den\u000a         vindbl\u00e5\u2019.\u000a      </div>\u000a      \u000a   </div>','myth293','myth')
INSERT INTO TEXTREFERENCE VALUES(10909,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth331">\u000a      \u000a      <div class="normForm">Vingolf</div>\u000a      \u000a      <div class="popUp">Vingolf er navnet p\u00e5 gudindernes bolig i den nordiske mytologi.</div>   \u000a      \u000a   </div>','myth331','myth')
INSERT INTO TEXTREFERENCE VALUES(10910,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth82">\u000a      \u000a      <div class="normForm">Vola</div>\u000a      \u000a      <div class="popUp">Vola er hos Grundtvig navnet p\u00e5 den v\u00f8lve, der fremsiger eddadigtet V\u00f8lvens Sp\u00e5dom.</div>\u000a      \u000a      \u000a   </div>','myth82','myth')
INSERT INTO TEXTREFERENCE VALUES(10911,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth31.a">\u000a      \u000a      <div class="latin">Vulcanus\u000a         <span class="sameAs">\u000a            <a href="#myth31"> &gt; Hefaistos</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth31.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10912,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth297">\u000a      \u000a      <div class="normForm">V\u00e6tmimir</div>\u000a      \u000a      <div class="popUp">V\u00e6tmimir er navnet p\u00e5 en af himlene i den nordiske mytologi.</div>\u000a      \u000a   </div>','myth297','myth')
INSERT INTO TEXTREFERENCE VALUES(10913,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth213">\u000a      \u000a      <div class="normForm">V\u00e5r</div>\u000a      \u000a      <div class="popUp">V\u00e5r er en asynje, dvs. gudinde, fra den nordiske mytologi. Hun er is\u00e6r knyttet til\u000a         \u00e6gteskabet og menneskers edsaftaler.\u000a      </div>\u000a      \u000a      <span class="orthography">Var,</span>\u000a      \u000a      <span class="orthography">V\u00e1r\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Vor</span>\u000a      \u000a   </div>','myth213','myth')
INSERT INTO TEXTREFERENCE VALUES(10914,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth329">\u000a      \u000a      <div class="normForm">Ydale</div>\u000a      \u000a      <div class="popUp">Ydale er navnet p\u00e5 Ullers bolig i den nordiske mytologi.</div>  \u000a      \u000a      <span class="orthography">Ydalir</span>\u000a      \u000a   </div>','myth329','myth')
INSERT INTO TEXTREFERENCE VALUES(10915,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth103">\u000a      \u000a      <div class="normForm">Yggdragsil</div>\u000a      \u000a      <div class="popUp">I den nordiske mytologi er Yggdrasil det store asketr\u00e6, der b\u00e6rer verden og g\u00e5r gennem\u000a         himmel, jord og underverden.\u000a      </div>\u000a      \u000a      <span class="orthography">Ydrasil\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">Ygdrasil</span>\u000a      \u000a      <div class="encyc">I toppen af tr\u00e6et ligger gudernes hjem, Asg\u00e5rd, mens r\u00f8dderne vandes af slam fra nornernes\u000a         kilde.\u000a      </div>\u000a      \u000a      <div class="encyc">Menneskenes hjem eller bolig, Midg\u00e5rd, findes i midten.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Mimamejdhat</span>\u000a   </div>','myth103','myth')
INSERT INTO TEXTREFERENCE VALUES(10916,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth59.u">\u000a      \u000a      <div class="epithet">Yggur\u000a         <span class="sameAs">\u000a            <a href="#myth59"> &gt; Odin</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth59.u','myth')
INSERT INTO TEXTREFERENCE VALUES(10917,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth84">\u000a      \u000a      <div class="normForm">Ymer</div>\u000a      \u000a      <div class="popUp">Ymer er urj\u00e6tte fra den nordiske mytologi. Da han dr\u00e6bes af Odin, Vile og Ve, danner\u000a         de verden af hans krop.\u000a      </div>\u000a      \u000a      <span class="orthography">Yme</span>\u000a      \u000a      <div class="encyc">Blodet bliver til vandene, selve kroppen til jorden, knoglerne til bjerge, hjerneskallen\u000a         til himmelhv\u00e6lvingen. \u00d8jenbrynene bliver til Asg\u00e5rd, gudernes hjem.\u000a      </div>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Augelmerhat</span>\u000a      <span>Brim.</span>\u000a   </div>','myth84','myth')
INSERT INTO TEXTREFERENCE VALUES(10918,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth171">\u000a      \u000a      <div class="normForm">Zeus</div>\u000a      \u000a      <div class="popUp">Zeus er gudernes overhoved i den gr\u00e6ske mytologi. Han hersker over himmel, vejr og\u000a         lynild og opfattes af Homer som guders og menneskers fader.\u000a      </div>\u000a      \u000a      <div class="encyc">I den romerske mytologi kaldes Zeus for Jupiter.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Jupiterhat</span>\u000a   </div>','myth171','myth')
INSERT INTO TEXTREFERENCE VALUES(10919,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth108">\u000a      \u000a      <div class="normForm">alf</div>\u000a      \u000a      <div class="popUp">En alf er et guddommeligt v\u00e6sen i den nordiske mytologi og folketro. Alferne er mest\u000a         knyttet til aser og vaner.\u000a      </div>\u000a      \u000a      <div class="encyc">Nogle opdeler dem i lysalfer (gode) og svartalfer (onde).</div>\u000a      \u000a   </div>','myth108','myth')
INSERT INTO TEXTREFERENCE VALUES(10920,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth384">\u000a      \u000a      <div class="normForm">alfunge</div>\u000a      \u000a      <div class="popUp">samme som askunge???</div>    \u000a      \u000a   </div>','myth384','myth')
INSERT INTO TEXTREFERENCE VALUES(10921,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth106">\u000a      \u000a      <div class="normForm">as</div>\u000a      \u000a      <div class="popUp">En as er et medlem af gudesl\u00e6gten aserne i den nordiske mytologi. De kvindelige aser\u000a         kaldes ofte asynjer.\u000a      </div>\u000a      \u000a      <div class="encyc">I nogle sammenh\u00e6nge \u00e6ndrer definitionerne systematik, formentlig under indflydelse\u000a         fra gr\u00e6sk mytologi, s\u00e5ledes at kun de tolv guder fra den nordiske mytologi, der bor\u000a         i Asg\u00e5rd, kaldes aser.\u000a      </div>\u000a      \u000a   </div>','myth106','myth')
INSERT INTO TEXTREFERENCE VALUES(10922,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth81.c">\u000a      \u000a      <div class="epithet">asers broder\u000a         <span class="sameAs">\u000a            <a href="#myth81"> &gt; Vidar</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth81.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10923,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth117.c">\u000a      \u000a      <div class="epithet">asunge\u000a         <span class="sameAs">\u000a            <a href="#myth117"> &gt; valkyrie</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Grundtvig formoder, at man ogs\u00e5 kan kalde valkyrierne asunger.</div>\u000a      \u000a   </div>','myth117.c','myth')
INSERT INTO TEXTREFERENCE VALUES(10924,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth107">\u000a      \u000a      <div class="normForm">asynje</div>\u000a      \u000a      <div class="popUp">En asynje er en kvindelig as fra den nordiske mytologi.</div>\u000a      \u000a      \u000a   </div>','myth107','myth')
INSERT INTO TEXTREFERENCE VALUES(10925,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth125">\u000a      \u000a      <div class="normForm">bjergrise</div>\u000a      \u000a      <div class="popUp">En bjergrise tilh\u00f8rer den j\u00e6tteart  i den nordiske mytologi, der is\u00e6r lever i bjergene.</div>\u000a      \u000a      \u000a   </div>','myth125','myth')
INSERT INTO TEXTREFERENCE VALUES(10926,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth409">\u000a      \u000a      <div class="normForm">br\u00e5valla-slaget</div>\u000a      \u000a      <div class="popUp">Br\u00e5valla er en slette i \u00d6sterg\u00f6tland i Sverige, hvor der if\u00f8lge Saxo og flere islandske\u000a         sagaer udk\u00e6mpedes et v\u00e6ldigt slag mellem den danske kong Harald Hildetand og den \u00f8stg\u00f8tiske\u000a         kong Ring.\u000a      </div>    \u000a      \u000a      <span class="orthography">bravalla-slaget\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">br\u00e1vellir-slaget</span>\u000a      \u000a      <div class="encyc">Dette sagnhistoriske slag er blevet et fast udtryk p\u00e5 dansk: \u2018et br\u00e5vallaslag\u2019 i betydningen\u000a         et v\u00e6ldigt slag eller en hidsig debat. \u000a      </div>\u000a      \u000a   </div>','myth409','myth')
INSERT INTO TEXTREFERENCE VALUES(10927,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth19.d">\u000a      \u000a      <div class="epithet">den gr\u00e5dfagre gudinde\u000a         <span class="sameAs">\u000a            <a href="#myth19"> &gt; Freja</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth19.d','myth')
INSERT INTO TEXTREFERENCE VALUES(10928,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth204.a">\u000a      \u000a      <div class="epithet">den h\u00e5rfagre gudinde\u000a         <span class="sameAs">\u000a            <a href="#myth204"> &gt; Sif</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth204.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10929,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth81.a">\u000a      \u000a      <div class="epithet">den tavse as\u000a         <span class="sameAs">\u000a            <a href="#myth81"> &gt; Vidar</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth81.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10930,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth379">\u000a      \u000a      <div class="normForm">draugr</div>\u000a      \u000a      <div class="popUp">Draugr er det norr\u00f8ne begreb for en genganger.</div>    \u000a      \u000a      <span class="orthography">drauge</span>\u000a      \u000a   </div>','myth379','myth')
INSERT INTO TEXTREFERENCE VALUES(10931,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth109">\u000a      \u000a      <div class="normForm">dv\u00e6rg</div>\u000a      \u000a      <div class="popUp">En dv\u00e6rg er et overnaturligt v\u00e6sen fra den nordiske mytologi og folketro. Dv\u00e6rgene\u000a         er altid dygtige smede og knyttes ofte sammen med guld og andre kostbarheder.\u000a      </div>\u000a      \u000a      <div class="encyc"> Som regel holder de til i bjerge og stenmassiver. De hj\u00e6lper indimellem aserne, men\u000a         ikke altid godvilligt. If\u00f8lge \u000a         <span class="italic">Snorres Edda</span> er det fire dv\u00e6rge, \u00d8stre, Vestre, Nordre og S\u00f8ndre, der b\u00e6rer himmelhv\u00e6lvingen.\u000a      </div>\u000a      \u000a   </div>','myth109','myth')
INSERT INTO TEXTREFERENCE VALUES(10932,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth110">\u000a      \u000a      <div class="normForm">einherje</div>\u000a      \u000a      <div class="popUp">En einherje er en mand, der er faldet i kamp og derfor lever blandt guderne (nordisk\u000a         mytologi). Einherjerne udg\u00f8r Odins h\u00e6r.\u000a      </div>\u000a      \u000a      <span class="orthography">einherie</span>\u000a      \u000a      <div class="encyc"> Hver dag g\u00e5r de ud for at sl\u00e5s; og som faldne, men genoplivede, vender de hver aften\u000a         tilbage til gilde i Valhal.\u000a      </div>\u000a      \u000a   </div>','myth110','myth')
INSERT INTO TEXTREFERENCE VALUES(10933,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth346">\u000a      \u000a      <div class="normForm">fylgje</div>\u000a      \u000a      <div class="popUp">En fylgje er en skyts\u00e5nd fra den nordiske mytologi, gerne i kvinde- eller dyreskikkelse.\u000a         De kvindelige fylgjer beskytter ofte hele sl\u00e6gter.\u000a      </div>   \u000a      \u000a      \u000a   </div>','myth346','myth')
INSERT INTO TEXTREFERENCE VALUES(10934,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth192.a">\u000a      \u000a      <div class="epithet">gjukunge\u000a         <span class="sameAs">\u000a            <a href="#myth192"> &gt; nibelunge</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth192.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10935,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth154">\u000a      \u000a      <div class="normForm">gyldne tavler</div>\u000a      \u000a      <div class="popUp">Efter Ragnarok finder guderne de gyldne tavler, som de tidligere ejede. At de igen\u000a         kan spille br\u00e6tspil p\u00e5 dem er et tegn p\u00e5, at kosmos er genoprettet og kan bevares,\u000a         s\u00e5 l\u00e6nge guderne spiller.\u000a      </div> \u000a      \u000a   </div>','myth154','myth')
INSERT INTO TEXTREFERENCE VALUES(10936,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth380">\u000a      \u000a      <div class="normForm">haugbue</div>\u000a      \u000a      <div class="popUp">En haugbue er en h\u00f8jlagt afd\u00f8d, som har gravh\u00f8jen som bolig.</div>\u000a      \u000a      <span class="orthography">h\u00f8jbo</span>\u000a      \u000a   </div>','myth380','myth')
INSERT INTO TEXTREFERENCE VALUES(10937,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth233.a">\u000a      \u000a      <div class="epithet">hvidalf\u000a         <span class="sameAs">\u000a            <a href="#myth233"> &gt; lysalf</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth233.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10938,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth32.d">\u000a      \u000a      <div class="epithet">hvide as\u000a         <span class="sameAs">\u000a            <a href="#myth32"> &gt; Heimdal</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth32.d','myth')
INSERT INTO TEXTREFERENCE VALUES(10939,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth111">\u000a      \u000a      <div class="normForm">j\u00e6tte</div>\u000a      \u000a      <div class="popUp">J\u00e6tter er ustyrlige kaosmagter i den nordiske mytologi, st\u00e6rke v\u00e6sener og asernes\u000a         fjender.\u000a      </div>\u000a      \u000a      <span class="orthography">jette\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">jotun</span>\u000a      \u000a      \u000a   </div>','myth111','myth')
INSERT INTO TEXTREFERENCE VALUES(10940,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth418">\u000a      \u000a      <div class="normForm">ledig</div>\u000a      \u000a      <div class="popUp"/>               \u000a      \u000a      \u000a   </div>','myth418','myth')
INSERT INTO TEXTREFERENCE VALUES(10941,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth233">\u000a      \u000a      <div class="normForm">lysalf</div>\u000a      \u000a      <div class="popUp">En lysalf er et guddommeligt v\u00e6sen i den nordiske mytologi og folketro. Lysalferne\u000a         udg\u00f8r en underafdeling af alferne og er s\u00e6rligt knyttet til lys og luft.\u000a      </div>\u000a      \u000a      <div class="encyc">Lysalferne har ofte forbindelse til vanerne. I senere sk\u00f8nlitteratur har de vinger\u000a         og kan opfattes som engleagtige.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>hvidalfhat</span>\u000a   </div>','myth233','myth')
INSERT INTO TEXTREFERENCE VALUES(10942,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth132">\u000a      \u000a      <div class="normForm">moira</div>\u000a      \u000a      <div class="popUp">En moira (pluralis: moirer) er en sk\u00e6bnegudinde fra den gr\u00e6ske mytologi. Det er moirerne,\u000a         der som de nordiske norner spinder, udm\u00e5ler og klipper menneskenes sk\u00e6bnetr\u00e5de.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth132','myth')
INSERT INTO TEXTREFERENCE VALUES(10943,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth167.a">\u000a      \u000a      <div class="epithet">m\u00f8rkalf\u000a         <span class="sameAs">\u000a            <a href="#myth167"> &gt; svartalf</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth167.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10944,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth32.e">\u000a      \u000a      <div class="epithet">ni m\u00f8dres s\u00f8n\u000a         <span class="sameAs">\u000a            <a href="#myth32"> &gt; Heimdal</a>\u000a         </span>\u000a      </div>\u000a      \u000a   </div>','myth32.e','myth')
INSERT INTO TEXTREFERENCE VALUES(10945,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth192">\u000a      \u000a      <div class="normForm">nibelunge</div>\u000a      \u000a      <div class="popUp">Niblungerne udg\u00f8r en sl\u00e6gt i nordisk sagnhistorie. De er s\u00f8nner eller efterkommere\u000a         af sagnkongen Gjuke og kaldes derfor ogs\u00e5 ofte for gjukunger.\u000a      </div>\u000a      \u000a      <span class="orthography">niflunge</span>\u000a      \u000a      <div class="encyc"> Mest kendt er Gunnar, H\u00f8gne og Guttorm, der dr\u00e6bte Sigurd Fafnersbane. Herved kom\u000a         de i besiddelse af nibelungeskatten\u000a      </div>\u000a      \u000a      <div class="encyc">Den tyske komponist Wagner brugte motiver fra historierne om nibelungerne i sin \u000a         <span class="italic">Nibelungenring.</span>\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>gjukungehat</span>\u000a   </div>','myth192','myth')
INSERT INTO TEXTREFERENCE VALUES(10946,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth112">\u000a      \u000a      <div class="normForm">norne</div>\u000a      \u000a      <div class="popUp">En norne er en sk\u00e6bnegudinde i den nordiske mytologi.</div>  \u000a      \u000a      <div class="encyc">I eddadigtet \u000a         <span class="italic">V\u01eblusp\u00e1</span> (da. \u000a         <span class="italic">V\u00f8lvens Sp\u00e5dom</span>) spiller nornerne en stor rolle. De tre, der sidder ved Yggdrasils r\u00f8dder, kaldes\u000a         Urd, Skuld og Verdande. Grundtvig till\u00e6gger nornerne en h\u00f8jere visdom end selv guderne.\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Dvalins d\u00f8trehat</span>\u000a   </div>','myth112','myth')
INSERT INTO TEXTREFERENCE VALUES(10947,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth113">\u000a      \u000a      <div class="normForm">parce</div>\u000a      \u000a      <div class="popUp">En parce er en f\u00f8dselsgudinde i den romerske mytologi. Tidligere identificeredes parcerne\u000a         ofte med de gr\u00e6ske sk\u00e6bnegudinder, moirerne.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth113','myth')
INSERT INTO TEXTREFERENCE VALUES(10948,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth114">\u000a      \u000a      <div class="normForm">rimturse</div>\u000a      \u000a      <div class="popUp">En rimturse er en slags j\u00e6tte fra den nordiske mytologi. Rimturser er dannet af rimfrost.</div>\u000a      \u000a      <span class="orthography">hrimturse,</span>\u000a      \u000a      <span class="orthography">hrimthusse\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">hrimthurse</span>\u000a      \u000a      \u000a   </div>','myth114','myth')
INSERT INTO TEXTREFERENCE VALUES(10949,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth276.a">\u000a      \u000a      <div class="epithet">skilfing\u000a         <span class="sameAs">\u000a            <a href="#myth276"> &gt; yngling</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <div class="popUp">Skilfingerne er et andet navn for de yngre generationer af ynglinge\u00e6tten.</div>\u000a      \u000a   </div>','myth276.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10950,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth368">\u000a      \u000a      <div class="normForm">skjoldm\u00f8</div>\u000a      \u000a      <div class="popUp">Skjoldm\u00f8erne er kvinder fra den nordiske mytologi og sagnhistorie. Som navnet antyder,\u000a         er de en slags kvindelige soldater.\u000a      </div>\u000a      \u000a      <div class="encyc">Ofte havde skjoldm\u00f8er svoret at leve i kyskhed.</div>\u000a      \u000a   </div>','myth368','myth')
INSERT INTO TEXTREFERENCE VALUES(10951,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth115">\u000a      \u000a      <div class="normForm">skjoldunge</div>\u000a      \u000a      <div class="popUp">En skjoldunge er en efterkommer af kong Skjold. Senere bruges betegnelsen om Danmarks\u000a         konger generelt. If\u00f8lge sagnet var Skjold s\u00f8n af Odin og Rind.\u000a      </div>\u000a      \u000a      \u000a   </div>','myth115','myth')
INSERT INTO TEXTREFERENCE VALUES(10952,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth167">\u000a      \u000a      <div class="normForm">svartalf</div>\u000a      \u000a      <div class="popUp">En svartalf er et guddommeligt v\u00e6sen i den nordiske mytologi og folketro. Svartalferne\u000a         udg\u00f8r en undergruppe af alferne og er s\u00e6rligt knyttet til m\u00f8rke og jord. Ofte optr\u00e6der\u000a         de sammen med dv\u00e6rgene.\u000a      </div>\u000a      \u000a      <span class="orthography">sortalf</span>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>m\u00f8rkalfhat</span>\u000a   </div>','myth167','myth')
INSERT INTO TEXTREFERENCE VALUES(10953,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth159">\u000a      \u000a      <div class="normForm">titan</div>\u000a      \u000a      <div class="popUp">En titan h\u00f8rer til den m\u00e6ngde af guder, der i den gr\u00e6ske mytologi eksisterede f\u00f8r\u000a         de olympiske guder. Titanerne var alle b\u00f8rn af Uranos og Gaia.\u000a      </div>\u000a      \u000a      <div class="encyc">Af s\u00e5vel titaner som af olympiske guder var der 12. De mest kendte titaner er Kronos\u000a         og Rheia, for\u00e6ldre til bl.a. Zeus, Poseidon og Hades. Titanerne blev besejret af de\u000a         olympiske guder og styrtet i dybet, hvorfra de for\u00e5rsager jordsk\u00e6lv o.lign.\u000a      </div>\u000a      \u000a   </div>','myth159','myth')
INSERT INTO TEXTREFERENCE VALUES(10954,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth265">\u000a      \u000a      <div class="normForm">trold</div>\u000a      \u000a      <div class="popUp">Trolde optr\u00e6der b\u00e5de i den nordiske mytologi og i folketroen. De er oftest st\u00e6rke,\u000a         men ikke altid for kloge, s\u00e5 de er lette at narre b\u00e5de for guder og mennesker. Mange\u000a         trolde bliver til sten, hvis de uds\u00e6ttes for dagslys.\u000a      </div>\u000a      \u000a   </div>','myth265','myth')
INSERT INTO TEXTREFERENCE VALUES(10955,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth116">\u000a      \u000a      <div class="normForm">turse</div>\u000a      \u000a      <div class="popUp">En turse er et overnaturligt v\u00e6sen (en slags j\u00e6tte) fra den nordiske mytologi. Turserne\u000a         er ikke er p\u00e5 det godes side. De bor i Udg\u00e5rd sammen med j\u00e6tterne.\u000a      </div>\u000a      \u000a      <span class="orthography">thurs,</span>\u000a      \u000a      <span class="orthography">thurse\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">turs</span>\u000a      \u000a      \u000a   </div>','myth116','myth')
INSERT INTO TEXTREFERENCE VALUES(10956,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth117">\u000a      \u000a      <div class="normForm">valkyrie</div>\u000a      \u000a      <div class="popUp">En valkyrie er en kvindelig guddom i den nordiske mytologi. Valkyrierne adlyder Odin\u000a         og udv\u00e6lger (k\u00e5rer) de krigere, der skal falde p\u00e5 slagmarken (valen).\u000a      </div>\u000a      \u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>asungehat</span>\u000a      <span>Herjans M\u00f8er eller </span>\u000a      <span>Odins M\u00f8er.</span>\u000a   </div>','myth117','myth')
INSERT INTO TEXTREFERENCE VALUES(10957,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth118">\u000a      \u000a      <div class="normForm">van</div>\u000a      \u000a      <div class="popUp">Vanerne er en sl\u00e6gt af frugtbarhedsguder fra den nordiske mytologi. De slutter sig\u000a         sammen med aserne p\u00e5 et meget tidligt tidspunkt i de nordiske guders historie. Njord\u000a         og hans b\u00f8rn, Frej og Freja, er vaner.\u000a      </div>\u000a      \u000a      <span class="orthography">vane,</span>\u000a      \u000a      <span class="orthography">vaner\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">vanir</span>\u000a      \u000a      \u000a   </div>','myth118','myth')
INSERT INTO TEXTREFERENCE VALUES(10958,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth404">\u000a      \u000a      <div class="normForm">vitke</div>\u000a      \u000a      <div class="popUp">En vitke er en sp\u00e5domskyndig person fra den nordiske mytologi og sagnhistorie. Alle\u000a         vitker stammer fra Vilmeid.\u000a      </div>    \u000a      \u000a   </div>','myth404','myth')
INSERT INTO TEXTREFERENCE VALUES(10959,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth232">\u000a      \u000a      <div class="normForm">v\u00e6tte</div>\u000a      \u000a      <div class="popUp">En v\u00e6tte er et overnaturligt v\u00e6sen fra nordisk mytologi og is\u00e6r nordisk folketro.\u000a         Til v\u00e6tterne regnes b\u00e5de nisser, alfer, trolde og dv\u00e6rge.\u000a      </div>\u000a      \u000a      <div class="encyc"> V\u00e6tterne holder som regel til ved en speciel lokalitet i naturen. En del af dem ligner\u000a         mennesker; og det er vigtigt for menneskene at holde sig gode venner med dem. I folketroen\u000a         kaldes v\u00e6tterne ofte for \u2018de underjordiske\u2019.\u000a      </div>\u000a      \u000a   </div>','myth232','myth')
INSERT INTO TEXTREFERENCE VALUES(10960,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth119">\u000a      \u000a      <div class="normForm">v\u00f8lsunge</div>\u000a      \u000a      <div class="popUp">V\u00f8lsungerne er en sl\u00e6gt fra den nordiske (og germanske) sagnhistorie, der i nogle\u000a         sammenh\u00e6nge antages at stamme fra Odin. Sigurd Fafnersbane var en v\u00f8lsunge.\u000a      </div>\u000a      \u000a      <span class="orthography">volsunge</span>\u000a      \u000a      \u000a   </div>','myth119','myth')
INSERT INTO TEXTREFERENCE VALUES(10961,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth120">\u000a      \u000a      <div class="normForm">v\u00f8lve</div>\u000a      \u000a      <div class="popUp">En v\u00f8lve er en sp\u00e5domskyndig kvinde i nordisk religionsud\u00f8velse og i nordisk mytologi.\u000a         Der optr\u00e6der v\u00f8lver i b\u00e5de sagaer og eddakvad.\u000a      </div>\u000a      \u000a      <span class="orthography">vole</span>\u000a      \u000a      \u000a   </div>','myth120','myth')
INSERT INTO TEXTREFERENCE VALUES(10962,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth276">\u000a      \u000a      <div class="normForm">yngling</div>\u000a      \u000a      <div class="popUp">Ynglingerne udg\u00f8r en kongesl\u00e6gt fra den nordiske sagnhistorie. Medlemmer heraf mente,\u000a         at de nedstammede fra Yngve-Frej, dvs. Frej.\u000a      </div>\u000a      \u000a      <div class="encyc">De yngre generationer af sl\u00e6gten kaldes ogs\u00e5 skilfinger.</div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>skilfinghat</span>\u000a   </div>','myth276','myth')
INSERT INTO TEXTREFERENCE VALUES(10963,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth41.a">\u000a      \u000a      <div class="epithet">\u00c6blernes vogter\u000a         <span class="sameAs">\u000a            <a href="#myth41"> &gt; Idun</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">\u00c6blernes gemmer</span>\u000a      \u000a   </div>','myth41.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10964,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth85">\u000a      \u000a      <div class="normForm">\u00c6gir</div>\u000a      \u000a      <div class="popUp">\u00c6gir er en havj\u00e6tte i den nordiske mytologi. Han bor hos aserne og er konge over havet.\u000a         Derudover er han en stor \u00f8lbrygger.\u000a      </div>\u000a      \u000a      <span class="orthography">\u00c6ger\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00c6gi</span>\u000a      \u000a      <div class="encyc">Han optr\u00e6der i \u000a         <span class="italic">\u00c6gis Drekka</span> (da. \u000a         <span class="italic">\u00c6girs G\u00e6stebud</span>).\u000a      </div>\u000a      \u000a      <i>Kaldes ogs\u00e5: </i>\u000a      <span>Gymerhat</span>\u000a      <span>Hler.</span>\u000a   </div>','myth85','myth')
INSERT INTO TEXTREFERENCE VALUES(10965,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth223.a">\u000a      \u000a      <div class="latin">\u00c6olus\u000a         <span class="sameAs">\u000a            <a href="#myth223"> &gt; Aiolos</a>\u000a         </span>\u000a      </div>\u000a      \u000a      <span class="orthography">Aeolus</span>\u000a      \u000a   </div>','myth223.a','myth')
INSERT INTO TEXTREFERENCE VALUES(10966,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth86">\u000a      \u000a      <div class="normForm">\u00d8dhumla</div>\u000a      \u000a      <div class="popUp">\u00d8dhumla er en m\u00e6lkerig ko, der i den nordiske mytologi medvirker til verdens skabelse.\u000a         Dels giver den m\u00e6lk til j\u00e6tten Ymer, af hvis d\u00f8de krop guderne senere bygger verden;\u000a         dels slikker den j\u00e6tten Bure frem af en saltsten.\u000a      </div>\u000a      \u000a      <span class="orthography">Audumbla,</span>\u000a      \u000a      <span class="orthography">Audhumbla,</span>\u000a      \u000a      <span class="orthography">\u00d8dhumbla\u000a         <i> eller </i>\u000a      </span>\u000a      \u000a      <span class="orthography">\u00d8dumla</span>\u000a      \u000a      <div class="encyc">Snorre fort\u00e6ller denne historie i \u000a         <span class="italic">Gylfaginning</span> (da. \u000a         <span class="italic">Gylfe Forbl\u00e6ndelse</span>) (da. \u000a         <span class="italic">Gylfes Forbl\u00e6ndelse.</span>\u000a      </div>\u000a      \u000a   </div>','myth86','myth')
INSERT INTO TEXTREFERENCE VALUES(10967,-1,'myth.xml','<div xmlns:TEI="http://www.tei-c.org/ns/1.0" class="row myth" id="myth308">\u000a      \u000a      <div class="normForm">\u00d8ster</div>\u000a      \u000a      <div class="popUp">\u00d8ster er en af de fire dv\u00e6rge i den nordiske mytologi, der b\u00e6rer himmelhv\u00e6lvingen.</div>\u000a      \u000a   </div>','myth308','myth')
